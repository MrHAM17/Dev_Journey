<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="detailPageNineVM" type="com.moviestreaming.app.modules.detailpagenine.data.viewmodel.DetailPageNineVM"/>
    </data>
    <LinearLayout android:id="@+id/linearArrowLeft"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;gray_900&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_left"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_left"/>
            <ImageView android:id="@+id/imageSearch"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_search"
                tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
        </androidx.appcompat.widget.Toolbar>
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollView"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearDetailPageNine"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearFrameFiftyTwo"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageThumbnailImage"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_140pxh"
                        android:layout_width="@dimen/_90pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_2pxh}"
                        imageRes="@{@drawable/img_thumbnail_image_28}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_thumbnail_image_28"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearFrameFortyFour"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTitle"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{detailPageNineVM.detailPageNineModel.txtTitle}"
                            style="@style/txtRobotobold18" tools:text="@string/lbl_captain_marvel"/>
                        <LinearLayout
                            android:id="@+id/linearInformation"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtZipcode"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{detailPageNineVM.detailPageNineModel.txtZipcode}"
                                style="@style/txtRobotoregular12_2" tools:text="@string/lbl_2019"/>
                            <View
                                android:background="@drawable/rectangle_bg_name_onprimarycontainer_radius_1"
                                android:id="@+id/viewView"
                                android:layout_height="@dimen/_3pxh" android:layout_width="@dimen/_3pxh"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtAction"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{detailPageNineVM.detailPageNineModel.txtAction}"
                                style="@style/txtRobotoregular12_2" tools:text="@string/lbl_action"/>
                            <View
                                android:background="@drawable/rectangle_bg_name_onprimarycontainer_radius_1"
                                android:id="@+id/viewView1"
                                android:layout_height="@dimen/_3pxh" android:layout_width="@dimen/_3pxh"/>
                            <LinearLayout
                                android:gravity="center_vertical"
                                android:id="@+id/linearRattings"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content" android:orientation="horizontal">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtStar"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{detailPageNineVM.detailPageNineModel.txtStar}"
                                    style="@style/txtRobotoregular12_2" tools:text="@string/lbl_4_5"/>
                                <ImageView android:id="@+id/imageSignal"
                                    android:layout_height="@dimen/_8pxh"
                                    android:layout_width="@dimen/_8pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_signal"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_signal"/>
                            </LinearLayout>
                        </LinearLayout>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTime"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{detailPageNineVM.detailPageNineModel.txtTime}"
                            style="@style/txtRobotoregular12_2" tools:text="@string/lbl_1_hr_49min"/>
                    </LinearLayout>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameFrameFiftyThree"
                    android:layout_height="wrap_content" android:layout_width="match_parent">
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_onsecondarycontainer_border_name_primary_radius_17"
                        android:id="@+id/frameTags"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_9pxv"
                        android:paddingEnd="@dimen/_10pxh"
                        android:paddingStart="@dimen/_10pxh" android:paddingTop="@dimen/_9pxv"/>
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_onsecondarycontainer_border_name_primary_radius_17"
                        android:id="@+id/frameTags1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_9pxv"
                        android:paddingEnd="@dimen/_10pxh"
                        android:paddingStart="@dimen/_10pxh" android:paddingTop="@dimen/_9pxv"/>
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_onsecondarycontainer_border_name_primary_radius_17"
                        android:id="@+id/frameTags2"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_9pxv"
                        android:paddingEnd="@dimen/_10pxh"
                        android:paddingStart="@dimen/_10pxh" android:paddingTop="@dimen/_9pxv"/>
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_onsecondarycontainer_border_name_primary_radius_17"
                        android:id="@+id/frameTags3"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_9pxv"
                        android:paddingEnd="@dimen/_10pxh"
                        android:paddingStart="@dimen/_10pxh" android:paddingTop="@dimen/_9pxv"/>
                </FrameLayout>
                <TextView android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:text="@{detailPageNineVM.detailPageNineModel.txtDescription}"
                    style="@style/txtRobotoregular12_2" tools:text="@string/msg_set_in_the_1990s2"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtYouMightAlsoLike"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{detailPageNineVM.detailPageNineModel.txtYouMightAlsoLike}"
                    style="@style/txtRobotosemibold14" tools:text="@string/msg_you_might_also_like"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerMovies"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:nestedScrollingEnabled="false"
                    android:orientation="horizontal"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_16pxh}" tools:itemCount="3" tools:listitem="@layout/row_movies1"/>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>
</layout>
