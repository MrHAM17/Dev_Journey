<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="channelsixRowModel" type="com.moviestreaming.app.modules.channelsix.data.model.ChannelsixRowModel"/>
    </data>
    <LinearLayout android:gravity="start" android:id="@+id/linearFrame"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="vertical">
        <TextView android:ellipsize="end" android:id="@+id/txtHeader"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{channelsixRowModel.txtHeader}"
            style="@style/txtRobotomedium16" tools:text="@string/msg_last_time_we_meet"/>
        <TextView android:ellipsize="end" android:id="@+id/txtTime"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{channelsixRowModel.txtTime}"
            style="@style/txtRobotoregular14_1" tools:text="@string/lbl_02_00_am_est"/>
    </LinearLayout>
</layout>
