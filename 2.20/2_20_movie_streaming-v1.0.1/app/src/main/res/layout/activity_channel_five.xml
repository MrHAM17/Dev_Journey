<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="channelFiveVM" type="com.moviestreaming.app.modules.channelfive.data.viewmodel.ChannelFiveVM"/>
    </data>
    <LinearLayout android:id="@+id/linearArrowLeft"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;gray_900&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_left"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_left"/>
            <TextView android:ellipsize="end" android:id="@+id/txtNons"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{channelFiveVM.channelFiveModel.txtNons}"
                style="@style/txtRobotosemibold16" tools:text="@string/lbl_live_channel"/>
            <LinearLayout android:id="@+id/linearGroup46"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageIcon"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_icon_onprimarycontainer"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_icon_onprimarycontainer"/>
                <ImageView android:id="@+id/imageIcon1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_icon_onprimarycontainer_24x24"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_icon_onprimarycontainer_24x24"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollView"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearChannelFive"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:id="@+id/frameVideoPlayer"
                    android:layout_height="@dimen/_180pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageThumbnailImage"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_180pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_thumbnail_image_180x360}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_thumbnail_image_180x360"/>
                    <LinearLayout android:id="@+id/linearIcon"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <ImageView android:id="@+id/imageIcon2"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_32pxh"
                            android:layout_width="@dimen/_32pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icon_white_a700_32x32"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_icon_white_a700_32x32"/>
                        <LinearLayout android:id="@+id/linearIcon1"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageIcon3"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_icon_onprimarycontainer_18x18"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_icon_onprimarycontainer_18x18"/>
                            <ImageView android:id="@+id/imageIcon4"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_icon_18x18"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_icon_18x18"/>
                            <ProgressBar
                                android:gravity="center_vertical"
                                android:id="@+id/progressBarProgressBar"
                                android:indeterminate="false"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:layout_width="0dp"
                                android:max="100"
                                android:minHeight="@dimen/_4pxv"
                                android:progress="57"
                                android:progressBackgroundTint="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                                android:progressTint="@color/{&quot;name&quot;:&quot;primary&quot;}" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                            <ImageView android:id="@+id/imageIcon5"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_icon_1"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_icon_1"/>
                            <ImageView android:id="@+id/imageIcon6"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_icon_2"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_icon_2"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDetails"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{channelFiveVM.channelFiveModel.txtDetails}"
                    style="@style/txtRobotosemibold18" tools:text="@string/msg_the_last_black_man"/>
                <TextView android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:text="@{channelFiveVM.channelFiveModel.txtDescription}"
                    style="@style/txtRobotoregular12_2" tools:text="@string/msg_fertility_and_desolation"/>
                <LinearLayout android:id="@+id/linearFrameFortyFive"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDirector"
                        android:layout_gravity="top"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{channelFiveVM.channelFiveModel.txtDirector}"
                        style="@style/txtRobotoregular12_2" tools:text="@string/lbl_director"/>
                    <TextView android:ellipsize="end"
                        android:gravity="bottom"
                        android:id="@+id/txtAnnaBodenRyan"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{channelFiveVM.channelFiveModel.txtAnnaBodenRyan}"
                        style="@style/txtRobotoregular12_1" tools:text="@string/msg_anna_boden_ryan"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearFrameFortySix"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDirector1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{channelFiveVM.channelFiveModel.txtDirector1}"
                        style="@style/txtRobotoregular12_2" tools:text="@string/lbl_cast"/>
                    <TextView android:id="@+id/txtAnnaBodenRyan1"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:text="@{channelFiveVM.channelFiveModel.txtAnnaBodenRyan1}"
                        style="@style/txtRobotoregular12_1" tools:text="@string/msg_brie_larson_samuel"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearFrameSixty"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAction"
                        android:layout_gravity="bottom"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{channelFiveVM.channelFiveModel.txtAction}"
                        style="@style/txtRobotobold14" tools:text="@string/lbl_special_for_you2"/>
                    <FrameLayout android:id="@+id/frameSpacer"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMore"
                        android:layout_gravity="top"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{channelFiveVM.channelFiveModel.txtMore}"
                        style="@style/txtRobotoregular12" tools:text="@string/lbl_more"/>
                    <ImageView android:id="@+id/imageIcon7"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_icon_primary"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_icon_primary"/>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerMovies"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:nestedScrollingEnabled="false"
                    android:orientation="horizontal"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_16pxh}" tools:itemCount="3" tools:listitem="@layout/row_movies6"/>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>
</layout>
