<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="channelFourVM" type="com.moviestreaming.app.modules.channelfour.data.viewmodel.ChannelFourVM"/>
    </data>
    <LinearLayout android:id="@+id/linearArrowLeft"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;gray_900&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_left"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_left"/>
            <TextView android:ellipsize="end" android:id="@+id/txtNons"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{channelFourVM.channelFourModel.txtNons}"
                style="@style/txtRobotosemibold16" tools:text="@string/lbl_live_channel"/>
            <LinearLayout android:id="@+id/linearGroup43"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageIcon"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_icon_onprimarycontainer"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_icon_onprimarycontainer"/>
                <ImageView android:id="@+id/imageIcon1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_icon_onprimarycontainer_24x24"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_icon_onprimarycontainer_24x24"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollView"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearChannelFour"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:gravity="center_horizontal"
                    android:id="@+id/frameVideoPlayer"
                    android:layout_height="@dimen/_180pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageThumbnailImage"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_180pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_thumbnail_image_180x360}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_thumbnail_image_180x360"/>
                    <LinearLayout android:id="@+id/linearIcon"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <ImageView android:id="@+id/imageIcon2"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_32pxh"
                            android:layout_width="@dimen/_32pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icon_white_a700_32x32"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_icon_white_a700_32x32"/>
                        <LinearLayout android:id="@+id/linearIcon1"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageIcon3"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_icon_onprimarycontainer_18x18"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_icon_onprimarycontainer_18x18"/>
                            <ImageView android:id="@+id/imageIcon4"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_icon_18x18"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_icon_18x18"/>
                            <ProgressBar
                                android:gravity="center_vertical"
                                android:id="@+id/progressBarProgressBar"
                                android:indeterminate="false"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:layout_width="0dp"
                                android:max="100"
                                android:minHeight="@dimen/_4pxv"
                                android:progress="57"
                                android:progressBackgroundTint="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                                android:progressTint="@color/{&quot;name&quot;:&quot;primary&quot;}" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                            <ImageView android:id="@+id/imageIcon5"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_icon_1"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_icon_1"/>
                            <ImageView android:id="@+id/imageIcon6"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_icon_2"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_icon_2"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:gravity="center_horizontal"
                    android:id="@+id/recyclerChannelFour"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:nestedScrollingEnabled="false"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_16pxh}" tools:itemCount="4" tools:listitem="@layout/row_channelfour"/>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>
</layout>
