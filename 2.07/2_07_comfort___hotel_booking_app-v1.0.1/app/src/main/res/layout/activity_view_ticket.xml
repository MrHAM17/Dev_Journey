<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="viewTicketVM" type="com.comforthotelbookingapp.app.modules.viewticket.data.viewmodel.ViewTicketVM"/>
    </data>
    <LinearLayout android:id="@+id/linearVector"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;onPrimary&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_down"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFillYourProfile"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{viewTicketVM.viewTicketModel.txtFillYourProfile}"
                style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_ticket"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearVector1"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_34pxv"
            android:paddingEnd="@dimen/_22pxh"
            android:paddingStart="@dimen/_22pxh"
            android:paddingTop="@dimen/_34pxv" style="@style/groupStyle_2">
            <TextView android:ellipsize="end"
                android:id="@+id/txtRoyalePresident"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{viewTicketVM.viewTicketModel.txtRoyalePresident}"
                style="@style/txtUrbanistromanbold18" tools:text="@string/msg_royale_president"/>
            <LinearLayout android:id="@+id/linearFrame"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingHorizontal="@dimen/_16pxh"
                android:paddingVertical="@dimen/_16pxv" style="@style/groupStyle{&quot;name&quot;:&quot;blue_gray_900&quot;}">
                <FrameLayout android:gravity="center_horizontal"
                    android:id="@+id/frameVector"
                    android:layout_height="match_parent" android:layout_width="match_parent"/>
            </LinearLayout>
            <View
                android:background="@color/{&quot;name&quot;:&quot;gray_700&quot;}"
                android:id="@+id/lineLine"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_2pxv" android:layout_width="@dimen/_335pxh"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearTicketDetails"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingEnd="@dimen/_9pxh" android:paddingStart="@dimen/_9pxh">
                <LinearLayout
                    android:id="@+id/linearAutoLayoutHorizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearAutoLayoutVertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtName"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{viewTicketVM.viewTicketModel.txtName}"
                            style="@style/txtUrbanistregular16_1" tools:text="@string/lbl_name"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDanielAustin"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{viewTicketVM.viewTicketModel.txtDanielAustin}"
                            style="@style/txtUrbanistsemibold18" tools:text="@string/lbl_daniel_austin"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearAutoLayoutVertical1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPhoneNumber"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{viewTicketVM.viewTicketModel.txtPhoneNumber}"
                            style="@style/txtUrbanistregular16_1" tools:text="@string/lbl_phone_number"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtWidget"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{viewTicketVM.viewTicketModel.txtWidget}"
                            style="@style/txtUrbanistsemibold18" tools:text="@string/msg_1_123_456_789_000"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearAutoLayoutHorizontal1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearAutoLayoutVertical2"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtCheckIn"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{viewTicketVM.viewTicketModel.txtCheckIn}"
                            style="@style/txtUrbanistregular16_1" tools:text="@string/lbl_check_in"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDec"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{viewTicketVM.viewTicketModel.txtDec}"
                            style="@style/txtUrbanistsemibold18" tools:text="@string/lbl_dec_16_2024"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearAutoLayoutVertical3"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtCheckOut"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{viewTicketVM.viewTicketModel.txtCheckOut}"
                            style="@style/txtUrbanistregular16_1" tools:text="@string/lbl_check_out"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDec1"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{viewTicketVM.viewTicketModel.txtDec1}"
                            style="@style/txtUrbanistsemibold18" tools:text="@string/lbl_dec_20_2024"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearAutoLayoutHorizontal2"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearAutoLayoutVertical4"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtHotel"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{viewTicketVM.viewTicketModel.txtHotel}"
                            style="@style/txtUrbanistregular16_1" tools:text="@string/lbl_hotel"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtRoyalePresident1"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{viewTicketVM.viewTicketModel.txtRoyalePresident1}"
                            style="@style/txtUrbanistsemibold18" tools:text="@string/msg_royale_president2"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearAutoLayoutVertical5"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtGuest"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{viewTicketVM.viewTicketModel.txtGuest}"
                            style="@style/txtUrbanistregular16_1" tools:text="@string/lbl_guest"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtThree"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{viewTicketVM.viewTicketModel.txtThree}"
                            style="@style/txtSourceSansProsemibold18" tools:text="@string/lbl_3"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:background="@drawable/rectangle_bg_name_primary_radius_27"
            android:ellipsize="end" android:fontFamily="@font/urbanist"
            android:gravity="center" android:id="@+id/btnDownloadTicket"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_54pxv"
            android:layout_width="@dimen/_380pxh"
            android:paddingBottom="@dimen/_15pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_15pxv" android:singleLine="true"
            android:text="@string/lbl_download_ticket"
            android:textAllCaps="false"
            android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
            android:textFontWeight="700"
            android:textSize="@dimen/_16pxh" android:textStyle="bold" tools:text="@string/lbl_download_ticket"/>
    </LinearLayout>
</layout>
