<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="filterresultRowModel" type="com.comforthotelbookingapp.app.modules.filterresult.data.model.FilterresultRowModel"/>
    </data>
    <LinearLayout android:gravity="spaceAround"
        android:id="@+id/linearTypeListThemeDark"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/_18pxv"
        android:paddingTop="@dimen/_18pxv" style="@style/groupStyle{&quot;name&quot;:&quot;blue_gray_900&quot;}cornerRadius">
        <LinearLayout android:id="@+id/linearPresidentHotel"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imagePresidentHotel"
                android:layout_height="@dimen/_100pxh"
                android:layout_width="@dimen/_100pxh"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_16pxh}"
                imageRes="@{@drawable/img_rectangle}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearAutoLayoutVertical1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPresidentHotel"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{filterresultRowModel.txtPresidentHotel}"
                    style="@style/txtUrbanistromanbold22" tools:text="@string/lbl_president_hotel"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtParisFrance"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{filterresultRowModel.txtParisFrance}"
                    style="@style/txtUrbanistregular14" tools:text="@string/lbl_paris_france"/>
                <LinearLayout
                    android:id="@+id/linearAutoLayoutHorizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageImage1"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_width="@dimen/_12pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_signal_yellow_a700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_signal_yellow_a700"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFortyEight"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{filterresultRowModel.txtFortyEight}"
                        style="@style/txtUrbanistsemibold14_1" tools:text="@string/lbl_4_8"/>
                    <TextView android:ellipsize="end"
                        android:gravity="bottom"
                        android:id="@+id/txtReviews"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{filterresultRowModel.txtReviews}"
                        style="@style/txtUrbanistregular12" tools:text="@string/lbl_4_378_reviews"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="end"
            android:id="@+id/linearAutoLayoutVertical2"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:ellipsize="end" android:id="@+id/txtPrice"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{filterresultRowModel.txtPrice}"
                style="@style/txtUrbanistromanbold24_1" tools:text="@string/lbl_35"/>
            <TextView android:ellipsize="end" android:id="@+id/txtNight"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{filterresultRowModel.txtNight}"
                style="@style/txtUrbanistromanmedium10_1" tools:text="@string/lbl_night"/>
            <ImageView android:id="@+id/imageImage2"
                android:layout_height="@dimen/_24pxh"
                android:layout_width="@dimen/_24pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_bookmark_primary"
                tools:ignore="ContentDescription" tools:src="@drawable/img_bookmark_primary"/>
        </LinearLayout>
    </LinearLayout>
</layout>
