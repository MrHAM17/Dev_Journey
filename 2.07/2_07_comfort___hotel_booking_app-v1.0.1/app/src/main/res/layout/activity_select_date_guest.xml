<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="selectDateGuestVM" type="com.comforthotelbookingapp.app.modules.selectdateguest.data.viewmodel.SelectDateGuestVM"/>
    </data>
    <LinearLayout android:id="@+id/linearContinue"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;onPrimary&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_down"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFillYourProfile"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{selectDateGuestVM.selectDateGuestModel.txtFillYourProfile}"
                style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_select_date"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearSelectDateGuest"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_20pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_20pxv">
            <LinearLayout android:id="@+id/linearDateCalendar"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_72pxv"
                android:paddingEnd="@dimen/_23pxh"
                android:paddingStart="@dimen/_23pxh"
                android:paddingTop="@dimen/_72pxv" style="@style/groupStylenameblue_gray_6">

                <com.prolificinteractive.materialcalendarview.MaterialCalendarView
                    android:gravity="center_horizontal"
                    android:id="@+id/calendarVector"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:mcv_selectionMode="single"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearBookingDetails"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearEightyEight"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCheckIn"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{selectDateGuestVM.selectDateGuestModel.txtCheckIn}"
                        style="@style/txtUrbanistromanbold18" tools:text="@string/lbl_check_in"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCheckOut"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{selectDateGuestVM.selectDateGuestModel.txtCheckOut}"
                        style="@style/txtUrbanistromanbold18" tools:text="@string/lbl_check_out"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearDecCounter"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearCheckIn"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:orientation="horizontal"
                        android:paddingBottom="@dimen/_18pxv"
                        android:paddingEnd="@dimen/_20pxh"
                        android:paddingStart="@dimen/_20pxh"
                        android:paddingTop="@dimen/_18pxv" style="@style/groupStylenameblue_gray_2">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDecCounter"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{selectDateGuestVM.selectDateGuestModel.txtDecCounter}"
                            style="@style/txtUrbanistsemibold14" tools:text="@string/lbl_dec_16"/>
                        <ImageView android:id="@+id/imageImage"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_iconly_curved_calendar_primary"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_curved_calendar_primary"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageFavorite"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_favorite_white_a700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_favorite_white_a700"/>
                    <LinearLayout android:id="@+id/linearCheckOut"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:orientation="horizontal"
                        android:paddingBottom="@dimen/_18pxv"
                        android:paddingEnd="@dimen/_20pxh"
                        android:paddingStart="@dimen/_20pxh"
                        android:paddingTop="@dimen/_18pxv" style="@style/groupStylenameblue_gray_2">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDecCounter1"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{selectDateGuestVM.selectDateGuestModel.txtDecCounter1}"
                            style="@style/txtUrbanistsemibold14" tools:text="@string/lbl_dec_20"/>
                        <ImageView android:id="@+id/imageImage1"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_iconly_curved_calendar_primary"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_curved_calendar_primary"/>
                    </LinearLayout>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtGuest"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{selectDateGuestVM.selectDateGuestModel.txtGuest}"
                    style="@style/txtUrbanistromanbold18" tools:text="@string/lbl_guest"/>
                <LinearLayout android:id="@+id/linearGuest"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_82pxh"
                    android:paddingStart="@dimen/_82pxh"
                    android:paddingTop="@dimen/_11pxv" style="@style/groupStylenameblue_gray_4">
                    <FrameLayout android:id="@+id/framePlus"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_52pxv"
                        android:layout_width="@dimen/_52pxh"
                        android:paddingHorizontal="@dimen/_16pxh"
                        android:paddingVertical="@dimen/_16pxv" style="@style/groupStyle{&quot;name&quot;:&quot;cyan_900&quot;}cornerRadius">
                        <ImageView android:id="@+id/imageImage2"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_frame_white_a700_20x20"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_frame_white_a700_20x20"/>
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtThree"
                        android:layout_gravity="top"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{selectDateGuestVM.selectDateGuestModel.txtThree}"
                        style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_3"/>
                    <FrameLayout android:id="@+id/frameMinus"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_52pxv"
                        android:layout_width="@dimen/_52pxh"
                        android:paddingHorizontal="@dimen/_16pxh"
                        android:paddingVertical="@dimen/_16pxv" style="@style/groupStyle{&quot;name&quot;:&quot;cyan_900&quot;}cornerRadius">
                        <ImageView android:id="@+id/imageImage3"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_frame_20x20"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_frame_20x20"/>
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtPrice"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{selectDateGuestVM.selectDateGuestModel.txtPrice}"
                style="@style/txtUrbanistromanbold18" tools:text="@string/lbl_total_435"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:background="@drawable/rectangle_bg_name_primary_radius_27"
            android:ellipsize="end" android:fontFamily="@font/urbanist"
            android:gravity="center" android:id="@+id/btnContinue"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_54pxv"
            android:layout_width="@dimen/_380pxh"
            android:paddingBottom="@dimen/_15pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_15pxv" android:singleLine="true"
            android:text="@string/lbl_continue"
            android:textAllCaps="false"
            android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
            android:textFontWeight="700"
            android:textSize="@dimen/_16pxh" android:textStyle="bold" tools:text="@string/lbl_continue"/>
    </LinearLayout>
</layout>
