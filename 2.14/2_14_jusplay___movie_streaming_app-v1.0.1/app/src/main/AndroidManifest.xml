<?xml version="1.0" encoding="UTF-8"?>
<manifest package="com.jusplaymoviestreamingapp.app" xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" android:largeHeap="true"
        android:name="com.jusplaymoviestreamingapp.app.appcomponents.di.MyApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true" android:theme="@style/Theme.MyApp" android:usesCleartextTraffic="true">
        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/facebook_app_id"/>
        <meta-data android:name="com.facebook.sdk.ClientToken" android:value="@string/facebook_client_token"/>
        <activity
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name" android:name="com.facebook.FacebookActivity"/>
        <activity android:exported="true" android:name="com.facebook.CustomTabActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data android:scheme="@string/fb_login_protocol_scheme"/>
            </intent-filter>
        </activity>
        <activity android:exported="true"
            android:name="com.jusplaymoviestreamingapp.app.modules.resetpassword.ui.ResetPasswordActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.selectsaved.ui.SelectSavedActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.notification.ui.NotificationActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.homerecentlywatchedcontainer.ui.HomeRecentlyWatchedContainerActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.searchresult.ui.SearchResultActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.language.ui.LanguageActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.otpverification.ui.OtpVerificationActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.resetpasswordsuccess.ui.ResetPasswordSuccessActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.editprofile.ui.EditProfileActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.paymentmethod.ui.PaymentMethodActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.nohistory.ui.NoHistoryActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.signup.ui.SignUpActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.help.ui.HelpActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.history.ui.HistoryActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.hometabcontainer.ui.HomeTabContainerActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.forgotpassword.ui.ForgotPasswordActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.login.ui.LoginActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.savedempty.ui.SavedEmptyActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.splash.ui.SplashActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.notfound.ui.NotFoundActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.setting.ui.SettingActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.episodetabcontainer.ui.EpisodeTabContainerActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.categorycomedy.ui.CategoryComedyActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.jusplaymoviestreamingapp.app.modules.chooseplan.ui.ChoosePlanActivity" android:screenOrientation="portrait"/>
    </application>
</manifest>
