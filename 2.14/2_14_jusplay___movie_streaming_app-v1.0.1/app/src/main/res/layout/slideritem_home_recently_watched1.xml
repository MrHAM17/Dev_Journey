<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="imageSliderWidgetModel" type="com.jusplaymoviestreamingapp.app.modules.homerecentlywatched.data.model.ImageSliderWidgetModel"/>
    </data>
    <ImageView android:id="@+id/imageImage"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:scaleType="fitXY"
        imageUrl="@{imageSliderWidgetModel.imageImage}"
        placeHolder="@{@drawable/image_not_found}" tools:ignore="ContentDescription"/>
</layout>
