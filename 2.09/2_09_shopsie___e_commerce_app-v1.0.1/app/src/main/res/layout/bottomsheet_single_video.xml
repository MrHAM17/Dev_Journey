<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="singleVideoVM" type="com.shopsieecommerceapp.app.modules.singlevideo.data.viewmodel.SingleVideoVM"/>
    </data>
    <LinearLayout android:id="@+id/linearSingleVideo"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:id="@+id/linearCard"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylenamewhite_1">
            <ImageView android:id="@+id/imageIndicatorOnLight"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_5pxh"
                android:layout_width="@dimen/_38pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_indicator_on_light"
                tools:ignore="ContentDescription" tools:src="@drawable/img_indicator_on_light"/>
            <FrameLayout android:id="@+id/frameVideoPlayerMobile"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_200pxv" android:layout_width="@dimen/_319pxh">
                <ImageView android:id="@+id/imageVideoBackground"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_200pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_8pxh}"
                    imageRes="@{@drawable/img_image_119x283}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image_119x283"/>
                <LinearLayout android:id="@+id/linearRefresh"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearRefresh1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageRefresh"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_18pxh"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_refresh"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_refresh"/>
                        <ImageView android:id="@+id/imageThumbsUp"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_width="@dimen/_28pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_thumbs_up"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_thumbs_up"/>
                        <ImageView android:id="@+id/imageContrast"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_18pxh"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_contrast"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_contrast"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearBottom"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTime"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{singleVideoVM.singleVideoModel.txtTime}"
                            style="@style/txtInterregular12_1" tools:text="@string/lbl_01_23"/>
                        <ProgressBar android:id="@+id/progressBarLines"
                            android:indeterminate="false"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:layout_width="0dp" android:max="100"
                            android:minHeight="@dimen/_4pxv"
                            android:progress="60"
                            android:progressDrawable="@drawable/layer_list_bg_name_blue_gray_100_opacity_0_42_radius_2" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTime1"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{singleVideoVM.singleVideoModel.txtTime1}"
                            style="@style/txtInterregular12_1" tools:text="@string/lbl_05_46"/>
                        <ImageView android:id="@+id/imageIconFullScreen"
                            android:layout_height="@dimen/_18pxh"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icon_fullscreen"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_icon_fullscreen"/>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
            <TextView android:gravity="start" android:id="@+id/txtTitle"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:text="@{singleVideoVM.singleVideoModel.txtTitle}"
                style="@style/txtInterbold22" tools:text="@string/msg_tropical_fresh_tourism"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearAuthor"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imagePlay"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_38pxh"
                    android:layout_width="@dimen/_38pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_play"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_play"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearName"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtName"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{singleVideoVM.singleVideoModel.txtName}"
                        style="@style/txtIntermedium14" tools:text="@string/lbl_edward_ford"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime2"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{singleVideoVM.singleVideoModel.txtTime2}"
                        style="@style/txtInterregular12" tools:text="@string/lbl_5_min_ago"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameSpacer"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <ImageButton
                    android:background="@drawable/rectangle_bg_name_primary_radius_12"
                    android:id="@+id/btnCheckmark"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_38pxh"
                    android:layout_width="@dimen/_38pxh"
                    android:paddingHorizontal="@dimen/_10pxh"
                    android:paddingVertical="@dimen/_10pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_checkmark_white_a700" tools:ignore="ContentDescription"/>
            </LinearLayout>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:text="@{singleVideoVM.singleVideoModel.txtDescription}"
                style="@style/txtInterregular14" tools:text="@string/msg_cancun_is_back"/>
            <FrameLayout android:gravity="start"
                android:id="@+id/frameTags"
                android:layout_height="wrap_content" android:layout_width="match_parent">
                <FrameLayout
                    android:background="@drawable/rectangle_bg_name_blue_gray_100_radius_4"
                    android:id="@+id/frameChipSmall"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_2pxv"
                    android:paddingEnd="@dimen/_8pxh"
                    android:paddingStart="@dimen/_8pxh" android:paddingTop="@dimen/_2pxv"/>
                <FrameLayout
                    android:background="@drawable/rectangle_bg_name_blue_gray_100_radius_4"
                    android:id="@+id/frameChipSmall1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_2pxv"
                    android:paddingEnd="@dimen/_7pxh"
                    android:paddingStart="@dimen/_7pxh" android:paddingTop="@dimen/_2pxv"/>
                <FrameLayout
                    android:background="@drawable/rectangle_bg_name_blue_gray_100_radius_4"
                    android:id="@+id/frameChipSmall2"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_2pxv"
                    android:paddingEnd="@dimen/_8pxh"
                    android:paddingStart="@dimen/_8pxh" android:paddingTop="@dimen/_2pxv"/>
                <FrameLayout
                    android:background="@drawable/rectangle_bg_name_blue_gray_100_radius_4"
                    android:id="@+id/frameChipSmall3"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_2pxv"
                    android:paddingEnd="@dimen/_7pxh"
                    android:paddingStart="@dimen/_7pxh" android:paddingTop="@dimen/_2pxv"/>
                <FrameLayout
                    android:background="@drawable/rectangle_bg_name_blue_gray_100_radius_4"
                    android:id="@+id/frameChipSmall4"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_2pxv"
                    android:paddingEnd="@dimen/_7pxh"
                    android:paddingStart="@dimen/_7pxh" android:paddingTop="@dimen/_2pxv"/>
            </FrameLayout>
            <FrameLayout android:gravity="center_horizontal"
                android:id="@+id/frameBottomInteractions"
                android:layout_height="@dimen/_118pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearModalMobile"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/_22pxv"
                    android:paddingEnd="@dimen/_168pxh"
                    android:paddingStart="@dimen/_168pxh"
                    android:paddingTop="@dimen/_22pxv" style="@style/groupStylenameonPrimarycornerRadius_1">
                    <ImageView android:id="@+id/imageIndicatorOnLight1"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_5pxh"
                        android:layout_width="@dimen/_38pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_indicator_on_light"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_indicator_on_light"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearTwentyTwo"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageFavorite"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_favorite_secondarycontainer"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_favorite_secondarycontainer"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLabel"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{singleVideoVM.singleVideoModel.txtLabel}"
                        style="@style/txtIntermedium14_2" tools:text="@string/lbl_326"/>
                    <FrameLayout android:id="@+id/frameSpacer1"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <ImageView android:id="@+id/imageComputer"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_computer"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_computer"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLabel1"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{singleVideoVM.singleVideoModel.txtLabel1}"
                        style="@style/txtIntermedium14_2" tools:text="@string/lbl_148"/>
                    <FrameLayout android:id="@+id/frameSpacer2"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <ImageView android:id="@+id/imageQuestion"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_question_white_a700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_question_white_a700"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLabel2"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{singleVideoVM.singleVideoModel.txtLabel2}"
                        style="@style/txtIntermedium14_2" tools:text="@string/lbl_share"/>
                    <FrameLayout android:id="@+id/frameSpacer3"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <ImageView android:id="@+id/imageIconOptions"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_icon_options"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_icon_options"/>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
