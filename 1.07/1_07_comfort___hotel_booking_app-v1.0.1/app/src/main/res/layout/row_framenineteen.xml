<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearThemeDarkComponentReview"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingBottom="@dimen/_22pxv"
        android:paddingEnd="@dimen/_24pxh"
        android:paddingStart="@dimen/_24pxh"
        android:paddingTop="@dimen/_22pxv" style="@style/groupStyle{&quot;name&quot;:&quot;blue_gray_900&quot;}cornerRadius">
        <LinearLayout android:id="@+id/linearAutoLayoutHorizontal"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageJennyWilson"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_48pxh"
                android:layout_width="@dimen/_48pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_ellipse_1}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_1"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearAutoLayoutVertical"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtJennyWilson"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/lbl_jenny_wilson"
                    style="@style/txtUrbanistromanbold16" tools:text="@string/lbl_jenny_wilson"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDec"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/lbl_dec_10_2024"
                    style="@style/txtUrbanistromanmedium12_1" tools:text="@string/lbl_dec_10_2024"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameSpacer"
                android:layout_height="wrap_content" android:layout_width="wrap_content"/>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_primary_radius_16"
                android:drawablePadding="@dimen/_8pxh"
                android:drawableStart="@drawable/img_signal"
                android:ellipsize="end"
                android:fontFamily="@font/urbanist"
                android:gravity="center" android:id="@+id/btnFive"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_32pxv"
                android:layout_width="@dimen/_60pxh"
                android:paddingBottom="@dimen/_7pxv"
                android:paddingEnd="@dimen/_16pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_7pxv"
                android:singleLine="true" android:text="@string/lbl_5"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:text="@string/lbl_5"/>
        </LinearLayout>
        <TextView android:id="@+id/txtVeryniceandcomfortab"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:text="@string/msg_very_nice_and_comfortable"
            style="@style/txtUrbanistregular14_2" tools:text="@string/msg_very_nice_and_comfortable"/>
    </LinearLayout>
</layout>
