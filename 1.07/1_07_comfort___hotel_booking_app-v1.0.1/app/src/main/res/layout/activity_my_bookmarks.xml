<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearArrowDown"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;onPrimary&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_down"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtMyBookmarks"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_my_bookmarks"
                style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_my_bookmarks"/>
            <LinearLayout android:id="@+id/linearGroup33"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton android:id="@+id/btnMegaphone"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_megaphone" tools:ignore="ContentDescription"/>
                <ImageView android:id="@+id/imageGrid"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_grid_primary"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_grid_primary"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollView"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearMyBookmarks"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.recyclerview.widget.RecyclerView
                    android:gravity="center_horizontal"
                    android:id="@+id/recyclerMyBookmarks"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:nestedScrollingEnabled="false"
                    app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
                    app:spanCount="2" tools:itemCount="4" tools:listitem="@layout/row_mybookmarks"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearPresidentMilaDe"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/linearTypeCardThemeDark"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:orientation="vertical"
                        android:paddingBottom="@dimen/_20pxv"
                        android:paddingEnd="@dimen/_18pxh"
                        android:paddingStart="@dimen/_18pxh"
                        android:paddingTop="@dimen/_20pxv" style="@style/groupStylenameblue_gray_5">
                        <ImageView android:id="@+id/imageImage"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_120pxh"
                            android:layout_width="@dimen/_140pxh"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_16pxh}"
                            imageRes="@{@drawable/img_rectangle}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle"/>
                        <TextView android:gravity="start"
                            android:id="@+id/txtPresidentMilaDe"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:text="@string/msg_grand_resort_lagonissi"
                            style="@style/txtUrbanistromanbold18" tools:text="@string/msg_grand_resort_lagonissi"/>
                        <LinearLayout
                            android:id="@+id/linearAutoLayoutHorizontal"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageSignal"
                                android:layout_height="@dimen/_12pxh"
                                android:layout_width="@dimen/_12pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_signal_yellow_a700"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_signal_yellow_a700"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtFortyEight"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@string/lbl_4_8"
                                style="@style/txtUrbanistsemibold14_1" tools:text="@string/lbl_4_8"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtParisFrance"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.01"
                                android:singleLine="true"
                                android:text="@string/lbl_paris_france"
                                style="@style/txtUrbanistregular12" tools:text="@string/lbl_paris_france"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearAutoLayoutHorizontal1"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtPrice"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@string/lbl_35"
                                style="@style/txtUrbanistromanbold20" tools:text="@string/lbl_35"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtNight"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:letterSpacing="0.01"
                                android:singleLine="true"
                                android:text="@string/lbl_night"
                                style="@style/txtUrbanistregular10" tools:text="@string/lbl_night"/>
                            <ImageView android:id="@+id/imageBookmark"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_24pxh"
                                android:layout_width="@dimen/_24pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_bookmark_primary"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_bookmark_primary"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearTypeCardThemeDark1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:orientation="vertical"
                        android:paddingBottom="@dimen/_20pxv"
                        android:paddingEnd="@dimen/_18pxh"
                        android:paddingStart="@dimen/_18pxh"
                        android:paddingTop="@dimen/_20pxv" style="@style/groupStylenameblue_gray_5">
                        <ImageView android:id="@+id/imageImage1"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_120pxh"
                            android:layout_width="@dimen/_140pxh"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_16pxh}"
                            imageRes="@{@drawable/img_rectangle_5}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_5"/>
                        <TextView android:gravity="center_horizontal"
                            android:id="@+id/txtPresidentMilaDe1"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:text="@string/msg_martinez_cannes"
                            style="@style/txtUrbanistromanbold18" tools:text="@string/msg_martinez_cannes"/>
                        <LinearLayout
                            android:id="@+id/linearAutoLayoutHorizontal2"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageSignal1"
                                android:layout_height="@dimen/_12pxh"
                                android:layout_width="@dimen/_12pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_signal_yellow_a700"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_signal_yellow_a700"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtFortyEight1"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@string/lbl_4_8"
                                style="@style/txtUrbanistsemibold14_1" tools:text="@string/lbl_4_8"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtParisFrance1"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.01"
                                android:singleLine="true"
                                android:text="@string/lbl_paris_france"
                                style="@style/txtUrbanistregular12" tools:text="@string/lbl_paris_france"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearAutoLayoutHorizontal3"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtPrice1"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@string/lbl_35"
                                style="@style/txtUrbanistromanbold20" tools:text="@string/lbl_35"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtNight1"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:letterSpacing="0.01"
                                android:singleLine="true"
                                android:text="@string/lbl_night"
                                style="@style/txtUrbanistregular10" tools:text="@string/lbl_night"/>
                            <ImageView android:id="@+id/imageBookmark1"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_24pxh"
                                android:layout_width="@dimen/_24pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_bookmark_primary"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_bookmark_primary"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>
</layout>
