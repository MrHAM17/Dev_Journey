<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="selectAppAlarmVM" type="com.vertexrealestateapp.app.modules.selectappalarm.data.viewmodel.SelectAppAlarmVM"/>
    </data>
    <LinearLayout android:id="@+id/linearEdit"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;gray_50&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStyle{&quot;name&quot;:&quot;gray_50&quot;}">
            <ImageButton
                android:background="@drawable/rectangle_bg_name_white_a700_border_name_gray_300_radius_20"
                android:id="@+id/btnArrowLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrow_left_primarycontainer" tools:ignore="ContentDescription"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNotification"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@{selectAppAlarmVM.selectAppAlarmModel.txtNotification}"
                style="@style/txtManropeextrabold18" tools:text="@string/msg_review_your_tour"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearSelectAppAlarm"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingHorizontal="@dimen/_24pxh" android:paddingVertical="@dimen/_24pxv">
            <LinearLayout android:id="@+id/linearMightyCincoFamily"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_15pxv"
                android:paddingEnd="@dimen/_19pxh"
                android:paddingStart="@dimen/_19pxh"
                android:paddingTop="@dimen/_15pxv" style="@style/groupStylecornerRadius">
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtMightyCincoFamily"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{selectAppAlarmVM.selectAppAlarmModel.txtMightyCincoFamily}"
                    style="@style/txtManropeextrabold18" tools:text="@string/msg_mighty_cinco_family"/>
                <LinearLayout android:id="@+id/linearLocation"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageLinkedin"
                        android:layout_gravity="top"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_linkedin_primary"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_linkedin_primary"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_vertical"
                        android:id="@+id/txtJakartaIndonesia"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.02"
                        android:singleLine="true"
                        android:text="@{selectAppAlarmVM.selectAppAlarmModel.txtJakartaIndonesia}"
                        style="@style/txtManrope12_1" tools:text="@string/msg_jakarta_indonesia"/>
                </LinearLayout>
                <View
                    android:background="@color/{&quot;name&quot;:&quot;gray_300&quot;}"
                    android:id="@+id/lineLine"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_287pxh"/>
                <LinearLayout android:id="@+id/linearDate"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTittle"
                        android:layout_gravity="top"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{selectAppAlarmVM.selectAppAlarmModel.txtTittle}"
                        style="@style/txtManropesemibold14_1" tools:text="@string/lbl_date"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMonth"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{selectAppAlarmVM.selectAppAlarmModel.txtMonth}"
                        style="@style/txtManropebold16" tools:text="@string/lbl_mon_april_4"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearTime"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTittle1"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{selectAppAlarmVM.selectAppAlarmModel.txtTittle1}"
                        style="@style/txtManropesemibold14_1" tools:text="@string/lbl_time"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{selectAppAlarmVM.selectAppAlarmModel.txtTime}"
                        style="@style/txtManropebold16" tools:text="@string/lbl_4_00_4_45_pm"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearErrorMsg"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/_15pxv"
                android:paddingEnd="@dimen/_16pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_15pxv" style="@style/groupStyle{&quot;name&quot;:&quot;red_400_19&quot;,&quot;opacity&quot;:0.05}cornerRadius">
                <ImageView android:id="@+id/imageVideoCamera"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_video_camera_secondarycontainer"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_video_camera_secondarycontainer"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearText"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtYourIdentityIs"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{selectAppAlarmVM.selectAppAlarmModel.txtYourIdentityIs}"
                        style="@style/txtManropebold14_2" tools:text="@string/msg_your_identity_is"/>
                    <TextView android:id="@+id/txtVerifyYourIdentity"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.02"
                        android:text="@{selectAppAlarmVM.selectAppAlarmModel.txtVerifyYourIdentity}"
                        style="@style/txtManrope12_2" tools:text="@string/msg_verify_your_identity"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearBTN"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStyle">
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_gray_50_border_name_primary_radius_10"
                android:ellipsize="end"
                android:fontFamily="@font/manrope"
                android:gravity="center" android:id="@+id/btnEdit"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_56pxv"
                android:layout_width="@dimen/_156pxh"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@string/lbl_edit"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                android:textFontWeight="700"
                android:textSize="@dimen/_16pxh"
                android:textStyle="bold" tools:text="@string/lbl_edit"/>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_primary_radius_10"
                android:ellipsize="end"
                android:fontFamily="@font/manrope"
                android:gravity="center" android:id="@+id/btnSchedule"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_56pxv"
                android:layout_width="@dimen/_156pxh"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@string/lbl_schedule"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                android:textFontWeight="700"
                android:textSize="@dimen/_16pxh"
                android:textStyle="bold" tools:text="@string/lbl_schedule"/>
        </LinearLayout>
    </LinearLayout>
</layout>
