<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="filterVM" type="com.vertexrealestateapp.app.modules.filter.data.viewmodel.FilterVM"/>
    </data>
    <ScrollView android:clipToPadding="false"
        android:fillViewport="true"
        android:id="@+id/scrollViewScrollView"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStyle{&quot;name&quot;:&quot;gray_50&quot;}">
        <LinearLayout android:id="@+id/linearReset"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearReset1"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearPriceRange"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_24pxh" android:paddingStart="@dimen/_24pxh">
                    <LinearLayout android:id="@+id/linearTitleAndPrice"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPriceRange"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{filterVM.filterModel.txtPriceRange}"
                            style="@style/txtManropeextrabold16_1" tools:text="@string/lbl_price_range"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPrice"
                            android:layout_gravity="top"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{filterVM.filterModel.txtPrice}"
                            style="@style/txtManropesemibold14_2" tools:text="@string/lbl_200_15_000"/>
                    </LinearLayout>
                    <FrameLayout android:id="@+id/frameLine"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_24pxv" android:layout_width="@dimen/_327pxh">
                        <View
                            android:background="@color/{&quot;name&quot;:&quot;gray_300&quot;}"
                            android:id="@+id/lineLine"
                            android:layout_gravity="bottom|center"
                            android:layout_height="@dimen/_3pxv" android:layout_width="@dimen/_327pxh"/>
                        <LinearLayout
                            android:id="@+id/linearChevronLeft"
                            android:layout_gravity="center"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <LinearLayout
                                android:gravity="center_vertical"
                                android:id="@+id/linearButton"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:orientation="horizontal"
                                android:paddingBottom="@dimen/_7pxv"
                                android:paddingEnd="@dimen/_3pxh"
                                android:paddingStart="@dimen/_3pxh"
                                android:paddingTop="@dimen/_7pxv" style="@style/groupStyle{&quot;name&quot;:&quot;primary&quot;}cornerRadius">
                                <ImageView
                                    android:id="@+id/imageChevronLeft"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="@dimen/_10pxh"
                                    android:layout_width="@dimen/_10pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_chevron_left"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_chevron_left"/>
                                <ImageView
                                    android:id="@+id/imageChevronRight"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="@dimen/_10pxh"
                                    android:layout_width="@dimen/_10pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_chevron_right"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_chevron_right"/>
                            </LinearLayout>
                            <View
                                android:background="@color/{&quot;name&quot;:&quot;primary&quot;}"
                                android:id="@+id/lineLine1"
                                android:layout_gravity="bottom"
                                android:layout_height="@dimen/_3pxv" android:layout_width="@dimen/_103pxh"/>
                            <LinearLayout
                                android:gravity="center_vertical"
                                android:id="@+id/linearButton1"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:orientation="horizontal"
                                android:paddingBottom="@dimen/_7pxv"
                                android:paddingEnd="@dimen/_3pxh"
                                android:paddingStart="@dimen/_3pxh"
                                android:paddingTop="@dimen/_7pxv" style="@style/groupStyle{&quot;name&quot;:&quot;primary&quot;}cornerRadius">
                                <ImageView
                                    android:id="@+id/imageChevronLeft1"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="@dimen/_10pxh"
                                    android:layout_width="@dimen/_10pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_chevron_left"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_chevron_left"/>
                                <ImageView
                                    android:id="@+id/imageChevronRight1"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="@dimen/_10pxh"
                                    android:layout_width="@dimen/_10pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_chevron_right"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_chevron_right"/>
                            </LinearLayout>
                        </LinearLayout>
                    </FrameLayout>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearFeatures"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_24pxh" android:paddingStart="@dimen/_24pxh">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFeatures"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{filterVM.filterModel.txtFeatures}"
                        style="@style/txtManropeextrabold16_1" tools:text="@string/lbl_features"/>
                    <LinearLayout android:id="@+id/linearBeds"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <LinearLayout android:gravity="center_vertical"
                            android:id="@+id/linearBeds1"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtBeds"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:letterSpacing="0.01"
                                android:singleLine="true"
                                android:text="@{filterVM.filterModel.txtBeds}"
                                style="@style/txtManropemedium16" tools:text="@string/lbl_beds"/>
                            <FrameLayout android:id="@+id/frameSpacer"
                                android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                            <ImageButton android:id="@+id/btnArrowLeft"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_24pxh"
                                android:layout_width="@dimen/_24pxh"
                                android:scaleType="fitCenter"
                                android:src="@drawable/img_arrow_left_gray_300" tools:ignore="ContentDescription"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtFour"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:letterSpacing="0.01"
                                android:singleLine="true"
                                android:text="@{filterVM.filterModel.txtFour}"
                                style="@style/txtManropeextrabold16_1" tools:text="@string/lbl_42"/>
                            <ImageButton
                                android:id="@+id/btnFloatingIcon"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_24pxh"
                                android:layout_width="@dimen/_24pxh"
                                android:scaleType="fitCenter"
                                android:src="@drawable/img_floating_icon" tools:ignore="ContentDescription"/>
                        </LinearLayout>
                        <LinearLayout android:gravity="center_vertical"
                            android:id="@+id/linearBaths"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtBaths"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:letterSpacing="0.01"
                                android:singleLine="true"
                                android:text="@{filterVM.filterModel.txtBaths}"
                                style="@style/txtManropemedium16" tools:text="@string/lbl_baths"/>
                            <FrameLayout android:id="@+id/frameSpacer1"
                                android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                            <ImageButton android:id="@+id/btnArrowLeft1"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_24pxh"
                                android:layout_width="@dimen/_24pxh"
                                android:scaleType="fitCenter"
                                android:src="@drawable/img_arrow_left_gray_300" tools:ignore="ContentDescription"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTwo"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:letterSpacing="0.01"
                                android:singleLine="true"
                                android:text="@{filterVM.filterModel.txtTwo}"
                                style="@style/txtManropeextrabold16_1" tools:text="@string/lbl_2"/>
                            <ImageButton
                                android:id="@+id/btnFloatingIcon1"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_24pxh"
                                android:layout_width="@dimen/_24pxh"
                                android:scaleType="fitCenter"
                                android:src="@drawable/img_floating_icon" tools:ignore="ContentDescription"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPropertyFacts"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{filterVM.filterModel.txtPropertyFacts}"
                    style="@style/txtManropeextrabold16_1" tools:text="@string/lbl_property_facts"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearSquareFeet"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearSquareFeet1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSquareFeet"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{filterVM.filterModel.txtSquareFeet}"
                            style="@style/txtManropesemibold14" tools:text="@string/lbl_square_feet"/>
                        <Spinner
                            android:background="@drawable/layer_list_bg_name_gray_50_border_name_blue_gray_500_radius_12"
                            android:id="@+id/spinnerMin"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowDown"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_down"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
                    <Spinner
                        android:background="@drawable/layer_list_bg_name_gray_50_border_name_blue_gray_500_radius_12"
                        android:id="@+id/spinnerMin1"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearSquareFeet2"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_24pxh" android:paddingStart="@dimen/_24pxh">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLotSize"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{filterVM.filterModel.txtLotSize}"
                        style="@style/txtManropesemibold14" tools:text="@string/lbl_lot_size"/>
                    <LinearLayout android:id="@+id/linearInputs"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <Spinner
                            android:background="@drawable/layer_list_bg_name_gray_50_border_name_blue_gray_500_radius_12"
                            android:gravity="center_vertical"
                            android:id="@+id/spinnerMin2"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
                        <ImageView android:id="@+id/imageArrowDown1"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrow_down"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
                        <Spinner
                            android:background="@drawable/layer_list_bg_name_gray_50_border_name_blue_gray_500_radius_12"
                            android:gravity="center_vertical"
                            android:id="@+id/spinnerMin3"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearPropertyType"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_24pxh" android:paddingStart="@dimen/_24pxh">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPropertyType"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{filterVM.filterModel.txtPropertyType}"
                        style="@style/txtManropeextrabold16_1" tools:text="@string/lbl_property_type"/>
                    <FrameLayout android:id="@+id/frameHome"
                        android:layout_height="wrap_content" android:layout_width="match_parent">
                        <FrameLayout
                            android:background="@drawable/rectangle_bg_name_gray_50_border_name_blue_gray_500_radius_18"
                            android:id="@+id/frameHome1"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh" android:paddingTop="@dimen/_8pxv"/>
                        <FrameLayout
                            android:background="@drawable/rectangle_bg_name_gray_50_border_name_blue_gray_500_radius_18"
                            android:id="@+id/frameVilla"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh" android:paddingTop="@dimen/_8pxv"/>
                        <FrameLayout
                            android:background="@drawable/rectangle_bg_name_gray_50_border_name_blue_gray_500_radius_18"
                            android:id="@+id/frameBreakfastIncluded"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh" android:paddingTop="@dimen/_8pxv"/>
                        <FrameLayout
                            android:background="@drawable/rectangle_bg_name_gray_50_border_name_blue_gray_500_radius_18"
                            android:id="@+id/frameHome2"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh" android:paddingTop="@dimen/_8pxv"/>
                        <FrameLayout
                            android:background="@drawable/rectangle_bg_name_gray_50_border_name_blue_gray_500_radius_18"
                            android:id="@+id/frameVilla1"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh" android:paddingTop="@dimen/_8pxv"/>
                    </FrameLayout>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearAmenities"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_24pxh" android:paddingStart="@dimen/_24pxh">
                    <LinearLayout android:id="@+id/linearTitle"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:gravity="center_vertical"
                            android:id="@+id/txtAmenities"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{filterVM.filterModel.txtAmenities}"
                            style="@style/txtManropeextrabold16_1" tools:text="@string/lbl_amenities"/>
                        <ImageView android:id="@+id/imageVideoCamera"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_video_camera"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_video_camera"/>
                    </LinearLayout>
                    <FrameLayout android:id="@+id/frameOptions"
                        android:layout_height="wrap_content" android:layout_width="match_parent">
                        <FrameLayout
                            android:background="@drawable/rectangle_bg_name_gray_50_border_name_blue_gray_500_radius_18"
                            android:id="@+id/frameFreeWiFi"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh" android:paddingTop="@dimen/_8pxv"/>
                        <FrameLayout
                            android:background="@drawable/rectangle_bg_name_gray_50_border_name_blue_gray_500_radius_18"
                            android:id="@+id/framePool"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh" android:paddingTop="@dimen/_8pxv"/>
                        <FrameLayout
                            android:background="@drawable/rectangle_bg_name_gray_50_border_name_blue_gray_500_radius_18"
                            android:id="@+id/framePool1"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh" android:paddingTop="@dimen/_8pxv"/>
                        <FrameLayout
                            android:background="@drawable/rectangle_bg_name_gray_50_border_name_blue_gray_500_radius_18"
                            android:id="@+id/frameAirConditioning"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh" android:paddingTop="@dimen/_8pxv"/>
                        <FrameLayout
                            android:background="@drawable/rectangle_bg_name_gray_50_border_name_blue_gray_500_radius_18"
                            android:id="@+id/frameSpa"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh" android:paddingTop="@dimen/_8pxv"/>
                        <FrameLayout
                            android:background="@drawable/rectangle_bg_name_gray_50_border_name_blue_gray_500_radius_18"
                            android:id="@+id/frameView"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh" android:paddingTop="@dimen/_8pxv"/>
                        <FrameLayout
                            android:background="@drawable/rectangle_bg_name_gray_50_border_name_blue_gray_500_radius_18"
                            android:id="@+id/frameElevator"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh" android:paddingTop="@dimen/_8pxv"/>
                        <FrameLayout
                            android:background="@drawable/rectangle_bg_name_gray_50_border_name_blue_gray_500_radius_18"
                            android:id="@+id/frameGarageSpots"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh" android:paddingTop="@dimen/_8pxv"/>
                    </FrameLayout>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearSeeMore"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:gravity="center_vertical"
                        android:id="@+id/txtSeeMore"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{filterVM.filterModel.txtSeeMore}"
                        style="@style/txtManropebold14_1" tools:text="@string/lbl_see_more"/>
                    <ImageView android:id="@+id/imageArrowLeft"
                        android:layout_gravity="top"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_left"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_left"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearBTN"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:paddingHorizontal="@dimen/_24pxh"
                    android:paddingVertical="@dimen/_24pxv" style="@style/groupStyle">
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_gray_50_border_name_primary_radius_10"
                        android:ellipsize="end"
                        android:fontFamily="@font/manrope"
                        android:gravity="center"
                        android:id="@+id/btnReset"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_56pxv"
                        android:layout_width="@dimen/_156pxh"
                        android:letterSpacing="0.01"
                        android:paddingBottom="@dimen/_17pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_17pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_reset"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                        android:textFontWeight="700"
                        android:textSize="@dimen/_16pxh"
                        android:textStyle="bold" tools:text="@string/lbl_reset"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_primary_radius_10"
                        android:ellipsize="end"
                        android:fontFamily="@font/manrope"
                        android:gravity="center"
                        android:id="@+id/btnApply"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_56pxv"
                        android:layout_width="@dimen/_156pxh"
                        android:letterSpacing="0.01"
                        android:paddingBottom="@dimen/_17pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_17pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_apply"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;gray_50&quot;}"
                        android:textFontWeight="700"
                        android:textSize="@dimen/_16pxh"
                        android:textStyle="bold" tools:text="@string/lbl_apply"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</layout>
