<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="addNewPropertyMeetWithAAgentVM" type="com.vertexrealestateapp.app.modules.addnewpropertymeetwithaagent.data.viewmodel.AddNewPropertyMeetWithAAgentVM"/>
    </data>
    <LinearLayout android:id="@+id/linear0208"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;gray_50&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageButton
                android:background="@drawable/rectangle_bg_name_white_a700_border_name_gray_300_radius_20"
                android:id="@+id/btnArrowLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrow_left_primarycontainer" tools:ignore="ContentDescription"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNotification"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@{addNewPropertyMeetWithAAgentVM.addNewPropertyMeetWithAAgentModel.txtNotification}"
                style="@style/txtManropeextrabold18" tools:text="@string/msg_add_new_property"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearAddNewPropertyMeet"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_24pxv" android:paddingTop="@dimen/_24pxv">
            <LinearLayout android:id="@+id/linearAddressAndProgress"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMeetWithAAgent"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{addNewPropertyMeetWithAAgentVM.addNewPropertyMeetWithAAgentModel.txtMeetWithAAgent}"
                    style="@style/txtManropesemibold14" tools:text="@string/msg_meet_with_a_agent"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:background="@drawable/rectangle_bg_name_primary_radius_16"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope"
                    android:gravity="center"
                    android:id="@+id/btnTwoHundredEight"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_33pxv"
                    android:layout_width="@dimen/_79pxh"
                    android:paddingBottom="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_02_08"
                    android:textAllCaps="false"
                    android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="bold" tools:text="@string/lbl_02_08"/>
            </LinearLayout>
            <ProgressBar android:gravity="center_horizontal"
                android:id="@+id/progressBarProgressBar"
                android:indeterminate="false"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:max="100"
                android:minHeight="@dimen/_6pxv" android:progress="25"
                android:progressDrawable="@drawable/layer_list_bg_name_gray_300_radius_3" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
            <LinearLayout android:id="@+id/linearPropertyAddress"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/_14pxv"
                android:paddingEnd="@dimen/_15pxh"
                android:paddingStart="@dimen/_15pxh"
                android:paddingTop="@dimen/_14pxv" style="@style/groupStylecornerRadius">
                <ImageButton
                    android:background="@drawable/rectangle_bg_name_primarycontainer_radius_20"
                    android:id="@+id/btnLinkedin"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:paddingHorizontal="@dimen/_10pxh"
                    android:paddingVertical="@dimen/_10pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_linkedin" tools:ignore="ContentDescription"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearAddress"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPropertyAddress"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{addNewPropertyMeetWithAAgentVM.addNewPropertyMeetWithAAgentModel.txtPropertyAddress}"
                        style="@style/txtManropebold14" tools:text="@string/msg_property_address"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtWoodlandSt"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.02"
                        android:singleLine="true"
                        android:text="@{addNewPropertyMeetWithAAgentVM.addNewPropertyMeetWithAAgentModel.txtWoodlandSt}"
                        style="@style/txtManrope12" tools:text="@string/msg_woodland_st_105"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearTitle"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtJanuary"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{addNewPropertyMeetWithAAgentVM.addNewPropertyMeetWithAAgentModel.txtJanuary}"
                    style="@style/txtManropebold18" tools:text="@string/lbl_january"/>
                <FrameLayout android:id="@+id/frameSpacer"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <ImageButton
                    android:background="@drawable/rectangle_border_name_gray_300_radius_5"
                    android:id="@+id/btnIcons"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_32pxh"
                    android:layout_width="@dimen/_32pxh"
                    android:paddingHorizontal="@dimen/_8pxh"
                    android:paddingVertical="@dimen/_8pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_arrow_left_primarycontainer" tools:ignore="ContentDescription"/>
                <ImageButton
                    android:background="@drawable/rectangle_border_name_gray_300_radius_5"
                    android:id="@+id/btnIcons1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_32pxh"
                    android:layout_width="@dimen/_32pxh"
                    android:paddingHorizontal="@dimen/_8pxh"
                    android:paddingVertical="@dimen/_8pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_icons" tools:ignore="ContentDescription"/>
            </LinearLayout>

            <com.prolificinteractive.materialcalendarview.MaterialCalendarView
                android:gravity="end" android:id="@+id/calendarDates"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:mcv_selectionMode="single"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPickATime"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@{addNewPropertyMeetWithAAgentVM.addNewPropertyMeetWithAAgentModel.txtPickATime}"
                style="@style/txtManropebold18" tools:text="@string/lbl_pick_a_time"/>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="end" android:id="@+id/recyclerTime"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_10pxh}" tools:itemCount="4" tools:listitem="@layout/row_time"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearBTN"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:orientation="vertical" style="@style/groupStyle">
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_primary_radius_10"
                android:ellipsize="end"
                android:fontFamily="@font/manrope"
                android:gravity="center" android:id="@+id/btnNext"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@string/lbl_next"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;gray_50&quot;}"
                android:textFontWeight="700"
                android:textSize="@dimen/_16pxh"
                android:textStyle="bold" tools:text="@string/lbl_next"/>
        </LinearLayout>
    </LinearLayout>
</layout>
