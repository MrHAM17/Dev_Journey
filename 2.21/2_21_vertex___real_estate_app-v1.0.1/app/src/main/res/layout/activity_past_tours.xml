<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="pastToursVM" type="com.vertexrealestateapp.app.modules.pasttours.data.viewmodel.PastToursVM"/>
    </data>
    <LinearLayout android:id="@+id/linearComplete"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;gray_50&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStyle{&quot;name&quot;:&quot;gray_50&quot;}">
            <ImageButton
                android:background="@drawable/rectangle_bg_name_white_a700_border_name_gray_300_radius_20"
                android:id="@+id/btnArrowLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrow_left_primarycontainer" tools:ignore="ContentDescription"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNotification"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@{pastToursVM.pastToursModel.txtNotification}"
                style="@style/txtManropeextrabold18" tools:text="@string/lbl_past_tours"/>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollView"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearPastTours"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearOne"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_15pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_13pxv" style="@style/groupStylecornerRadius">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{pastToursVM.pastToursModel.txtTime}"
                        style="@style/txtManropesemibold14" tools:text="@string/msg_mon_apr_4_10_00"/>
                    <LinearLayout android:id="@+id/linearMSG"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageClose"
                            android:layout_gravity="top"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_close"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_vertical"
                            android:id="@+id/txtCanceled"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.02"
                            android:singleLine="true"
                            android:text="@{pastToursVM.pastToursModel.txtCanceled}"
                            style="@style/txtManropesemibold12_2" tools:text="@string/lbl_canceled"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearYouCanRequest"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:gravity="start"
                            android:id="@+id/txtYouCanRequest"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.02"
                            android:text="@{pastToursVM.pastToursModel.txtYouCanRequest}"
                            style="@style/txtManropemedium12_2" tools:text="@string/msg_you_can_request"/>
                        <ImageView android:gravity="center_horizontal"
                            android:id="@+id/imageImage"
                            android:layout_height="@dimen/_180pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_5pxh}"
                            imageRes="@{@drawable/img_rectangle_4259}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_4259"/>
                        <LinearLayout
                            android:id="@+id/linearGoldenMeadows"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearTitleAndLocation"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtGoldenMeadows"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:letterSpacing="0.01"
                                    android:singleLine="true"
                                    android:text="@{pastToursVM.pastToursModel.txtGoldenMeadows}"
                                    style="@style/txtManropeextrabold16_1" tools:text="@string/lbl_golden_meadows"/>
                                <LinearLayout
                                    android:id="@+id/linearLocation"
                                    android:layout_height="wrap_content"
                                    android:layout_width="wrap_content" android:orientation="horizontal">
                                    <ImageView
                                    android:id="@+id/imageIconMapPin"
                                    android:layout_gravity="top"
                                    android:layout_height="@dimen/_14pxh"
                                    android:layout_width="@dimen/_14pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_icon_map_pin"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_icon_map_pin"/>
                                    <TextView android:ellipsize="end"
                                    android:gravity="center_vertical"
                                    android:id="@+id/txtStCelinaDelaware"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:letterSpacing="0.02"
                                    android:singleLine="true"
                                    android:text="@{pastToursVM.pastToursModel.txtStCelinaDelaware}"
                                    style="@style/txtManrope12" tools:text="@string/msg_st_celina_delaware2"/>
                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/linearRateAndDuration"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:gravity="end"
                                    android:id="@+id/txtPrice"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:letterSpacing="0.02"
                                    android:singleLine="true"
                                    android:text="@{pastToursVM.pastToursModel.txtPrice}"
                                    style="@style/txtManrope12" tools:text="@string/lbl_price"/>
                                <TextView android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtPrice1"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:letterSpacing="0.01"
                                    android:singleLine="true"
                                    android:text="@{pastToursVM.pastToursModel.txtPrice1}"
                                    style="@style/txtManropeextrabold18_1" tools:text="@string/lbl_436"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearTwo"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_15pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_13pxv" style="@style/groupStylecornerRadius">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime1"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{pastToursVM.pastToursModel.txtTime1}"
                        style="@style/txtManropesemibold14" tools:text="@string/msg_mon_apr_4_10_00"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearComplete1"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <androidx.appcompat.widget.AppCompatCheckBox
                            android:drawablePadding="@dimen/_4pxh"
                            android:drawableStart="@drawable/img_checkmark_onerror"
                            android:ellipsize="end"
                            android:fontFamily="@font/manrope"
                            android:id="@+id/checkBoxComplete"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:letterSpacing="0.02"
                            android:singleLine="true"
                            android:text="@string/lbl_complete"
                            android:textColor="@color/{&quot;name&quot;:&quot;onError&quot;}"
                            android:textFontWeight="600"
                            android:textSize="@dimen/_12pxh"
                            android:textStyle="bold" tools:text="@string/lbl_complete"/>
                        <TextView android:id="@+id/txtYouCanRequest1"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.02"
                            android:text="@{pastToursVM.pastToursModel.txtYouCanRequest1}"
                            style="@style/txtManropemedium12_2" tools:text="@string/msg_you_can_request"/>
                        <ImageView android:id="@+id/imageImage1"
                            android:layout_height="@dimen/_180pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_5pxh}"
                            imageRes="@{@drawable/img_rectangle_4259_180x295}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_4259_180x295"/>
                        <LinearLayout
                            android:id="@+id/linearGoldenMeadows1"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearTitleAndLocation1"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtGoldenMeadows1"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:letterSpacing="0.01"
                                    android:singleLine="true"
                                    android:text="@{pastToursVM.pastToursModel.txtGoldenMeadows1}"
                                    style="@style/txtManropeextrabold16_1" tools:text="@string/lbl_golden_meadows"/>
                                <LinearLayout
                                    android:id="@+id/linearLocation1"
                                    android:layout_height="wrap_content"
                                    android:layout_width="wrap_content" android:orientation="horizontal">
                                    <ImageView
                                    android:id="@+id/imageIconMapPin1"
                                    android:layout_gravity="top"
                                    android:layout_height="@dimen/_14pxh"
                                    android:layout_width="@dimen/_14pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_icon_map_pin"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_icon_map_pin"/>
                                    <TextView android:ellipsize="end"
                                    android:gravity="center_vertical"
                                    android:id="@+id/txtStCelinaDelaware1"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:letterSpacing="0.02"
                                    android:singleLine="true"
                                    android:text="@{pastToursVM.pastToursModel.txtStCelinaDelaware1}"
                                    style="@style/txtManrope12" tools:text="@string/msg_st_celina_delaware2"/>
                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/linearRateAndDuration1"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:gravity="end"
                                    android:id="@+id/txtPrice2"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:letterSpacing="0.02"
                                    android:singleLine="true"
                                    android:text="@{pastToursVM.pastToursModel.txtPrice2}"
                                    style="@style/txtManrope12" tools:text="@string/lbl_price"/>
                                <TextView android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtPrice3"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:letterSpacing="0.01"
                                    android:singleLine="true"
                                    android:text="@{pastToursVM.pastToursModel.txtPrice3}"
                                    style="@style/txtManropeextrabold18_1" tools:text="@string/lbl_436"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
