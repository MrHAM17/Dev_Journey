<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearSplash"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/_339pxv"
        android:paddingEnd="@dimen/_74pxh"
        android:paddingStart="@dimen/_74pxh"
        android:paddingTop="@dimen/_339pxv" style="@style/groupStyle{&quot;name&quot;:&quot;primary&quot;}">
        <ImageView android:id="@+id/imageImage"
            android:layout_height="@dimen/_32pxh"
            android:layout_width="@dimen/_37pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_group_17"
            tools:ignore="ContentDescription" tools:src="@drawable/img_group_17"/>
        <LinearLayout android:id="@+id/linearFrame"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtSAFEBANK"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.09" android:singleLine="true"
                android:text="@string/lbl_safebank"
                style="@style/txtProximaNovaAltproximanovaabold28" tools:text="@string/lbl_safebank"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtWeServeWithFaith"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.08" android:singleLine="true"
                android:text="@string/msg_we_serve_with_faith"
                android:textAllCaps="true"
                style="@style/txtPoppinsmedium10_2" tools:text="@string/msg_we_serve_with_faith"/>
        </LinearLayout>
    </LinearLayout>
</layout>
