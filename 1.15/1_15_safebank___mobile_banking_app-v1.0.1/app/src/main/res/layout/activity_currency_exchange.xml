<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linear120000"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageButton
                android:background="@drawable/rectangle_border_name_gray_300_radius_17"
                android:id="@+id/btnArrowLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrow_left_black_900" tools:ignore="ContentDescription"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTransfer"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/msg_currency_exchange"
                style="@style/txtPoppinssemibold18" tools:text="@string/msg_currency_exchange"/>
            <ImageButton android:id="@+id/btnContrast"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_contrast_gray_300" tools:ignore="ContentDescription"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearCurrencyExchange"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_22pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_22pxv">
            <LinearLayout android:id="@+id/linearMastercard"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_30pxv"
                android:paddingEnd="@dimen/_20pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_30pxv" style="@style/groupStyle{&quot;name&quot;:&quot;indigo_50&quot;}cornerRadius">
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearMastercard1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageContrast"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_23pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_contrast_red_700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_contrast_red_700"/>
                    <ImageView android:id="@+id/imageOne"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_4pxh"
                        android:layout_width="@dimen/_32pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_1_gray_700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_1_gray_700"/>
                    <ImageView android:id="@+id/imageTwo"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_4pxh"
                        android:layout_width="@dimen/_32pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_1_gray_700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_1_gray_700"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtZipcode"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_4023"
                        style="@style/txtPoppinsmedium10_3" tools:text="@string/lbl_4023"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtZipcode1"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_5534"
                        style="@style/txtPoppinsmedium10_3" tools:text="@string/lbl_5534"/>
                    <FrameLayout android:id="@+id/frameSpacer"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <ImageView android:id="@+id/imageArrowDown"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_4pxh"
                        android:layout_width="@dimen/_8pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_down_onprimarycontainer"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down_onprimarycontainer"/>
                </LinearLayout>
                <View
                    android:background="@color/{&quot;name&quot;:&quot;primary&quot;}"
                    android:id="@+id/lineLineTwelve"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_285pxh"/>
                <LinearLayout android:id="@+id/linearFortySeven"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_border_name_primary_radius_5"
                        android:ellipsize="end"
                        android:fontFamily="@font/poppins"
                        android:gravity="center"
                        android:id="@+id/btnOneHundredTwentyThousand"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_30pxv"
                        android:layout_width="@dimen/_140pxh"
                        android:paddingBottom="@dimen/_6pxv"
                        android:paddingEnd="@dimen/_19pxh"
                        android:paddingStart="@dimen/_19pxh"
                        android:paddingTop="@dimen/_6pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_1_200_00"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;black_900&quot;}"
                        android:textFontWeight="500"
                        android:textSize="@dimen/_12pxh"
                        android:textStyle="bold" tools:text="@string/lbl_1_200_00"/>
                    <Spinner android:gravity="center_vertical"
                        android:id="@+id/spinnerUSD"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
                </LinearLayout>
            </LinearLayout>
            <ImageButton
                android:background="@drawable/rectangle_bg_name_indigo_50_radius_25"
                android:id="@+id/btnShare"
                android:layout_gravity="start"
                android:layout_height="@dimen/_50pxh"
                android:layout_width="@dimen/_50pxh"
                android:paddingHorizontal="@dimen/_16pxh"
                android:paddingVertical="@dimen/_16pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_share" tools:ignore="ContentDescription"/>
            <LinearLayout android:id="@+id/linearVisa"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_30pxv"
                android:paddingEnd="@dimen/_21pxh"
                android:paddingStart="@dimen/_21pxh"
                android:paddingTop="@dimen/_30pxv" style="@style/groupStyle{&quot;name&quot;:&quot;indigo_50&quot;}cornerRadius">
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearMastercard2"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageSettings"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_15pxh"
                        android:layout_width="@dimen/_47pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_settings"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
                    <ImageView android:id="@+id/imageOne1"
                        android:layout_gravity="bottom"
                        android:layout_height="@dimen/_4pxh"
                        android:layout_width="@dimen/_32pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_1_gray_500"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_1_gray_500"/>
                    <ImageView android:id="@+id/imageTwo1"
                        android:layout_gravity="bottom"
                        android:layout_height="@dimen/_4pxh"
                        android:layout_width="@dimen/_32pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_1_gray_500"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_1_gray_500"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtZipcode2"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_4023"
                        style="@style/txtPoppinsmedium10_3" tools:text="@string/lbl_4023"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtZipcode3"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_5534"
                        style="@style/txtPoppinsmedium10_3" tools:text="@string/lbl_5534"/>
                    <FrameLayout android:id="@+id/frameSpacer1"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <ImageView android:id="@+id/imageArrowDown1"
                        android:layout_gravity="bottom"
                        android:layout_height="@dimen/_4pxh"
                        android:layout_width="@dimen/_8pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_down_onprimarycontainer"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down_onprimarycontainer"/>
                </LinearLayout>
                <View
                    android:background="@color/{&quot;name&quot;:&quot;primary&quot;}"
                    android:id="@+id/lineLineTwelve1"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_285pxh"/>
                <LinearLayout android:id="@+id/linearFortyFive"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <EditText
                        android:background="@drawable/rectangle_border_name_primary_radius_5"
                        android:ellipsize="end"
                        android:fontFamily="@font/poppins"
                        android:hint="@string/lbl_1_500_00"
                        android:id="@+id/etPrice"
                        android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_6pxv"
                        android:paddingEnd="@dimen/_19pxh"
                        android:paddingStart="@dimen/_19pxh"
                        android:paddingTop="@dimen/_6pxv"
                        android:singleLine="true"
                        android:textColor="@color/{&quot;name&quot;:&quot;black_900&quot;}"
                        android:textColorHint="@color/{&quot;name&quot;:&quot;black_900&quot;}"
                        android:textFontWeight="500"
                        android:textSize="@dimen/_12pxh"
                        android:textStyle="bold" tools:ignore="Autofill"/>
                    <Spinner android:gravity="center_vertical"
                        android:id="@+id/spinnerUSD1"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearComment"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtComment"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_comment"
                    style="@style/txtPoppinsmedium16" tools:text="@string/lbl_comment"/>
                <EditText
                    android:background="@drawable/rectangle_border_name_gray_200_radius_10"
                    android:fontFamily="@font/poppins"
                    android:hint="@string/msg_enter_your_comment"
                    android:id="@+id/etComment" android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:textColor="@color/{&quot;name&quot;:&quot;secondaryContainer&quot;}"
                    android:textColorHint="@color/{&quot;name&quot;:&quot;secondaryContainer&quot;}"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
            </LinearLayout>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:background="@drawable/rectangle_bg_name_primary_radius_12"
            android:ellipsize="end" android:fontFamily="@font/poppins"
            android:gravity="center" android:id="@+id/btnTransferMoney"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_50pxv"
            android:layout_width="@dimen/_327pxh"
            android:paddingBottom="@dimen/_13pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_13pxv" android:singleLine="true"
            android:text="@string/lbl_transfer_money"
            android:textAllCaps="false"
            android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
            android:textFontWeight="600"
            android:textSize="@dimen/_16pxh" android:textStyle="bold" tools:text="@string/lbl_transfer_money"/>
    </LinearLayout>
</layout>
