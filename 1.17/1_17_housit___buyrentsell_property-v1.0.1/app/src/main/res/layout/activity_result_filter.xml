<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearResultFilter"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageButton
                android:background="@drawable/rectangle_bg_name_gray_100_radius_bl_25_br_25"
                android:id="@+id/btnArrowLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrow_left" tools:ignore="ContentDescription"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPageTitle"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_search_results"
                style="@style/txtRalewaybold14" tools:text="@string/lbl_search_results"/>
            <ImageButton
                android:background="@drawable/rectangle_bg_name_primary_radius_25"
                android:id="@+id/btnIconSetting"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_icon_setting_white_a700" tools:ignore="ContentDescription"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearHouse"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_10pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_10pxv">
            <androidx.appcompat.widget.SearchView
                android:background="@drawable/rectangle_bg_name_gray_100_radius_12"
                android:gravity="center_horizontal"
                android:id="@+id/searchViewSearch"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:theme="@style/searchViewsearch"
                app:iconifiedByDefault="false"
                app:queryBackground="@android:color/transparent"
                app:queryHint="@string/lbl_modern_house" app:searchIcon="@drawable/img_rewind"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearItemHeader"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFound"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_found"
                    style="@style/txtRalewaymedium18" tools:text="@string/lbl_found"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtText"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_0"
                    style="@style/txtRalewaybold18_1" tools:text="@string/lbl_0"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEstates"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_estates"
                    style="@style/txtRalewaymedium18" tools:text="@string/lbl_estates"/>
                <FrameLayout android:id="@+id/frameSpacer"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <LinearLayout android:id="@+id/linearButtonShow"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_8pxv"
                    android:paddingTop="@dimen/_8pxv" style="@style/groupStylenamegray_1">
                    <FrameLayout android:id="@+id/frameButtonShow"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_24pxv"
                        android:layout_width="@dimen/_36pxh"
                        android:paddingBottom="@dimen/_6pxv"
                        android:paddingEnd="@dimen/_12pxh"
                        android:paddingStart="@dimen/_12pxh"
                        android:paddingTop="@dimen/_6pxv" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}cornerRadius">
                        <ImageView android:id="@+id/imageGrid"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_12pxh"
                            android:layout_width="@dimen/_12pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_grid_onprimarycontainer"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_grid_onprimarycontainer"/>
                    </FrameLayout>
                    <FrameLayout android:id="@+id/frameButtonShow1"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_24pxv"
                        android:layout_width="@dimen/_36pxh"
                        android:paddingBottom="@dimen/_6pxv"
                        android:paddingEnd="@dimen/_12pxh"
                        android:paddingStart="@dimen/_12pxh"
                        android:paddingTop="@dimen/_6pxv" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}cornerRadius">
                        <ImageView android:id="@+id/imageIconHorizontal"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_12pxh"
                            android:layout_width="@dimen/_12pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icon_horizontal_blue_gray_800_01_12x12"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_icon_horizontal_blue_gray_800_01_12x12"/>
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
            <FrameLayout android:gravity="start"
                android:id="@+id/frameFilter"
                android:layout_height="wrap_content" android:layout_width="match_parent">
                <FrameLayout
                    android:background="@drawable/rectangle_gradient_s_name_green_a400_opacity_0_15_e_name_teal_a700_26_radius_25"
                    android:id="@+id/frameButtonFilter"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingEnd="@dimen/_10pxh" android:paddingTop="@dimen/_17pxv"/>
                <FrameLayout
                    android:background="@drawable/rectangle_gradient_s_name_green_a400_opacity_0_15_e_name_teal_a700_26_radius_25"
                    android:id="@+id/frameButtonFilter1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingEnd="@dimen/_11pxh" android:paddingTop="@dimen/_17pxv"/>
            </FrameLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerLayout"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_10pxh}" tools:itemCount="3" tools:listitem="@layout/row_layout4"/>
        </LinearLayout>
    </LinearLayout>
</layout>
