<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearEstatesCardHorizontal"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:paddingHorizontal="@dimen/_8pxh"
        android:paddingVertical="@dimen/_8pxv" style="@style/groupStyle{&quot;name&quot;:&quot;gray_100&quot;}cornerRadius">
        <FrameLayout android:id="@+id/frameShape"
            android:layout_height="@dimen/_140pxv" android:layout_width="@dimen/_168pxh">
            <ImageView android:id="@+id/imageImage1"
                android:layout_gravity="center"
                android:layout_height="@dimen/_140pxh"
                android:layout_width="@dimen/_168pxh"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_18pxh}"
                imageRes="@{@drawable/img_shape_140x126}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_shape_140x126"/>
            <LinearLayout android:id="@+id/linearVilla"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <ImageButton
                    android:background="@drawable/rectangle_bg_name_red_a200_radius_12"
                    android:id="@+id/btnFavorite"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_width="@dimen/_25pxh"
                    android:paddingHorizontal="@dimen/_6pxh"
                    android:paddingVertical="@dimen/_6pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_favorite" tools:ignore="ContentDescription"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:background="@drawable/rectangle_bg_name_blue_gray_700_af_radius_8"
                    android:drawablePadding="@dimen/_6pxh"
                    android:drawableStart="@drawable/drawable_img_close"
                    android:ellipsize="end"
                    android:fontFamily="@font/raleway"
                    android:gravity="center" android:id="@+id/btnVilla"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_7pxh"
                    android:paddingStart="@dimen/_7pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_villa"
                    android:textAllCaps="false"
                    android:textColor="@color/{&quot;name&quot;:&quot;gray_100&quot;}"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_8pxh"
                    android:textStyle="bold" tools:text="@string/lbl_villa"/>
            </LinearLayout>
        </FrameLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearBaliBeachviewVilla"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:id="@+id/txtBaliBeachviewVilla"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="@string/msg_bali_beachview_villa"
                style="@style/txtRalewaysemibold14_1" tools:text="@string/msg_bali_beachview_villa"/>
            <LinearLayout android:id="@+id/linearLayout2"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageImage2"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_9pxh"
                    android:layout_width="@dimen/_9pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_signal_yellow_700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_signal_yellow_700"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtText"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_4_2"
                    style="@style/txtRalewaybold8" tools:text="@string/lbl_4_2"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearLayout3"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageBaliIndonesia"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_9pxh"
                    android:layout_width="@dimen/_9pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_linkedin"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_linkedin"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:id="@+id/txtBaliIndonesia"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/lbl_bali_indonesia"
                    style="@style/txtRalewaymedium8" tools:text="@string/lbl_bali_indonesia"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearLayout"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPrice"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/lbl_420"
                    style="@style/txtRalewaysemibold16" tools:text="@string/lbl_420"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMonth"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_month"
                    style="@style/txtRalewaymedium8_2" tools:text="@string/lbl_month"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
