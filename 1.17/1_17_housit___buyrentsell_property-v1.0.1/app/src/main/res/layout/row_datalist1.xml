<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearEstatesCardUser"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingBottom="@dimen/_14pxv"
        android:paddingEnd="@dimen/_16pxh"
        android:paddingStart="@dimen/_16pxh"
        android:paddingTop="@dimen/_14pxv" style="@style/groupStyle{&quot;name&quot;:&quot;gray_100&quot;}cornerRadius">
        <FrameLayout android:id="@+id/frameOne"
            android:layout_gravity="start"
            android:layout_height="@dimen/_124pxv" android:layout_width="@dimen/_114pxh">
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_orange_300_radius_8"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:gravity="center" android:id="@+id/btnOne"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_25pxh"
                android:layout_width="@dimen/_22pxh"
                android:paddingBottom="@dimen/_5pxv"
                android:paddingEnd="@dimen/_8pxh"
                android:paddingStart="@dimen/_8pxh"
                android:paddingTop="@dimen/_5pxv"
                android:singleLine="true" android:text="@string/lbl_1"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_12pxh"
                android:textStyle="bold" tools:text="@string/lbl_1"/>
            <ImageView android:id="@+id/imageAmanda"
                android:layout_gravity="bottom|end"
                android:layout_height="@dimen/_100pxh"
                android:layout_width="@dimen/_100pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_shape_100x100}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_shape_100x100"/>
        </FrameLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtAmanda"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:singleLine="true" android:text="@string/lbl_amanda"
            style="@style/txtRalewaysemibold14_1" tools:text="@string/lbl_amanda"/>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearLayout2"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:id="@+id/linearLayout3"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageImage1"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_signal_orange_300"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_signal_orange_300"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtText1"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_5_0"
                    style="@style/txtRalewaybold12_1" tools:text="@string/lbl_5_0"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearLayout"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageImage2"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtText2"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_112"
                    style="@style/txtRalewaybold12_1" tools:text="@string/lbl_112"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSold"
                    android:layout_gravity="top"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_sold"
                    style="@style/txtRalewayregular12" tools:text="@string/lbl_sold"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
