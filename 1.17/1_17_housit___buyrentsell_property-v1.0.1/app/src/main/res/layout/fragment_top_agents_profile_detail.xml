<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <androidx.core.widget.NestedScrollView android:clipToPadding="false"
        android:fillViewport="true"
        android:id="@+id/scrollViewScrollView"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <LinearLayout android:id="@+id/linearListing"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearListing1"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearButton"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_8pxh"
                    android:paddingStart="@dimen/_8pxh"
                    android:paddingTop="@dimen/_9pxv" style="@style/groupStyle{&quot;name&quot;:&quot;gray_100&quot;}cornerRadius">
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_white_a700_radius_tl_16_bl_16_br_16"
                        android:ellipsize="end"
                        android:fontFamily="@font/raleway"
                        android:gravity="center"
                        android:id="@+id/btnListing"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_32pxv"
                        android:layout_width="@dimen/_152pxh"
                        android:paddingBottom="@dimen/_7pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_7pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_listing"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_800_01&quot;}"
                        android:textFontWeight="600"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:text="@string/lbl_listing"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHistory"
                        android:layout_gravity="bottom"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_history"
                        style="@style/txtRalewaysemibold14_2" tools:text="@string/lbl_history"/>
                </LinearLayout>
                <FrameLayout android:gravity="center_horizontal"
                    android:id="@+id/frameDataList"
                    android:layout_height="@dimen/_567pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_14pxv"
                    android:paddingEnd="@dimen/_24pxh"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_14pxv" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
                    <LinearLayout android:id="@+id/linearFrameEightyTwo"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <LinearLayout android:gravity="center_vertical"
                            android:id="@+id/linearItemHeader"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtText"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@string/lbl_140"
                                style="@style/txtRalewaybold18" tools:text="@string/lbl_140"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtListings"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@string/lbl_listings"
                                style="@style/txtRalewaymedium18" tools:text="@string/lbl_listings"/>
                            <LinearLayout android:id="@+id/linearLayout"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:orientation="horizontal"
                                android:paddingHorizontal="@dimen/_8pxh"
                                android:paddingVertical="@dimen/_8pxv" style="@style/groupStylenamegray_1">
                                <FrameLayout
                                    android:id="@+id/frameButtonShow"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="@dimen/_24pxv"
                                    android:layout_width="@dimen/_36pxh"
                                    android:paddingBottom="@dimen/_6pxv"
                                    android:paddingEnd="@dimen/_12pxh"
                                    android:paddingStart="@dimen/_12pxh"
                                    android:paddingTop="@dimen/_6pxv" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}cornerRadius">
                                    <ImageView
                                    android:id="@+id/imageGrid"
                                    android:layout_gravity="center"
                                    android:layout_height="@dimen/_12pxh"
                                    android:layout_width="@dimen/_12pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_grid"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_grid"/>
                                </FrameLayout>
                                <ImageView
                                    android:id="@+id/imageIconHorizontal"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="@dimen/_12pxh"
                                    android:layout_width="@dimen/_12pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_icon_horizontal"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_icon_horizontal"/>
                            </LinearLayout>
                        </LinearLayout>
                        <androidx.recyclerview.widget.RecyclerView
                            android:gravity="center_horizontal"
                            android:id="@+id/recyclerTopAgentsProfileDetail"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:nestedScrollingEnabled="false"
                            app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
                            app:spanCount="2"
                            itemSpace="@{@dimen/_7pxh}"
                            tools:itemCount="4" tools:listitem="@layout/row_topagentsprofiledetail"/>
                    </LinearLayout>
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_primary_radius_35"
                        android:ellipsize="end"
                        android:fontFamily="@font/raleway"
                        android:gravity="center"
                        android:id="@+id/btnStartChat"
                        android:layout_gravity="bottom|center"
                        android:layout_height="@dimen/_70pxv"
                        android:layout_width="@dimen/_327pxh"
                        android:paddingBottom="@dimen/_25pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_25pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_start_chat"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                        android:textFontWeight="600"
                        android:textSize="@dimen/_16pxh"
                        android:textStyle="bold" tools:text="@string/lbl_start_chat"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</layout>
