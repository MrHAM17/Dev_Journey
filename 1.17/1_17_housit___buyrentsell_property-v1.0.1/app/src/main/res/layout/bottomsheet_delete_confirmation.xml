<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearDeleteConfirmation"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:id="@+id/linearModal"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingHorizontal="@dimen/_24pxh"
            android:paddingVertical="@dimen/_24pxv" style="@style/groupStylenamewhite_2">
            <View
                android:background="@color/{&quot;name&quot;:&quot;blue_gray_600&quot;}"
                android:id="@+id/lineSlide"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_3pxv" android:layout_width="@dimen/_60pxh"/>
            <LinearLayout android:id="@+id/linearItemAlertWarning"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingHorizontal="@dimen/_16pxh"
                android:paddingVertical="@dimen/_16pxv" style="@style/groupStylenameorange_1">
                <LinearLayout android:id="@+id/linearTwentyTwo"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingHorizontal="@dimen/_20pxh"
                    android:paddingVertical="@dimen/_20pxv" style="@style/groupStylecornerRadius_3">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearTwentyOne"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_27pxh"
                        android:paddingStart="@dimen/_27pxh"
                        android:paddingTop="@dimen/_19pxv" style="@style/groupStylecornerRadius_5">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtText"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.04"
                            android:singleLine="true"
                            android:text="@string/lbl3"
                            style="@style/txtMontserratsemibold25" tools:text="@string/lbl3"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtConfirmation"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="@string/msg_are_you_sure_want"
                style="@style/txtRalewaysemibold24" tools:text="@string/msg_are_you_sure_want"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDescription"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/msg_this_action_can_t"
                style="@style/txtRalewayregular14" tools:text="@string/msg_this_action_can_t"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearCancel"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <androidx.appcompat.widget.AppCompatButton
                    android:background="@drawable/rectangle_bg_name_red_a200_radius_35"
                    android:ellipsize="end"
                    android:fontFamily="@font/raleway"
                    android:gravity="center" android:id="@+id/btnCancel"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_70pxv"
                    android:layout_width="@dimen/_158pxh"
                    android:paddingBottom="@dimen/_25pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_25pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_cancel"
                    android:textAllCaps="false"
                    android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/lbl_cancel"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:background="@drawable/rectangle_bg_name_gray_100_radius_35"
                    android:ellipsize="end"
                    android:fontFamily="@font/raleway"
                    android:gravity="center" android:id="@+id/btnDelete"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_70pxv"
                    android:layout_width="@dimen/_158pxh"
                    android:paddingBottom="@dimen/_25pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_25pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_delete"
                    android:textAllCaps="false"
                    android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_800_01&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/lbl_delete"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
