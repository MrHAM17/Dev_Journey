<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearFormPassword"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingBottom="@dimen/_21pxv"
        android:paddingTop="@dimen/_21pxv" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageButton
                android:background="@drawable/rectangle_bg_name_gray_100_radius_bl_25_br_25"
                android:id="@+id/btnArrowLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrow_left" tools:ignore="ContentDescription"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPageTitle"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_add_listing"
                style="@style/txtRalewaybold14" tools:text="@string/lbl_add_listing"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearFormDetail"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_11pxv"
            android:paddingEnd="@dimen/_7pxh"
            android:paddingStart="@dimen/_7pxh" android:paddingTop="@dimen/_11pxv">
            <TextView android:gravity="start" android:id="@+id/txtTitle"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="@string/msg_hi_josh_fill_detail"
                style="@style/txtRalewaysemibold24" tools:text="@string/msg_hi_josh_fill_detail"/>
            <EditText
                android:background="@drawable/rectangle_bg_name_gray_100_radius_12"
                android:drawableEnd="@drawable/drawable_img_home_blue_gray_800_01"
                android:drawablePadding="@dimen/_30pxh"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:hint="@string/lbl_the_lodge_house"
                android:id="@+id/etFormPassword"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_27pxv"
                android:paddingEnd="@dimen/_16pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_27pxv"
                android:singleLine="true"
                android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_800_01&quot;}"
                android:textColorHint="@color/{&quot;name&quot;:&quot;blue_gray_800_01&quot;}"
                android:textFontWeight="700"
                android:textSize="@dimen/_12pxh"
                android:textStyle="bold" tools:ignore="Autofill"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearListingType"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtListingtype"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_listing_type"
                    style="@style/txtRalewaysemibold18" tools:text="@string/lbl_listing_type"/>
                <FrameLayout android:id="@+id/frameLayout"
                    android:layout_height="wrap_content" android:layout_width="match_parent">
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_primary_radius_25"
                        android:id="@+id/frameButtonCategory"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_24pxh" android:paddingTop="@dimen/_19pxv"/>
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_primary_radius_25"
                        android:id="@+id/frameButtonCategory1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_24pxh" android:paddingTop="@dimen/_19pxv"/>
                </FrameLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearPropertyCategory"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPropertycategory"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/msg_property_category"
                    style="@style/txtRalewaysemibold18" tools:text="@string/msg_property_category"/>
                <FrameLayout android:id="@+id/frameLayout1"
                    android:layout_height="wrap_content" android:layout_width="match_parent">
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_primary_radius_25"
                        android:id="@+id/frameButtonCategory2"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_24pxh" android:paddingTop="@dimen/_19pxv"/>
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_gray_100_radius_bl_25_br_25"
                        android:id="@+id/frameButtonCategory3"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_24pxh" android:paddingTop="@dimen/_19pxv"/>
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_gray_100_radius_bl_25_br_25"
                        android:id="@+id/frameButtonCategory4"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_24pxh" android:paddingTop="@dimen/_19pxv"/>
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_gray_100_radius_bl_25_br_25"
                        android:id="@+id/frameButtonCategory5"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_24pxh" android:paddingTop="@dimen/_19pxv"/>
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_gray_100_radius_bl_25_br_25"
                        android:id="@+id/frameButtonCategory6"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_24pxh" android:paddingTop="@dimen/_19pxv"/>
                </FrameLayout>
            </LinearLayout>
            <ImageView android:id="@+id/imageProgressBarGradient"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxh"
                android:layout_width="@dimen/_100pxh"
                android:scaleType="fitXY" tools:ignore="ContentDescription"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:background="@drawable/rectangle_bg_name_primary_radius_35"
            android:ellipsize="end" android:fontFamily="@font/raleway"
            android:gravity="center" android:id="@+id/btnNext"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_70pxv"
            android:layout_width="@dimen/_327pxh"
            android:paddingBottom="@dimen/_25pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_25pxv" android:singleLine="true"
            android:text="@string/lbl_next" android:textAllCaps="false"
            android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
            android:textFontWeight="600"
            android:textSize="@dimen/_16pxh" android:textStyle="bold" tools:text="@string/lbl_next"/>
    </LinearLayout>
</layout>
