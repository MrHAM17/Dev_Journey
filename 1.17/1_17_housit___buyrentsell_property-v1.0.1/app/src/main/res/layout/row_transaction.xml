<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearEstatesCardVertical"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="vertical"
        android:paddingHorizontal="@dimen/_8pxh"
        android:paddingVertical="@dimen/_8pxv" style="@style/groupStyle{&quot;name&quot;:&quot;gray_100&quot;}cornerRadius">
        <FrameLayout android:id="@+id/frameShape"
            android:layout_height="@dimen/_160pxv" android:layout_width="@dimen/_144pxh">
            <ImageView android:id="@+id/imageWingsTower"
                android:layout_gravity="center"
                android:layout_height="@dimen/_160pxh"
                android:layout_width="@dimen/_144pxh"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_15pxh}"
                imageRes="@{@drawable/img_shape_17}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_shape_17"/>
            <LinearLayout android:id="@+id/linearRent1"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageButton
                    android:background="@drawable/rectangle_bg_name_white_a700_opacity_0_78_radius_12"
                    android:id="@+id/btnFavorite"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_width="@dimen/_25pxh"
                    android:paddingHorizontal="@dimen/_6pxh"
                    android:paddingVertical="@dimen/_6pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_favorite_red_a200" tools:ignore="ContentDescription"/>
                <LinearLayout android:id="@+id/linearRent2"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_name_blue_gray_700_af_radius_8"
                        android:id="@+id/btnClose"
                        android:layout_gravity="bottom"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_width="@dimen/_25pxh"
                        android:paddingHorizontal="@dimen/_5pxh"
                        android:paddingVertical="@dimen/_5pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_close" tools:ignore="ContentDescription"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_blue_gray_700_af_radius_8"
                        android:ellipsize="end"
                        android:fontFamily="@font/raleway"
                        android:gravity="center"
                        android:id="@+id/btnRent"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_28pxv"
                        android:layout_width="@dimen/_43pxh"
                        android:paddingBottom="@dimen/_6pxv"
                        android:paddingEnd="@dimen/_8pxh"
                        android:paddingStart="@dimen/_8pxh"
                        android:paddingTop="@dimen/_6pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_rent"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;gray_100&quot;}"
                        android:textFontWeight="600"
                        android:textSize="@dimen/_12pxh"
                        android:textStyle="bold" tools:text="@string/lbl_rent"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtWingsTower"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@string/lbl_wings_tower"
            style="@style/txtRalewaybold12" tools:text="@string/lbl_wings_tower"/>
        <LinearLayout android:id="@+id/linearLayout"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageNovember"
                android:layout_gravity="top"
                android:layout_height="@dimen/_9pxh"
                android:layout_width="@dimen/_9pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_clock_primary"
                tools:ignore="ContentDescription" tools:src="@drawable/img_clock_primary"/>
            <TextView android:ellipsize="end"
                android:gravity="center_vertical"
                android:id="@+id/txtDate"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/msg_november_21_2021"
                style="@style/txtRalewaymedium8" tools:text="@string/msg_november_21_2021"/>
        </LinearLayout>
    </LinearLayout>
</layout>
