<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="imageSliderTwelveModel" type="com.streetstyleecommerceapp.app.modules.product.data.model.ImageSliderTwelveModel"/>
    </data>
    <ImageView android:id="@+id/imageImage"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:scaleType="fitXY"
        cornerRadius="@{@dimen/_30pxh}"
        imageUrl="@{imageSliderTwelveModel.imageImage}"
        placeHolder="@{@drawable/image_not_found}"
        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_14"/>
</layout>
