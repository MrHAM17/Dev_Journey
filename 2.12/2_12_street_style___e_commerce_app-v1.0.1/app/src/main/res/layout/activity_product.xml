<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="productVM" type="com.streetstyleecommerceapp.app.modules.product.data.viewmodel.ProductVM"/>
    </data>
    <LinearLayout android:id="@+id/linearFortyThree"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;gray_300_01&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_down"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
            <LinearLayout android:id="@+id/linearGroup16"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageSearch"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_search"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                <ImageView android:id="@+id/imageImage"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group_86"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group_86"/>
                <ImageView android:id="@+id/imageImage1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group_84"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group_84"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearProduct"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_27pxv" android:paddingTop="@dimen/_27pxv">
            <TextView android:ellipsize="end"
                android:id="@+id/txtBoyfriendTee"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{productVM.productModel.txtBoyfriendTee}"
                style="@style/txtPlayfairDisplayitalicregular40" tools:text="@string/lbl_boyfriend_tee"/>
            <com.asksira.loopingviewpager.LoopingViewPager
                android:id="@+id/imageSliderTwelve"
                android:layout_height="@dimen/_400pxv"
                android:layout_width="match_parent"
                app:autoScroll="true" app:isInfinite="true" app:scrollInterval="5000"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearFiftyTwo"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end" android:id="@+id/txtS"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_17pxh"
                    android:paddingStart="@dimen/_17pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@{productVM.productModel.txtS}"
                    style="@style/txtSolidRoundedOutline" tools:text="@string/lbl_s"/>
                <TextView android:ellipsize="end" android:id="@+id/txtM"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_13pxh"
                    android:paddingStart="@dimen/_13pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@{productVM.productModel.txtM}"
                    style="@style/txtSolidRounded" tools:text="@string/lbl_m"/>
                <TextView android:ellipsize="end" android:id="@+id/txtL"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@{productVM.productModel.txtL}"
                    style="@style/txtSolidRoundedOutline" tools:text="@string/lbl_l"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtXL"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_8pxh"
                    android:paddingStart="@dimen/_8pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:singleLine="true"
                    android:text="@{productVM.productModel.txtXL}"
                    style="@style/txtSolidRoundedOutline" tools:text="@string/lbl_xl"/>
                <FrameLayout android:id="@+id/frameSpacer"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <ImageView android:id="@+id/imageSettings"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_46pxh"
                    android:layout_width="@dimen/_33pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_settings"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEightySeven"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_18pxh"
                    android:paddingStart="@dimen/_18pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:singleLine="true"
                    android:text="@{productVM.productModel.txtEightySeven}"
                    style="@style/txtOutline" tools:text="@string/lbl_1"/>
                <ImageView android:id="@+id/imagePlus"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_46pxh"
                    android:layout_width="@dimen/_33pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_plus_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_plus_black_900"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearButton"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;black_900&quot;}">
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtAddToCart"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{productVM.productModel.txtAddToCart}"
                style="@style/txtPlayfairDisplayplayfairdisplaybold22" tools:text="@string/lbl_add_to_cart"/>
            <ImageView android:id="@+id/imageArrowLeft"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_13pxh"
                android:layout_width="@dimen/_91pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_left"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_left"/>
        </LinearLayout>
    </LinearLayout>
</layout>
