<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="center_vertical"
        android:id="@+id/linearUser"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/_19pxv"
        android:paddingEnd="@dimen/_18pxh"
        android:paddingStart="@dimen/_18pxh"
        android:paddingTop="@dimen/_19pxv" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}cornerRadius">
        <ImageView android:id="@+id/imageLoganNasser1"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_38pxh"
            android:layout_width="@dimen/_38pxh"
            android:scaleType="fitXY" android:src="@drawable/img_play"
            tools:ignore="ContentDescription" tools:src="@drawable/img_play"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearUsername"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtUsername"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:singleLine="true"
                android:text="@string/lbl_logan_nasser"
                style="@style/txtIntermedium14" tools:text="@string/lbl_logan_nasser"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFullname"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:singleLine="true"
                android:text="@string/lbl_louisaingram"
                style="@style/txtInterregular12" tools:text="@string/lbl_louisaingram"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameSpacer"
            android:layout_height="wrap_content" android:layout_width="wrap_content"/>
        <ImageView android:id="@+id/imageLoganNasser2"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_28pxh"
            android:layout_width="@dimen/_28pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_chip_chip_icon_left"
            tools:ignore="ContentDescription" tools:src="@drawable/img_chip_chip_icon_left"/>
    </LinearLayout>
</layout>
