<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearSearchResults"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;gray_50&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
            <ImageButton
                android:background="@drawable/rectangle_bg_name_white_a700_border_name_blue_gray_100_radius_12"
                android:id="@+id/btnButtonIcon"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_button_icon" tools:ignore="ContentDescription"/>
            <androidx.appcompat.widget.SearchView
                android:background="@drawable/rectangle_bg_name_gray_400_radius_8"
                android:id="@+id/searchViewSearch"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:theme="@style/searchViewsearch"
                app:iconifiedByDefault="false"
                app:queryBackground="@android:color/transparent"
                app:queryHint="@string/lbl_nass" app:searchIcon="@drawable/img_close_primary"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearSeeMore"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_9pxv"
            android:paddingEnd="@dimen/_28pxh"
            android:paddingStart="@dimen/_28pxh" android:paddingTop="@dimen/_9pxv">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearPeople"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearFortyEight"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPeople"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_5pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_people"
                        style="@style/txtInterbold22" tools:text="@string/lbl_people"/>
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearUser04"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_18pxh"
                        android:paddingStart="@dimen/_18pxh"
                        android:paddingTop="@dimen/_19pxv" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}cornerRadius">
                        <ImageView android:id="@+id/imagePlay"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_38pxh"
                            android:layout_width="@dimen/_38pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_play"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_play"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearUsername"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtUsername"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:text="@string/lbl_logan_nasser"
                                style="@style/txtIntermedium14" tools:text="@string/lbl_logan_nasser"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtFullname"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:text="@string/lbl_louisaingram"
                                style="@style/txtInterregular12" tools:text="@string/lbl_louisaingram"/>
                        </LinearLayout>
                        <FrameLayout android:id="@+id/frameSpacer"
                            android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                        <ImageView
                            android:id="@+id/imageChipChipIconLeft"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_width="@dimen/_28pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_chip_chip_icon_left"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_chip_chip_icon_left"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearUser5"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_18pxh"
                        android:paddingStart="@dimen/_18pxh"
                        android:paddingTop="@dimen/_19pxv" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}cornerRadius">
                        <ImageView android:id="@+id/imagePlay1"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_38pxh"
                            android:layout_width="@dimen/_38pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_play"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_play"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearUsername1"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtUsername1"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:text="@string/lbl_logan_nasser"
                                style="@style/txtIntermedium14" tools:text="@string/lbl_logan_nasser"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtFullname1"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:text="@string/lbl_louisaingram"
                                style="@style/txtInterregular12" tools:text="@string/lbl_louisaingram"/>
                        </LinearLayout>
                        <FrameLayout android:id="@+id/frameSpacer1"
                            android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                        <ImageView
                            android:id="@+id/imageChipChipIconLeft1"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_width="@dimen/_28pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_chip_chip_icon_left"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_chip_chip_icon_left"/>
                    </LinearLayout>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:background="@drawable/rectangle_border_name_blue_gray_400_radius_4"
                    android:drawableEnd="@drawable/img_arrowright"
                    android:ellipsize="end"
                    android:fontFamily="@font/inter"
                    android:gravity="center"
                    android:id="@+id/btnSeeMore"
                    android:layout_height="@dimen/_30pxv"
                    android:layout_width="@dimen/_97pxh"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingBottom="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_11pxh"
                    android:paddingStart="@dimen/_11pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_see_more"
                    android:textAllCaps="false"
                    android:textColor="@color/{&quot;name&quot;:&quot;onPrimary&quot;}"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="bold" tools:text="@string/lbl_see_more"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearPosts"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPosts"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_posts"
                    style="@style/txtInterbold22" tools:text="@string/lbl_posts"/>
                <LinearLayout android:id="@+id/linearCard03"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/_16pxv"
                    android:paddingEnd="@dimen/_18pxh"
                    android:paddingStart="@dimen/_18pxh"
                    android:paddingTop="@dimen/_16pxv" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}cornerRadius">
                    <LinearLayout android:id="@+id/linearTop"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:id="@+id/linearUser"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView android:id="@+id/imagePlay2"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_33pxh"
                                android:layout_width="@dimen/_38pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_play"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_play"/>
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearName"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtName"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_3pxh"
                                    android:singleLine="true"
                                    android:text="@string/lbl_katherine_cole"
                                    style="@style/txtIntermedium14" tools:text="@string/lbl_katherine_cole"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtTime"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_3pxh"
                                    android:singleLine="true"
                                    android:text="@string/lbl_5_min_ago"
                                    style="@style/txtInterregular12" tools:text="@string/lbl_5_min_ago"/>
                            </LinearLayout>
                        </LinearLayout>
                        <View
                            android:background="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                            android:id="@+id/lineVector"
                            android:layout_gravity="bottom"
                            android:layout_height="@dimen/_16pxv" android:layout_width="@dimen/_4pxh"/>
                    </LinearLayout>
                    <ImageView android:gravity="center_horizontal"
                        android:id="@+id/imageImage"
                        android:layout_height="@dimen/_119pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_8pxh}"
                        imageRes="@{@drawable/img_image_119x283}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image_119x283"/>
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtTitle"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:text="@string/msg_the_best_fashion"
                        style="@style/txtInterbold16" tools:text="@string/msg_the_best_fashion"/>
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearBottomInteractions"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:id="@+id/linearLikes"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageFavorite"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_14pxh"
                                android:layout_width="@dimen/_14pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_favorite_gray_500"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_favorite_gray_500"/>
                            <TextView android:ellipsize="end"
                                android:gravity="center_vertical"
                                android:id="@+id/txtLabel"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:text="@string/lbl_326"
                                style="@style/txtIntermedium14" tools:text="@string/lbl_326"/>
                        </LinearLayout>
                        <LinearLayout android:gravity="center_vertical"
                            android:id="@+id/linearComments"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageInstagram"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_14pxh"
                                android:layout_width="@dimen/_14pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_instagram"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_instagram"/>
                            <TextView android:ellipsize="end"
                                android:gravity="center_vertical"
                                android:id="@+id/txtLabel1"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:text="@string/lbl_148"
                                style="@style/txtIntermedium14" tools:text="@string/lbl_148"/>
                        </LinearLayout>
                        <FrameLayout android:id="@+id/frameSpacer2"
                            android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLabel2"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_share"
                            style="@style/txtIntermedium14" tools:text="@string/lbl_share"/>
                        <ImageView android:id="@+id/imageQuestion"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_question"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_question"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
