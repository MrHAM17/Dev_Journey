<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="framethirtysixRowModel" type="com.blogapp.app.modules.home.data.model.FramethirtysixRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearPeopleList"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" android:orientation="vertical">
        <ImageView android:id="@+id/imageLoremIpsumDolor"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_80pxh"
            android:layout_width="@dimen/_80pxh"
            android:scaleType="centerCrop" circular="@{true}"
            imageRes="@{@drawable/img_thumbnail}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_thumbnail"/>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtLoremIpsumIsA"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:text="@{framethirtysixRowModel.txtLoremIpsumIsA}"
            style="@style/txtPoppinsregular12" tools:text="@string/msg_lorem_ipsum_dolor"/>
    </LinearLayout>
</layout>
