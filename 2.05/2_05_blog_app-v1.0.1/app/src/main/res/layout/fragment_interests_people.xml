<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="interestsPeopleVM" type="com.blogapp.app.modules.interestspeople.data.viewmodel.InterestsPeopleVM"/>
    </data>
    <LinearLayout android:id="@+id/linearFiftyTwo"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearFiftyOne"
            android:layout_height="match_parent"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end" android:id="@+id/txtPrice"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{interestsPeopleVM.interestsPeopleModel.txtPrice}"
                style="@style/txtPoppinssemibold14" tools:text="@string/msg_see_all_people_you"/>
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerInterestsPeople"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_20pxh}" tools:itemCount="2" tools:listitem="@layout/row_interestspeople"/>
        </LinearLayout>
    </LinearLayout>
</layout>
