<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="postsUploadMediaTabContainerVM" type="com.tikpikshortvideosocialnetworking.app.modules.postsuploadmediatabcontainer.data.viewmodel.PostsUploadMediaTabContainerVM"/>
    </data>
    <LinearLayout android:id="@+id/linearNext"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;onErrorContainer&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageCategoriesXClose"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_categories_x_close_gray_900"
                tools:ignore="ContentDescription" tools:src="@drawable/img_categories_x_close_gray_900"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtAllSounds"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{postsUploadMediaTabContainerVM.postsUploadMediaTabContainerModel.txtAllSounds}"
                style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_all_media"/>
            <ImageButton android:id="@+id/btnSettings"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_settings_gray_900" tools:ignore="ContentDescription"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearNext1"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <com.google.android.material.tabs.TabLayout
                android:id="@+id/tabLayoutTabview"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:tabMode="fixed"
                app:tabSelectedTextColor="@color/{&quot;name&quot;:&quot;primary&quot;}" app:tabTextColor="@color/{&quot;name&quot;:&quot;gray_500&quot;}"/>
            <FrameLayout android:id="@+id/frameNext"
                android:layout_height="@dimen/_772pxv" android:layout_width="match_parent">
                <androidx.viewpager2.widget.ViewPager2
                    android:id="@+id/viewPagerTabBarView"
                    android:layout_gravity="center"
                    android:layout_height="match_parent"
                    android:layout_weight="1" android:layout_width="match_parent"/>
                <LinearLayout android:id="@+id/linearAutoLayoutVertical"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/_19pxv"
                    android:paddingEnd="@dimen/_24pxh"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_19pxv" style="@style/groupStylecornerRadius_5">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtYouCanSelectBoth"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{postsUploadMediaTabContainerVM.postsUploadMediaTabContainerModel.txtYouCanSelectBoth}"
                        style="@style/txtUrbanistromanmedium14_4" tools:text="@string/msg_you_can_select_both"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_primary_radius_16"
                        android:ellipsize="end"
                        android:fontFamily="@font/urbanist"
                        android:gravity="center"
                        android:id="@+id/btnNext"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_58pxv"
                        android:layout_width="@dimen/_380pxh"
                        android:letterSpacing="0.01"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_19pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_next"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;onErrorContainer&quot;}"
                        android:textFontWeight="700"
                        android:textSize="@dimen/_16pxh"
                        android:textStyle="bold" tools:text="@string/lbl_next"/>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
