<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="messagesBoxVM" type="com.tikpikshortvideosocialnetworking.app.modules.messagesbox.data.viewmodel.MessagesBoxVM"/>
    </data>
    <LinearLayout android:id="@+id/linearMessagesBox"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;onErrorContainer&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_down"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTitleSection"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{messagesBoxVM.messagesBoxModel.txtTitleSection}"
                style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_theresa_varnes"/>
            <LinearLayout android:id="@+id/linearGroup76"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCall"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_call"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_call"/>
                <ImageButton android:id="@+id/btnSettings"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_settings_gray_900" tools:ignore="ContentDescription"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearToday"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_23pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_23pxv">
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_gray_600_opacity_0_12_radius_6"
                android:ellipsize="end"
                android:fontFamily="@font/urbanist"
                android:gravity="center" android:id="@+id/btnToday"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_24pxv"
                android:layout_width="@dimen/_75pxh"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_6pxv"
                android:paddingEnd="@dimen/_22pxh"
                android:paddingStart="@dimen/_22pxh"
                android:paddingTop="@dimen/_6pxv"
                android:singleLine="true"
                android:text="@string/lbl_today"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;gray_600&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_10pxh"
                android:textStyle="bold" tools:text="@string/lbl_today"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearAutoLayoutHorizontal"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_20pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_11pxv" style="@style/groupStylecornerRadius_3">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLoremIpsumDolor"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{messagesBoxVM.messagesBoxModel.txtLoremIpsumDolor}"
                    style="@style/txtUrbanistregular16_1" tools:text="@string/msg_hi_theresa_good"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTime"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{messagesBoxVM.messagesBoxModel.txtTime}"
                    style="@style/txtUrbanistregular12" tools:text="@string/lbl_10_002"/>
            </LinearLayout>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearAutoLayoutHorizontal1"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/_9pxv"
                android:paddingEnd="@dimen/_20pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_9pxv" style="@style/groupStylecornerRadius_3">
                <TextView android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:text="@{messagesBoxVM.messagesBoxModel.txtDescription}"
                    style="@style/txtUrbanistregular16_1" tools:text="@string/msg_how_are_you_today_we"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTime1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{messagesBoxVM.messagesBoxModel.txtTime1}"
                    style="@style/txtUrbanistregular12" tools:text="@string/lbl_10_002"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearAutoLayoutHorizontal2"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_20pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_11pxv" style="@style/groupStyle{&quot;name&quot;:&quot;gray_100&quot;}cornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLoremIpsumDolor1"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{messagesBoxVM.messagesBoxModel.txtLoremIpsumDolor1}"
                    style="@style/txtUrbanistregular16" tools:text="@string/msg_hi_morning_too"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTime2"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{messagesBoxVM.messagesBoxModel.txtTime2}"
                    style="@style/txtUrbanistregular12_1" tools:text="@string/lbl_10_002"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal3"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/_9pxv"
                android:paddingEnd="@dimen/_20pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_9pxv" style="@style/groupStyle{&quot;name&quot;:&quot;gray_100&quot;}cornerRadius">
                <TextView android:id="@+id/txtDescription1"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:text="@{messagesBoxVM.messagesBoxModel.txtDescription1}"
                    style="@style/txtUrbanistregular16" tools:text="@string/msg_yes_you_re_right_i"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTime3"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{messagesBoxVM.messagesBoxModel.txtTime3}"
                    style="@style/txtUrbanistregular12_1" tools:text="@string/lbl_10_002"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameSpacer"
                android:layout_height="wrap_content" android:layout_width="match_parent"/>
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal4"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <EditText
                    android:background="@drawable/rectangle_bg_name_onerrorcontainer_border_name_gray_400_radius_12"
                    android:drawableEnd="@drawable/img_camera"
                    android:drawablePadding="@dimen/_30pxh"
                    android:drawableStart="@drawable/img_categoriessticker_componentadditional_icons"
                    android:ellipsize="end"
                    android:fontFamily="@font/urbanist"
                    android:hint="@string/lbl_message2"
                    android:id="@+id/etMessage" android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:paddingBottom="@dimen/_19pxv"
                    android:paddingEnd="@dimen/_17pxh"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_19pxv"
                    android:singleLine="true"
                    android:text="@={messagesBoxVM.messagesBoxModel.etMessageValue}"
                    android:textColor="@color/{&quot;name&quot;:&quot;gray_500&quot;}"
                    android:textColorHint="@color/{&quot;name&quot;:&quot;gray_500&quot;}"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
                <ImageButton
                    android:background="@drawable/rectangle_gradient_s_name_deep_orange_a400_e_name_orange_600_radius_28"
                    android:id="@+id/btnIconButton"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_56pxh"
                    android:layout_width="@dimen/_56pxh"
                    android:paddingHorizontal="@dimen/_16pxh"
                    android:paddingVertical="@dimen/_16pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_auto_layout_horizontal_onerrorcontainer_56x56" tools:ignore="ContentDescription"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
