<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="theresavarnesRowModel" type="com.tikpikshortvideosocialnetworking.app.modules.messages.data.model.TheresavarnesRowModel"/>
    </data>
    <LinearLayout android:gravity="center_vertical"
        android:id="@+id/linearTypeMessengerList"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageTheresaVarnes"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_60pxh"
            android:layout_width="@dimen/_60pxh"
            android:scaleType="centerCrop" circular="@{true}"
            imageRes="@{@drawable/img_ellipse_4}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_4"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearAutoLayoutVertical1"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:ellipsize="end" android:id="@+id/txtName"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{theresavarnesRowModel.txtName}"
                style="@style/txtUrbanistromanbold18_1" tools:text="@string/lbl_theresa_varnes"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtMessage"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@{theresavarnesRowModel.txtMessage}"
                style="@style/txtUrbanistromanmedium14_2" tools:text="@string/msg_hi_morning_too"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameSpacer"
            android:layout_height="wrap_content" android:layout_width="wrap_content"/>
        <LinearLayout android:id="@+id/linearAutoLayoutVertical2"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:ellipsize="end" android:gravity="end"
                android:id="@+id/txtAutoLayoutHorizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_6pxv"
                android:paddingEnd="@dimen/_10pxh"
                android:paddingStart="@dimen/_10pxh"
                android:paddingTop="@dimen/_6pxv"
                android:singleLine="true"
                android:text="@{theresavarnesRowModel.txtAutoLayoutHorizontal}"
                style="@style/txtGradientRounded" tools:text="@string/lbl_1"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtTwoThousand"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@{theresavarnesRowModel.txtTwoThousand}"
                style="@style/txtUrbanistromanmedium14_2" tools:text="@string/lbl_10_00"/>
        </LinearLayout>
    </LinearLayout>
</layout>
