<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="tellUsAboutYourselfVM" type="com.tikpikshortvideosocialnetworking.app.modules.tellusaboutyourself.data.viewmodel.TellUsAboutYourselfVM"/>
    </data>
    <LinearLayout android:id="@+id/linearTellusaboutyourself"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;onErrorContainer&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_down"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTitleSection"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{tellUsAboutYourselfVM.tellUsAboutYourselfModel.txtTitleSection}"
                style="@style/txtUrbanistromanbold24" tools:text="@string/msg_tell_us_about_yourself"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearSkip"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingHorizontal="@dimen/_24pxh" android:paddingVertical="@dimen/_24pxv">
            <TextView android:gravity="start"
                android:id="@+id/txtChooseYourIdentity"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01"
                android:text="@{tellUsAboutYourselfVM.tellUsAboutYourselfModel.txtChooseYourIdentity}"
                style="@style/txtUrbanistromanmedium18_1" tools:text="@string/msg_choose_your_identity"/>
            <FrameLayout android:id="@+id/frameSpacer"
                android:layout_height="wrap_content" android:layout_width="match_parent"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearAutoLayoutVertical"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearVector"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/_29pxv"
                    android:paddingEnd="@dimen/_59pxh"
                    android:paddingStart="@dimen/_59pxh"
                    android:paddingTop="@dimen/_29pxv" style="@style/groupStylecornerRadius_2">
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_53pxh"
                        android:layout_width="@dimen/_53pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMale"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{tellUsAboutYourselfVM.tellUsAboutYourselfModel.txtMale}"
                        style="@style/txtUrbanistromanbold21" tools:text="@string/lbl_male"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearLaptop"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/_27pxv"
                    android:paddingEnd="@dimen/_48pxh"
                    android:paddingStart="@dimen/_48pxh"
                    android:paddingTop="@dimen/_27pxv" style="@style/groupStyle{&quot;name&quot;:&quot;deep_orange_50&quot;}cornerRadius">
                    <ImageView android:id="@+id/imageLaptop"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_59pxh"
                        android:layout_width="@dimen/_35pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_laptop"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_laptop"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFemale"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{tellUsAboutYourselfVM.tellUsAboutYourselfModel.txtFemale}"
                        style="@style/txtUrbanistromanbold24_1" tools:text="@string/lbl_female"/>
                </LinearLayout>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameSpacer1"
                android:layout_height="wrap_content" android:layout_width="match_parent"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearAutoLayoutHorizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <androidx.appcompat.widget.AppCompatButton
                    android:background="@drawable/rectangle_bg_name_deep_orange_50_radius_16"
                    android:ellipsize="end"
                    android:fontFamily="@font/urbanist"
                    android:gravity="center" android:id="@+id/btnSkip"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_58pxv"
                    android:layout_width="@dimen/_184pxh"
                    android:letterSpacing="0.01"
                    android:paddingBottom="@dimen/_19pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_19pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_skip"
                    android:textAllCaps="false"
                    android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                    android:textFontWeight="700"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/lbl_skip"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:background="@drawable/rectangle_bg_name_primary_radius_16"
                    android:ellipsize="end"
                    android:fontFamily="@font/urbanist"
                    android:gravity="center"
                    android:id="@+id/btnContinue"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_58pxv"
                    android:layout_width="@dimen/_184pxh"
                    android:letterSpacing="0.01"
                    android:paddingBottom="@dimen/_19pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_19pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_continue"
                    android:textAllCaps="false"
                    android:textColor="@color/{&quot;name&quot;:&quot;onErrorContainer&quot;}"
                    android:textFontWeight="700"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/lbl_continue"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
