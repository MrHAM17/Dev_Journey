<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="editProfileVM" type="com.tikpikshortvideosocialnetworking.app.modules.editprofile.data.viewmodel.EditProfileVM"/>
    </data>
    <LinearLayout android:id="@+id/linearEditProfile"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;onErrorContainer&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_down"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTitleSection"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{editProfileVM.editProfileModel.txtTitleSection}"
                style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_edit_profile"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearEdit"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_31pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_31pxv">
            <FrameLayout android:id="@+id/frameTypeEditAvatar"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_160pxv" android:layout_width="@dimen/_160pxh">
                <ImageView android:id="@+id/imageCircleImage"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_160pxh"
                    android:layout_width="@dimen/_160pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse_160x160}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_160x160"/>
                <ImageButton android:id="@+id/btnEdit"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:paddingHorizontal="@dimen/_3pxh"
                    android:paddingVertical="@dimen/_3pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_edit" tools:ignore="ContentDescription"/>
            </FrameLayout>
            <View
                android:background="@color/{&quot;name&quot;:&quot;gray_200&quot;}"
                android:id="@+id/lineLine"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_380pxh"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtAboutYou"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{editProfileVM.editProfileModel.txtAboutYou}"
                style="@style/txtUrbanistromanbold20" tools:text="@string/lbl_about_you"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearAutoLayoutHorizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageLock"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_lock_gray_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_lock_gray_900"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtName"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtName}"
                    style="@style/txtUrbanistsemibold18_3" tools:text="@string/lbl_name"/>
                <FrameLayout android:id="@+id/frameSpacer"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAndrewAinsley"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtAndrewAinsley}"
                    style="@style/txtUrbanistsemibold18_3" tools:text="@string/lbl_andrew_ainsley"/>
                <ImageView android:id="@+id/imageArrowRight"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right_gray_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right_gray_900"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearAutoLayoutHorizontal1"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageIconlyCurvedTick"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconly_curved_tick"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_curved_tick"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtUsername"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtUsername}"
                    style="@style/txtUrbanistsemibold18_3" tools:text="@string/lbl_username"/>
                <FrameLayout android:id="@+id/frameSpacer1"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAndrewAinsley1"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtAndrewAinsley1}"
                    style="@style/txtUrbanistsemibold18_3" tools:text="@string/lbl_andrew_ainsley2"/>
                <ImageView android:id="@+id/imageArrowRight1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right_gray_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right_gray_900"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearAutoLayoutHorizontal2"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageIconlyCurvedInfo"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconly_curved_info"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_curved_info"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtBio"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtBio}"
                    style="@style/txtUrbanistsemibold18_3" tools:text="@string/lbl_bio"/>
                <FrameLayout android:id="@+id/frameSpacer2"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDesignerVid"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtDesignerVid}"
                    style="@style/txtUrbanistsemibold18_3" tools:text="@string/msg_designer_vid"/>
                <ImageView android:id="@+id/imageArrowRight2"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right_gray_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right_gray_900"/>
            </LinearLayout>
            <View
                android:background="@color/{&quot;name&quot;:&quot;gray_200&quot;}"
                android:id="@+id/lineLine1"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_380pxh"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSocial"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{editProfileVM.editProfileModel.txtSocial}"
                style="@style/txtUrbanistromanbold20" tools:text="@string/lbl_social"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearAutoLayoutHorizontal3"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearCategoriesInstagram"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/imageCategoriesInstagram"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_categories_instagram_gray_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_categories_instagram_gray_900"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtInstagram"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{editProfileVM.editProfileModel.txtInstagram}"
                        style="@style/txtUrbanistsemibold18_3" tools:text="@string/lbl_instagram"/>
                    <FrameLayout android:id="@+id/frameSpacer3"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAndrewAins"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{editProfileVM.editProfileModel.txtAndrewAins}"
                        style="@style/txtUrbanistsemibold18_3" tools:text="@string/lbl_andrew_ains"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowRight3"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right_gray_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right_gray_900"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearAutoLayoutHorizontal4"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCategoriesFacebook"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_categories_facebook_gray_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_categories_facebook_gray_900"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFacebook"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtFacebook}"
                    style="@style/txtUrbanistsemibold18_3" tools:text="@string/lbl_facebook"/>
                <FrameLayout android:id="@+id/frameSpacer4"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAndrewAinsley2"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtAndrewAinsley2}"
                    style="@style/txtUrbanistsemibold18_3" tools:text="@string/lbl_andrewainsley2"/>
                <ImageView android:id="@+id/imageArrowRight4"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right_gray_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right_gray_900"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearCategoriesTwitter"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCategoriesTwitter"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_categories_twitter"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_categories_twitter"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTwitter"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtTwitter}"
                    style="@style/txtUrbanistsemibold18_3" tools:text="@string/lbl_twitter"/>
                <FrameLayout android:id="@+id/frameSpacer5"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAndrewAinsl"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtAndrewAinsl}"
                    style="@style/txtUrbanistsemibold18_3" tools:text="@string/lbl_andrew_ainsl"/>
                <ImageView android:id="@+id/imageImage"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_width="@dimen/_5pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group_gray_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group_gray_900"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
