<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="logoutModalVM" type="com.tikpikshortvideosocialnetworking.app.modules.logoutmodal.data.viewmodel.LogoutModalVM"/>
    </data>
    <LinearLayout android:id="@+id/linearLogoutModal"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameAutoLayoutVertical"
            android:layout_height="@dimen/_266pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_7pxv"
            android:paddingEnd="@dimen/_23pxh"
            android:paddingStart="@dimen/_23pxh"
            android:paddingTop="@dimen/_7pxv" style="@style/groupStylenameonErrorContainercornerRadius_4">
            <View
                android:background="@color/{&quot;name&quot;:&quot;gray_200&quot;}"
                android:id="@+id/lineLine"
                android:layout_gravity="top|center"
                android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_380pxh"/>
            <LinearLayout android:id="@+id/linearCancel"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <ImageView android:id="@+id/imageImage"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_3pxh"
                    android:layout_width="@dimen/_38pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_frame_gray_300"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_frame_gray_300"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLogout"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{logoutModalVM.logoutModalModel.txtLogout}"
                    style="@style/txtUrbanistromanbold24_4" tools:text="@string/lbl_logout"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtConfirmation"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{logoutModalVM.logoutModalModel.txtConfirmation}"
                    style="@style/txtUrbanistromanbold20_1" tools:text="@string/msg_are_you_sure_you"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearCancel1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_deep_orange_50_radius_16"
                        android:ellipsize="end"
                        android:fontFamily="@font/urbanist"
                        android:gravity="center"
                        android:id="@+id/btnCancel"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_58pxv"
                        android:layout_width="@dimen/_166pxh"
                        android:letterSpacing="0.01"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_19pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_cancel"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                        android:textFontWeight="700"
                        android:textSize="@dimen/_16pxh"
                        android:textStyle="bold" tools:text="@string/lbl_cancel"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_secondarycontainer_radius_16"
                        android:ellipsize="end"
                        android:fontFamily="@font/urbanist"
                        android:gravity="center"
                        android:id="@+id/btnYesLogOut"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_58pxv"
                        android:layout_width="@dimen/_166pxh"
                        android:letterSpacing="0.01"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_19pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_yes_log_out"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;onErrorContainer&quot;}"
                        android:textFontWeight="700"
                        android:textSize="@dimen/_16pxh"
                        android:textStyle="bold" tools:text="@string/lbl_yes_log_out"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
