<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="seeLiveVM" type="com.tikpikshortvideosocialnetworking.app.modules.seelive.data.viewmodel.SeeLiveVM"/>
    </data>
    <LinearLayout android:id="@+id/linearSeeLive"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylenameonErrorContainer_1">
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameFollow"
            android:layout_height="@dimen/_858pxv" android:layout_width="match_parent">
            <View
                android:background="@drawable/rectangle_gradient_s_name_primarycontainer_e_name_primarycontainer_opacity_0_6"
                android:id="@+id/viewView"
                android:layout_gravity="bottom|center"
                android:layout_height="@dimen/_200pxv" android:layout_width="@dimen/_428pxh"/>
            <LinearLayout android:id="@+id/linearAutoLayoutVertical"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearFollow"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearFollow1"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imagePlay"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_48pxh"
                            android:layout_width="@dimen/_48pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_play"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_play"/>
                        <LinearLayout
                            android:id="@+id/linearAutoLayoutVertical1"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtSarahWilona"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.01"
                                android:singleLine="true"
                                android:text="@{seeLiveVM.seeLiveModel.txtSarahWilona}"
                                style="@style/txtUrbanistromanbold14" tools:text="@string/lbl_sarah_wilona"/>
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtDancerSinger"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.01"
                                android:singleLine="true"
                                android:text="@{seeLiveVM.seeLiveModel.txtDancerSinger}"
                                style="@style/txtUrbanistromanmedium12_3" tools:text="@string/lbl_dancer_singer"/>
                        </LinearLayout>
                        <androidx.appcompat.widget.AppCompatButton
                            android:background="@drawable/rectangle_bg_name_primary_radius_8"
                            android:ellipsize="end"
                            android:fontFamily="@font/urbanist"
                            android:gravity="center"
                            android:id="@+id/btnFollow"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_32pxv"
                            android:layout_width="@dimen/_73pxh"
                            android:letterSpacing="0.01"
                            android:paddingBottom="@dimen/_7pxv"
                            android:paddingEnd="@dimen/_16pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_7pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_follow"
                            android:textAllCaps="false"
                            android:textColor="@color/{&quot;name&quot;:&quot;onErrorContainer&quot;}"
                            android:textFontWeight="600"
                            android:textSize="@dimen/_14pxh"
                            android:textStyle="bold" tools:text="@string/lbl_follow"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:background="@drawable/rectangle_bg_name_gray_800_99_radius_16"
                            android:drawablePadding="@dimen/_8pxh"
                            android:drawableStart="@drawable/img_lock"
                            android:ellipsize="end"
                            android:fontFamily="@font/urbanist"
                            android:gravity="center"
                            android:id="@+id/btn36K"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_32pxv"
                            android:layout_width="@dimen/_87pxh"
                            android:letterSpacing="0.01"
                            android:paddingBottom="@dimen/_7pxv"
                            android:paddingEnd="@dimen/_16pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_7pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_3_6k"
                            android:textAllCaps="false"
                            android:textColor="@color/{&quot;name&quot;:&quot;onErrorContainer&quot;}"
                            android:textFontWeight="600"
                            android:textSize="@dimen/_14pxh"
                            android:textStyle="bold" tools:text="@string/lbl_3_6k"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageCategoriesXClose"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_categories_x_close_onerrorcontainer"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_categories_x_close_onerrorcontainer"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearAutoLayoutHorizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_gray_800_99_radius_14"
                        android:drawablePadding="@dimen/_9pxh"
                        android:drawableStart="@drawable/img_signal"
                        android:ellipsize="end"
                        android:fontFamily="@font/urbanist"
                        android:gravity="center"
                        android:id="@+id/btnWeeklyRanking"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_28pxv"
                        android:layout_width="@dimen/_143pxh"
                        android:letterSpacing="0.01"
                        android:paddingBottom="@dimen/_6pxv"
                        android:paddingEnd="@dimen/_17pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_6pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_weekly_ranking"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;onErrorContainer&quot;}"
                        android:textFontWeight="500"
                        android:textSize="@dimen/_12pxh"
                        android:textStyle="bold" tools:text="@string/lbl_weekly_ranking"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_gray_800_99_radius_14"
                        android:drawablePadding="@dimen/_9pxh"
                        android:drawableStart="@drawable/img_floatingicon"
                        android:ellipsize="end"
                        android:fontFamily="@font/urbanist"
                        android:gravity="center"
                        android:id="@+id/btnExplore"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_28pxv"
                        android:layout_width="@dimen/_98pxh"
                        android:letterSpacing="0.01"
                        android:paddingBottom="@dimen/_6pxv"
                        android:paddingEnd="@dimen/_16pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_6pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_explore"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;onErrorContainer&quot;}"
                        android:textFontWeight="500"
                        android:textSize="@dimen/_12pxh"
                        android:textStyle="bold" tools:text="@string/lbl_explore"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameSpacer"
                    android:layout_height="wrap_content" android:layout_width="match_parent"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:gravity="center_horizontal"
                    android:id="@+id/recyclerSeeLive"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_12pxh}" tools:itemCount="7" tools:listitem="@layout/row_seelive"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
