<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="postsQuickPhotoVM" type="com.tikpikshortvideosocialnetworking.app.modules.postsquickphoto.data.viewmodel.PostsQuickPhotoVM"/>
    </data>
    <LinearLayout android:id="@+id/linearPostsQuickPhoto"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylenameonErrorContainer_4">
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameAddSound"
            android:layout_height="@dimen/_850pxv" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearFour"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyle_2">
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutHorizontal"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingEnd="@dimen/_89pxh"
                    android:paddingStart="@dimen/_89pxh"
                    android:paddingTop="@dimen/_17pxv" style="@style/groupStyle{&quot;name&quot;:&quot;primaryContainer&quot;,&quot;opacity&quot;:1}">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCamera"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{postsQuickPhotoVM.postsQuickPhotoModel.txtCamera}"
                        style="@style/txtUrbanistromanbold16_3" tools:text="@string/lbl_camera"/>
                    <FrameLayout android:id="@+id/frameSpacer"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtQuick"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{postsQuickPhotoVM.postsQuickPhotoModel.txtQuick}"
                        style="@style/txtUrbanistromanbold16_4" tools:text="@string/lbl_quick"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTemplates"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{postsQuickPhotoVM.postsQuickPhotoModel.txtTemplates}"
                        style="@style/txtUrbanistromanbold16_3" tools:text="@string/lbl_templates"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearAutoLayoutVertical"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarAppBar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <ImageView android:id="@+id/imageCategoriesXClose"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_categories_x_close_onerrorcontainer"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_categories_x_close_onerrorcontainer"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_gray_800_99_radius_16"
                        android:drawablePadding="@dimen/_8pxh"
                        android:drawableStart="@drawable/img_categoriesmusic_song_componentadditional_icons"
                        android:ellipsize="end"
                        android:fontFamily="@font/urbanist"
                        android:gravity="center"
                        android:id="@+id/btnAddSound"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:paddingStart="@dimen/_16pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_add_sound"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;onErrorContainer&quot;}"
                        android:textFontWeight="600"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:text="@string/lbl_add_sound"/>
                </androidx.appcompat.widget.Toolbar>
                <ImageView android:id="@+id/imageCategoriesRepost"
                    android:layout_height="@dimen/_36pxh"
                    android:layout_width="@dimen/_36pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_categories_repost"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_categories_repost"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFlip"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{postsQuickPhotoVM.postsQuickPhotoModel.txtFlip}"
                    style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_flip"/>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutVertical1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_26pxh" android:paddingStart="@dimen/_26pxh">
                    <ImageView android:id="@+id/imageLock"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_lock_onerrorcontainer"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_lock_onerrorcontainer"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFilters"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{postsQuickPhotoVM.postsQuickPhotoModel.txtFilters}"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_filters"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutVertical2"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_24pxh" android:paddingStart="@dimen/_24pxh">
                    <ImageView android:id="@+id/imageUser"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user_onerrorcontainer"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user_onerrorcontainer"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtBeauty"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{postsQuickPhotoVM.postsQuickPhotoModel.txtBeauty}"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_beauty"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutVertical3"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_26pxh" android:paddingStart="@dimen/_26pxh">
                    <ImageView android:id="@+id/imageUser1"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtReply"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{postsQuickPhotoVM.postsQuickPhotoModel.txtReply}"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_reply"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageCategoriesFlash"
                    android:layout_height="@dimen/_36pxh"
                    android:layout_width="@dimen/_36pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_categories_flash"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_categories_flash"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFlash"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{postsQuickPhotoVM.postsQuickPhotoModel.txtFlash}"
                    style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_flash"/>
                <FrameLayout android:id="@+id/frameSpacer1"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutHorizontal1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_gray_800_99_radius_16"
                        android:drawablePadding="@dimen/_8pxh"
                        android:drawableStart="@drawable/img_iconly_bold_image"
                        android:ellipsize="end"
                        android:fontFamily="@font/urbanist"
                        android:gravity="center"
                        android:id="@+id/btnPhoto"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_32pxv"
                        android:layout_width="@dimen/_97pxh"
                        android:letterSpacing="0.01"
                        android:paddingBottom="@dimen/_7pxv"
                        android:paddingEnd="@dimen/_16pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_7pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_photo"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;onErrorContainer&quot;}"
                        android:textFontWeight="600"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:text="@string/lbl_photo"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_gray_800_99_radius_16"
                        android:drawablePadding="@dimen/_8pxh"
                        android:drawableStart="@drawable/img_iconly_bold_video"
                        android:ellipsize="end"
                        android:fontFamily="@font/urbanist"
                        android:gravity="center"
                        android:id="@+id/btnVideo"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_32pxv"
                        android:layout_width="@dimen/_98pxh"
                        android:letterSpacing="0.01"
                        android:paddingBottom="@dimen/_7pxv"
                        android:paddingEnd="@dimen/_16pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_7pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_video"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;gray_500&quot;}"
                        android:textFontWeight="600"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:text="@string/lbl_video"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearAutoLayoutHorizontal2"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/linearAutoLayoutVertical4"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <ImageView android:id="@+id/imageImage"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_36pxh"
                            android:layout_width="@dimen/_36pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_frame_red_400"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_frame_red_400"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtEffects"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{postsQuickPhotoVM.postsQuickPhotoModel.txtEffects}"
                            style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_effects"/>
                    </LinearLayout>
                    <FrameLayout android:id="@+id/frameSpacer2"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <View
                        android:background="@drawable/rectangle_gradient_s_name_deep_orange_a400_e_name_orange_600_border_name_onerrorcontainer_radius_40"
                        android:id="@+id/viewView"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_80pxh" android:layout_width="@dimen/_80pxh"/>
                    <FrameLayout android:id="@+id/frameSpacer3"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <LinearLayout
                        android:id="@+id/linearAutoLayoutVertical5"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <ImageView
                            android:id="@+id/imageIconlyBoldUpload"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_36pxh"
                            android:layout_width="@dimen/_36pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_iconly_bold_upload"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_bold_upload"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtUpload"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{postsQuickPhotoVM.postsQuickPhotoModel.txtUpload}"
                            style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_upload"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
