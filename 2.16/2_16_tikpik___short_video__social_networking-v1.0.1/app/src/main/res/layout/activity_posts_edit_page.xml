<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="postsEditPageVM" type="com.tikpikshortvideosocialnetworking.app.modules.postseditpage.data.viewmodel.PostsEditPageVM"/>
    </data>
    <LinearLayout android:id="@+id/linearPostsEditPage"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylenameonErrorContainer_8">
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameAddSound"
            android:layout_height="@dimen/_850pxv" android:layout_width="match_parent">
            <View
                android:background="@drawable/rectangle_gradient_s_name_primarycontainer_e_name_primarycontainer_opacity_0_6"
                android:id="@+id/viewView"
                android:layout_gravity="bottom|center"
                android:layout_height="@dimen/_200pxv" android:layout_width="@dimen/_428pxh"/>
            <LinearLayout android:id="@+id/linearAutoLayoutVertical"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarAppBar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <ImageView android:id="@+id/imageArrowDown"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_down_onerrorcontainer"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down_onerrorcontainer"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_gray_800_99_radius_16"
                        android:drawablePadding="@dimen/_8pxh"
                        android:drawableStart="@drawable/img_categories_music"
                        android:ellipsize="end"
                        android:fontFamily="@font/urbanist"
                        android:gravity="center"
                        android:id="@+id/btnAddSound"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:paddingStart="@dimen/_16pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_add_sound"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;onErrorContainer&quot;}"
                        android:textFontWeight="600"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:text="@string/lbl_add_sound"/>
                </androidx.appcompat.widget.Toolbar>
                <ImageView android:id="@+id/imageCategoriesFont"
                    android:layout_height="@dimen/_36pxh"
                    android:layout_width="@dimen/_36pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_categories_font"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_categories_font"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFlip"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{postsEditPageVM.postsEditPageModel.txtFlip}"
                    style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_flip"/>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutVertical1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_26pxh" android:paddingStart="@dimen/_26pxh">
                    <ImageView android:id="@+id/imageCategoriesSticker"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_categories_sticker"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_categories_sticker"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFlip1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{postsEditPageVM.postsEditPageModel.txtFlip1}"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_flip"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutVertical2"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_26pxh" android:paddingStart="@dimen/_26pxh">
                    <ImageView android:id="@+id/imageCategoriesFilter"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_categories_filter_onerrorcontainer"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_categories_filter_onerrorcontainer"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFlip2"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{postsEditPageVM.postsEditPageModel.txtFlip2}"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_flip"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutVertical3"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_26pxh" android:paddingStart="@dimen/_26pxh">
                    <ImageView android:id="@+id/imageCategoriesSubtitle"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_categories_subtitle"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_categories_subtitle"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSpeed"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{postsEditPageVM.postsEditPageModel.txtSpeed}"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_speed"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutVertical4"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_26pxh" android:paddingStart="@dimen/_26pxh">
                    <ImageView android:id="@+id/imageLocation"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_location_onerrorcontainer"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_location_onerrorcontainer"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFilters"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{postsEditPageVM.postsEditPageModel.txtFilters}"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_filters"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutVertical5"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_24pxh" android:paddingStart="@dimen/_24pxh">
                    <ImageView android:id="@+id/imageUser"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtBeauty"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{postsEditPageVM.postsEditPageModel.txtBeauty}"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_beauty"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutVertical6"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_26pxh" android:paddingStart="@dimen/_26pxh">
                    <ImageView android:id="@+id/imageMenu"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_menu"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_menu"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTimer"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{postsEditPageVM.postsEditPageModel.txtTimer}"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_timer"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutVertical7"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_26pxh" android:paddingStart="@dimen/_26pxh">
                    <ImageView android:id="@+id/imageCategoriesEnhance"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_categories_enhance"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_categories_enhance"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtReply"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{postsEditPageVM.postsEditPageModel.txtReply}"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_reply"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageCategoriesFlash"
                    android:layout_height="@dimen/_36pxh"
                    android:layout_width="@dimen/_36pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_categories_flash"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_categories_flash"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFlash"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{postsEditPageVM.postsEditPageModel.txtFlash}"
                    style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_flash"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearAutoLayoutHorizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_deep_orange_50_radius_16"
                        android:ellipsize="end"
                        android:fontFamily="@font/urbanist"
                        android:gravity="center"
                        android:id="@+id/btnPostToStory"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_58pxv"
                        android:layout_width="@dimen/_184pxh"
                        android:letterSpacing="0.01"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_19pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_post_to_story"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                        android:textFontWeight="700"
                        android:textSize="@dimen/_16pxh"
                        android:textStyle="bold" tools:text="@string/lbl_post_to_story"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_primary_radius_16"
                        android:ellipsize="end"
                        android:fontFamily="@font/urbanist"
                        android:gravity="center"
                        android:id="@+id/btnNext"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_58pxv"
                        android:layout_width="@dimen/_184pxh"
                        android:letterSpacing="0.01"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_19pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_next"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;onErrorContainer&quot;}"
                        android:textFontWeight="700"
                        android:textSize="@dimen/_16pxh"
                        android:textStyle="bold" tools:text="@string/lbl_next"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
