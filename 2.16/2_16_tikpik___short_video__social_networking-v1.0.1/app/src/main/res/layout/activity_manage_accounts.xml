<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="manageAccountsVM" type="com.tikpikshortvideosocialnetworking.app.modules.manageaccounts.data.viewmodel.ManageAccountsVM"/>
    </data>
    <LinearLayout android:id="@+id/linearManageAccounts"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;onErrorContainer&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_down"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTitleSection"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{manageAccountsVM.manageAccountsModel.txtTitleSection}"
                style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_manage_accounts"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearAccountInformation"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_19pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_19pxv">
            <TextView android:ellipsize="end"
                android:id="@+id/txtAccountInformation"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{manageAccountsVM.manageAccountsModel.txtAccountInformation}"
                style="@style/txtUrbanistromanbold20" tools:text="@string/msg_account_information"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearAutoLayoutHorizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageIconlyCurvedCall"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconly_curved_call"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_curved_call"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPhoneNumber"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{manageAccountsVM.manageAccountsModel.txtPhoneNumber}"
                    style="@style/txtUrbanistsemibold18_3" tools:text="@string/lbl_phone_number"/>
                <FrameLayout android:id="@+id/frameSpacer"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtThreeHundredNinetySixMillionSe"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{manageAccountsVM.manageAccountsModel.txtThreeHundredNinetySixMillionSe}"
                    style="@style/txtUrbanistsemibold18_3" tools:text="@string/lbl_1_123_456_789"/>
                <ImageView android:id="@+id/imageArrowRight"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right_gray_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right_gray_900"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearAutoLayoutHorizontal1"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageIconlyCurvedMessage"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconly_curved_message"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_curved_message"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEmail"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{manageAccountsVM.manageAccountsModel.txtEmail}"
                    style="@style/txtUrbanistsemibold18_3" tools:text="@string/lbl_email"/>
                <FrameLayout android:id="@+id/frameSpacer1"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAndrewAins"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{manageAccountsVM.manageAccountsModel.txtAndrewAins}"
                    style="@style/txtUrbanistsemibold18_3" tools:text="@string/lbl_andrew_ains2"/>
                <ImageView android:id="@+id/imageArrowRight1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right_gray_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right_gray_900"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearAutoLayoutHorizontal2"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageImage"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconly_curved_calendar"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_curved_calendar"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDateOfBirth"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{manageAccountsVM.manageAccountsModel.txtDateOfBirth}"
                    style="@style/txtUrbanistsemibold18_3" tools:text="@string/lbl_date_of_birth"/>
                <FrameLayout android:id="@+id/frameSpacer2"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDate"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{manageAccountsVM.manageAccountsModel.txtDate}"
                    style="@style/txtUrbanistsemibold18_3" tools:text="@string/lbl_27_12_1995"/>
                <ImageView android:id="@+id/imageCalendar"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_calendar"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_calendar"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtAccountControl"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{manageAccountsVM.manageAccountsModel.txtAccountControl}"
                style="@style/txtUrbanistromanbold20" tools:text="@string/lbl_account_control"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearAutoLayoutHorizontal3"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearSixtyNine"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageIconlyCurvedSwap"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_iconly_curved_swap"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_curved_swap"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_vertical"
                        android:id="@+id/txtSwitchToBusiness"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{manageAccountsVM.manageAccountsModel.txtSwitchToBusiness}"
                        style="@style/txtUrbanistsemibold18_3" tools:text="@string/msg_switch_to_business"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowRight2"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right_gray_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right_gray_900"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearSeventy"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageIconlyCurvedDelete"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconly_curved_delete"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_curved_delete"/>
                <TextView android:ellipsize="end"
                    android:gravity="bottom"
                    android:id="@+id/txtDeleteAccount"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{manageAccountsVM.manageAccountsModel.txtDeleteAccount}"
                    style="@style/txtUrbanistsemibold18_4" tools:text="@string/lbl_delete_account"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
