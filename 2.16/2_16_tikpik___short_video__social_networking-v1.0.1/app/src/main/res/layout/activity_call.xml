<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="callVM" type="com.tikpikshortvideosocialnetworking.app.modules.call.data.viewmodel.CallVM"/>
    </data>
    <LinearLayout android:id="@+id/linearCall"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;onErrorContainer&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageImage"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_group"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearTheresaVarnes"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_48pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh" android:paddingTop="@dimen/_48pxv">
            <LinearLayout android:id="@+id/linearWidget"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_94pxv"
                android:paddingEnd="@dimen/_68pxh"
                android:paddingStart="@dimen/_68pxh"
                android:paddingTop="@dimen/_94pxv" style="@style/groupStyle_3">
                <FrameLayout android:id="@+id/frameTypeDefaultC"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_230pxv"
                    android:layout_width="@dimen/_230pxh"
                    android:paddingHorizontal="@dimen/_15pxh"
                    android:paddingVertical="@dimen/_15pxv" style="@style/groupStylecornerRadius_6">
                    <ImageView android:id="@+id/imageCircleImage"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_200pxh"
                        android:layout_width="@dimen/_200pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_ellipse_200x200}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_200x200"/>
                </FrameLayout>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtTheresaVarnes"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{callVM.callModel.txtTheresaVarnes}"
                style="@style/txtUrbanistromanbold32_1" tools:text="@string/lbl_theresa_varnes"/>
            <TextView android:alpha="0.8" android:ellipsize="end"
                android:id="@+id/txtDuration"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@{callVM.callModel.txtDuration}"
                style="@style/txtUrbanistromanmedium18_3" tools:text="@string/lbl_07_39_minutes"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearAutoLayoutHorizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_gradient_s_name_deep_orange_a400_e_name_orange_600_radius_40"
                    android:id="@+id/btnClose"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_80pxh"
                    android:layout_width="@dimen/_80pxh"
                    android:paddingHorizontal="@dimen/_22pxh"
                    android:paddingVertical="@dimen/_22pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_close_onerrorcontainer_80x80" tools:ignore="ContentDescription"/>
                <ImageButton
                    android:background="@drawable/rectangle_gradient_s_name_deep_purple_a400_e_name_onerror_radius_40"
                    android:id="@+id/btnVideoOne"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_80pxh"
                    android:layout_width="@dimen/_80pxh"
                    android:paddingHorizontal="@dimen/_22pxh"
                    android:paddingVertical="@dimen/_22pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_video_1" tools:ignore="ContentDescription"/>
                <ImageButton
                    android:background="@drawable/rectangle_gradient_s_name_onprimarycontainer_e_name_orange_a200_radius_40"
                    android:id="@+id/btnUser"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_80pxh"
                    android:layout_width="@dimen/_80pxh"
                    android:paddingHorizontal="@dimen/_22pxh"
                    android:paddingVertical="@dimen/_22pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_user_onerrorcontainer_80x80" tools:ignore="ContentDescription"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
