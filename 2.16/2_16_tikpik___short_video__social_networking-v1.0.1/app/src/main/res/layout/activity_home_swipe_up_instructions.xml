<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homeSwipeUpInstructionsVM" type="com.tikpikshortvideosocialnetworking.app.modules.homeswipeupinstructions.data.viewmodel.HomeSwipeUpInstructionsVM"/>
    </data>
    <LinearLayout android:id="@+id/linearCategoriesLive"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylenameonErrorContainer_10">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageCategoriesLive"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_categories_live"
                tools:ignore="ContentDescription" tools:src="@drawable/img_categories_live"/>
            <ImageView android:id="@+id/imageRewind"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_rewind"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rewind"/>
        </androidx.appcompat.widget.Toolbar>
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameClose"
            android:layout_height="@dimen/_751pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_23pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_23pxv">
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearAutoLayoutVertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <LinearLayout
                        android:id="@+id/linearAutoLayoutHorizontal1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <FrameLayout
                            android:id="@+id/frameTypeEditAvatar"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_60pxv" android:layout_width="@dimen/_60pxh">
                            <ImageView
                                android:id="@+id/imageCircleImage"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_60pxh"
                                android:layout_width="@dimen/_60pxh"
                                android:scaleType="centerCrop"
                                circular="@{true}"
                                imageRes="@{@drawable/img_ellipse_60x60}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_60x60"/>
                            <ImageView android:id="@+id/imageClose"
                                android:layout_gravity="bottom|end"
                                android:layout_height="@dimen/_15pxh"
                                android:layout_width="@dimen/_15pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_close"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
                        </FrameLayout>
                        <LinearLayout
                            android:id="@+id/linearAutoLayoutVertical1"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtName"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{homeSwipeUpInstructionsVM.homeSwipeUpInstructionsModel.txtName}"
                                style="@style/txtUrbanistromanbold18" tools:text="@string/lbl_jenny_wilson"/>
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtInformation"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.01"
                                android:singleLine="true"
                                android:text="@{homeSwipeUpInstructionsVM.homeSwipeUpInstructionsModel.txtInformation}"
                                style="@style/txtUrbanistromanmedium14" tools:text="@string/msg_actress_singer"/>
                        </LinearLayout>
                    </LinearLayout>
                    <TextView android:id="@+id/txtDescription"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:text="@{homeSwipeUpInstructionsVM.homeSwipeUpInstructionsModel.txtDescription}"
                        style="@style/txtUrbanistromanmedium14_1" tools:text="@string/msg_hi_everyone_in"/>
                    <LinearLayout
                        android:id="@+id/linearAutoLayoutHorizontal2"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imagePlay"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_24pxh"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_play"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_play"/>
                        <ImageView
                            android:id="@+id/imageCategoriesMusic"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_categories_music"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_categories_music"/>
                        <TextView android:ellipsize="end"
                            android:gravity="bottom"
                            android:id="@+id/txtFavoriteGirlBy"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{homeSwipeUpInstructionsVM.homeSwipeUpInstructionsModel.txtFavoriteGirlBy}"
                            style="@style/txtUrbanistromanmedium12" tools:text="@string/msg_favorite_girl_by"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearAutoLayoutVertical2"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <ImageView android:id="@+id/imageCategoriesReport"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_categories_report"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_categories_report"/>
                    <LinearLayout
                        android:id="@+id/linearAutoLayoutVertical3"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <ImageView android:gravity="center_horizontal"
                            android:id="@+id/imageIconlyBoldHeart"
                            android:layout_height="@dimen/_36pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_iconly_bold_heart"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_bold_heart"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtK"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{homeSwipeUpInstructionsVM.homeSwipeUpInstructionsModel.txtK}"
                            style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_225_9k"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageUser"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtK1"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{homeSwipeUpInstructionsVM.homeSwipeUpInstructionsModel.txtK1}"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_24_8k"/>
                    <ImageView android:id="@+id/imageBookmark"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_bookmark"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_bookmark"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtK2"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{homeSwipeUpInstructionsVM.homeSwipeUpInstructionsModel.txtK2}"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_15_6k"/>
                    <ImageView android:id="@+id/imageCategoriesShare"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_categories_share"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_categories_share"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtK3"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{homeSwipeUpInstructionsVM.homeSwipeUpInstructionsModel.txtK3}"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_20_7k"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearAutoLayoutVertical4"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtSwipeUpToSee"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{homeSwipeUpInstructionsVM.homeSwipeUpInstructionsModel.txtSwipeUpToSee}"
                    style="@style/txtUrbanistromanmedium18" tools:text="@string/msg_swipe_up_to_see"/>
                <ImageView android:id="@+id/imageVector"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_169pxh"
                    android:layout_width="@dimen/_187pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_cyan_a400"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_cyan_a400"/>
            </LinearLayout>
        </FrameLayout>
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameBottomBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStylecornerRadius_7">
            <LinearLayout android:id="@+id/linearHome"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <ImageView android:id="@+id/imageHome"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_nav_home"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_nav_home"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtHome"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{homeSwipeUpInstructionsVM.homeSwipeUpInstructionsModel.txtHome}"
                    style="@style/txtUrbanistromanbold10" tools:text="@string/lbl_home"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearDiscover"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <ImageView android:id="@+id/imageDiscover"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_nav_discover"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_nav_discover"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtDiscover"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{homeSwipeUpInstructionsVM.homeSwipeUpInstructionsModel.txtDiscover}"
                    style="@style/txtUrbanistromanmedium10" tools:text="@string/lbl_discover"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearFrame"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;primary&quot;}cornerRadius">
                <ImageView android:id="@+id/imageIconlyCurvedPlus"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconly_curved_plus"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_curved_plus"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearAutoLayoutVertical5"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <ImageView android:id="@+id/imageInbox"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_nav_inbox"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_nav_inbox"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtInbox"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{homeSwipeUpInstructionsVM.homeSwipeUpInstructionsModel.txtInbox}"
                    style="@style/txtUrbanistromanmedium10" tools:text="@string/lbl_inbox"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearProfile"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <ImageView android:id="@+id/imageProfile"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_nav_profile"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_nav_profile"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtProfile"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{homeSwipeUpInstructionsVM.homeSwipeUpInstructionsModel.txtProfile}"
                    style="@style/txtUrbanistromanmedium10" tools:text="@string/lbl_profile"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
