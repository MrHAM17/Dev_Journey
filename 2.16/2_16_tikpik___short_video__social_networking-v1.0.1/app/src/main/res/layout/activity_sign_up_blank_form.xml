<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpBlankFormVM" type="com.tikpikshortvideosocialnetworking.app.modules.signupblankform.data.viewmodel.SignUpBlankFormVM"/>
    </data>
    <LinearLayout android:id="@+id/linearSignUpBlankForm"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;onErrorContainer&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageImage"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_group"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearEmail"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_47pxv"
            android:paddingEnd="@dimen/_23pxh"
            android:paddingStart="@dimen/_23pxh" android:paddingTop="@dimen/_47pxv">
            <TextView android:gravity="start"
                android:id="@+id/txtCreateYourAccount"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="@{signUpBlankFormVM.signUpBlankFormModel.txtCreateYourAccount}"
                style="@style/txtUrbanistromanbold48" tools:text="@string/msg_create_your_account"/>
            <EditText
                android:background="@drawable/rectangle_bg_name_onerrorcontainer_border_name_gray_400_radius_12"
                android:drawablePadding="@dimen/_12pxh"
                android:drawableStart="@drawable/img_checkmark"
                android:ellipsize="end"
                android:fontFamily="@font/urbanist"
                android:hint="@string/lbl_email"
                android:id="@+id/etEmail" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_21pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_21pxv"
                android:singleLine="true"
                android:text="@={signUpBlankFormVM.signUpBlankFormModel.etEmailValue}"
                android:textColor="@color/{&quot;name&quot;:&quot;gray_500&quot;}"
                android:textColorHint="@color/{&quot;name&quot;:&quot;gray_500&quot;}"
                android:textFontWeight="400"
                android:textSize="@dimen/_14pxh"
                android:textStyle="normal" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_name_onerrorcontainer_border_name_gray_400_radius_12"
                android:drawableEnd="@drawable/img_eye"
                android:drawablePadding="@dimen/_30pxh"
                android:drawableStart="@drawable/img_location"
                android:ellipsize="end"
                android:fontFamily="@font/urbanist"
                android:hint="@string/lbl_password"
                android:id="@+id/etPassword" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_19pxv"
                android:paddingEnd="@dimen/_20pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:text="@={signUpBlankFormVM.signUpBlankFormModel.etPasswordValue}"
                android:textColor="@color/{&quot;name&quot;:&quot;gray_500&quot;}"
                android:textColorHint="@color/{&quot;name&quot;:&quot;gray_500&quot;}"
                android:textFontWeight="400"
                android:textSize="@dimen/_14pxh"
                android:textStyle="normal" tools:ignore="Autofill"/>
            <androidx.appcompat.widget.AppCompatCheckBox
                android:drawablePadding="@dimen/_8pxh"
                android:drawableStart="@drawable/drawable_dhi_0"
                android:ellipsize="end"
                android:fontFamily="@font/urbanist"
                android:id="@+id/checkBoxRememberme"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_3pxv"
                android:paddingTop="@dimen/_3pxv"
                android:singleLine="true"
                android:text="@string/lbl_remember_me"
                android:textColor="@color/{&quot;name&quot;:&quot;gray_900&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:text="@string/lbl_remember_me"/>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_blue_gray_100_radius_16"
                android:ellipsize="end"
                android:fontFamily="@font/urbanist"
                android:gravity="center" android:id="@+id/btnSignUp"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_55pxv"
                android:layout_width="@dimen/_380pxh"
                android:letterSpacing="0.01"
                android:paddingHorizontal="@dimen/_16pxh"
                android:paddingVertical="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_sign_up2"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;gray_600&quot;}"
                android:textFontWeight="700"
                android:textSize="@dimen/_16pxh"
                android:textStyle="bold" tools:text="@string/lbl_sign_up2"/>
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <View
                    android:background="@color/{&quot;name&quot;:&quot;gray_200&quot;}"
                    android:id="@+id/lineLine"
                    android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_96pxh"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOrContinueWith"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{signUpBlankFormVM.signUpBlankFormModel.txtOrContinueWith}"
                    style="@style/txtUrbanistsemibold18_2" tools:text="@string/msg_or_continue_with"/>
                <View
                    android:background="@color/{&quot;name&quot;:&quot;gray_200&quot;}"
                    android:id="@+id/lineLine1"
                    android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_96pxh"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearWidget"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameTypeLoginType"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_60pxv"
                    android:layout_width="@dimen/_111pxh"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingEnd="@dimen/_42pxh"
                    android:paddingStart="@dimen/_42pxh"
                    android:paddingTop="@dimen/_17pxv" style="@style/groupStylecornerRadius_1">
                    <ImageView android:id="@+id/imageImage1"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_auto_layout_horizontal"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_auto_layout_horizontal"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameTypeLoginType1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_60pxv"
                    android:layout_width="@dimen/_111pxh"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingEnd="@dimen/_42pxh"
                    android:paddingStart="@dimen/_42pxh"
                    android:paddingTop="@dimen/_17pxv" style="@style/groupStylecornerRadius_1">
                    <ImageView android:id="@+id/imageImage2"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_auto_layout_horizontal_red_500"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_auto_layout_horizontal_red_500"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameTypeLoginType2"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_60pxv"
                    android:layout_width="@dimen/_111pxh"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingEnd="@dimen/_42pxh"
                    android:paddingStart="@dimen/_42pxh"
                    android:paddingTop="@dimen/_17pxv" style="@style/groupStylecornerRadius_1">
                    <ImageView android:id="@+id/imageImage3"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_auto_layout_horizontal_black_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_auto_layout_horizontal_black_900"/>
                </FrameLayout>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearConfirmation"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:id="@+id/txtConfirmation"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{signUpBlankFormVM.signUpBlankFormModel.txtConfirmation}"
                    style="@style/txtUrbanistregular14" tools:text="@string/msg_already_have_an"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSignIn"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{signUpBlankFormVM.signUpBlankFormModel.txtSignIn}"
                    style="@style/txtUrbanistsemibold14" tools:text="@string/lbl_sign_in"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
