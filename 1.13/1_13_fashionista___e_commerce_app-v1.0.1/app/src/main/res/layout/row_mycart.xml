<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearFrame2"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageJacket"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_171pxh"
            android:layout_width="@dimen/_142pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_20pxh}"
            imageRes="@{@drawable/img_unsplash_8hqpxttomn0}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_unsplash_8hqpxttomn0"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearFrame3"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtJacket"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_jacket"
                style="@style/txtPoppinsmedium18" tools:text="@string/lbl_jacket"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtBlackJacket"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_black_jacket"
                style="@style/txtPoppinsregular16" tools:text="@string/lbl_black_jacket"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSizeXL"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_size_xl"
                style="@style/txtPoppinsregular14_2" tools:text="@string/lbl_size_xl"/>
            <TextView android:ellipsize="end" android:id="@+id/txtPrice"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_134_98"
                style="@style/txtPoppinssemibold18" tools:text="@string/lbl_134_98"/>
            <LinearLayout android:id="@+id/linearFrame"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <LinearLayout android:gravity="spaceAround"
                    android:id="@+id/linearUser"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_2pxv"
                    android:paddingTop="@dimen/_2pxv" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}cornerRadius">
                    <ImageButton android:id="@+id/btnUser"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_user_white_a700" tools:ignore="ContentDescription"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtOne"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_1"
                        style="@style/txtPoppinsregular14" tools:text="@string/lbl_1"/>
                    <ImageButton android:id="@+id/btnClose"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_user_white_a700" tools:ignore="ContentDescription"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageImage"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_close"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
