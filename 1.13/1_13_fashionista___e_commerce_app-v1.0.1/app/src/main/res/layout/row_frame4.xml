<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearFrame1"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/_10pxv"
        android:paddingEnd="@dimen/_16pxh"
        android:paddingStart="@dimen/_16pxh"
        android:paddingTop="@dimen/_10pxv" style="@style/groupStyle{&quot;name&quot;:&quot;gray_100&quot;}cornerRadius">
        <ImageView android:id="@+id/imageYoupurchasedShoes"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_40pxh"
            android:layout_width="@dimen/_40pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_10pxh}"
            imageRes="@{@drawable/img_unsplash_qnu_ur0o5x8}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_unsplash_qnu_ur0o5x8"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearFrame2"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtYouPurchasedShoes"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/msg_you_purchased_shoes"
                style="@style/txtPoppinsregular14" tools:text="@string/msg_you_purchased_shoes"/>
            <TextView android:ellipsize="end" android:id="@+id/txtTime"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_02_minites_ago"
                style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_02_minites_ago"/>
        </LinearLayout>
    </LinearLayout>
</layout>
