<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="chatVM" type="com.housitbuyrentsellproperty.app.modules.chat.data.viewmodel.ChatVM"/>
    </data>
    <LinearLayout android:id="@+id/linearChat"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageButton
                android:background="@drawable/rectangle_bg_name_gray_100_radius_bl_25_br_25"
                android:id="@+id/btnArrowLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrow_left" tools:ignore="ContentDescription"/>
            <LinearLayout android:id="@+id/linearGroup37"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearGroup40"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <FrameLayout android:id="@+id/frameGroup38"
                        android:layout_height="@dimen/_50pxv" android:layout_width="@dimen/_50pxh">
                        <ImageView android:id="@+id/imageShape"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_50pxh"
                            android:layout_width="@dimen/_50pxh"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_shape_27}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_shape_27"/>
                        <LinearLayout android:id="@+id/linearShape"
                            android:layout_gravity="top|end"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:paddingHorizontal="@dimen/_2pxh"
                            android:paddingVertical="@dimen/_2pxv" style="@style/groupStylenamegray_4">
                            <View
                                android:background="@drawable/rectangle_bg_name_green_a400_radius_4"
                                android:id="@+id/viewShape"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_8pxh" android:layout_width="@dimen/_8pxh"/>
                        </LinearLayout>
                    </FrameLayout>
                    <LinearLayout android:id="@+id/linearGroup39"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtMilano"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{chatVM.chatModel.txtMilano}"
                            style="@style/txtRalewaysemibold14_1" tools:text="@string/lbl_milano"/>
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtOnline"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{chatVM.chatModel.txtOnline}"
                            style="@style/txtRalewaymedium12_1" tools:text="@string/lbl_online"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <ImageButton
                android:background="@drawable/rectangle_bg_name_gray_100_radius_bl_25_br_25"
                android:id="@+id/btnSettings"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_settings_blue_gray_800_01" tools:ignore="ContentDescription"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="end"
            android:id="@+id/linearChatArea"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_24pxv"
            android:paddingEnd="@dimen/_18pxh"
            android:paddingStart="@dimen/_18pxh"
            android:paddingTop="@dimen/_24pxv" style="@style/groupStylenamegray_5">
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_onprimarycontainer_radius_12"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:gravity="center"
                android:id="@+id/btnDecember122022"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_24pxv"
                android:layout_width="@dimen/_124pxh"
                android:paddingBottom="@dimen/_6pxv"
                android:paddingEnd="@dimen/_17pxh"
                android:paddingStart="@dimen/_17pxh"
                android:paddingTop="@dimen/_6pxv"
                android:singleLine="true"
                android:text="@string/msg_december_12_2022"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                android:textFontWeight="500"
                android:textSize="@dimen/_10pxh"
                android:textStyle="bold" tools:text="@string/msg_december_12_2022"/>
            <LinearLayout android:id="@+id/linearEstatesCardWide"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:paddingHorizontal="@dimen/_8pxh"
                android:paddingVertical="@dimen/_8pxv" style="@style/groupStylenamewhite_6">
                <FrameLayout android:id="@+id/frameShape"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_104pxv" android:layout_width="@dimen/_128pxh">
                    <ImageView android:id="@+id/imageShape1"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_104pxh"
                        android:layout_width="@dimen/_128pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_12pxh}"
                        imageRes="@{@drawable/img_shape_104x128}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_shape_104x128"/>
                    <LinearLayout android:id="@+id/linearSixtyNine"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <ImageButton
                            android:background="@drawable/rectangle_bg_name_white_a700_opacity_0_78_radius_12"
                            android:id="@+id/btnFavorite"
                            android:layout_height="@dimen/_25pxh"
                            android:layout_width="@dimen/_25pxh"
                            android:paddingHorizontal="@dimen/_6pxh"
                            android:paddingVertical="@dimen/_6pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_favorite_red_a200" tools:ignore="ContentDescription"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:background="@drawable/rectangle_bg_name_blue_gray_700_af_radius_8"
                            android:drawablePadding="@dimen/_6pxh"
                            android:drawableStart="@drawable/drawable_img_close"
                            android:ellipsize="end"
                            android:fontFamily="@font/raleway"
                            android:gravity="center"
                            android:id="@+id/btnApartment"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_7pxv"
                            android:paddingEnd="@dimen/_7pxh"
                            android:paddingStart="@dimen/_7pxh"
                            android:paddingTop="@dimen/_7pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_apartment"
                            android:textAllCaps="false"
                            android:textColor="@color/{&quot;name&quot;:&quot;gray_100&quot;}"
                            android:textFontWeight="500"
                            android:textSize="@dimen/_8pxh"
                            android:textStyle="bold" tools:text="@string/lbl_apartment"/>
                    </LinearLayout>
                </FrameLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearFrameEightyNine"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView
                        android:id="@+id/txtSkyDandelionsApartment"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:text="@{chatVM.chatModel.txtSkyDandelionsApartment}"
                        style="@style/txtRalewaysemibold14_1" tools:text="@string/msg_sky_dandelions_apartment"/>
                    <LinearLayout android:id="@+id/linearLayout"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageSignal"
                            android:layout_gravity="top"
                            android:layout_height="@dimen/_9pxh"
                            android:layout_width="@dimen/_9pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_signal_yellow_700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_signal_yellow_700"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtText"
                            android:layout_gravity="bottom"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{chatVM.chatModel.txtText}"
                            style="@style/txtRalewaybold8" tools:text="@string/lbl_4_2"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearLayout1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageLinkedin"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_9pxh"
                            android:layout_width="@dimen/_9pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_linkedin"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_linkedin"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_vertical"
                            android:id="@+id/txtJakartaIndonesia"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{chatVM.chatModel.txtJakartaIndonesia}"
                            style="@style/txtRalewaymedium8" tools:text="@string/msg_jakarta_indonesia"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <EditText
                android:background="@drawable/rectangle_bg_name_white_a700_radius_tl_16_bl_16_br_16"
                android:fontFamily="@font/raleway"
                android:hint="@string/msg_lorem_ipsum_dolor4"
                android:id="@+id/etDescription" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_14pxv"
                android:paddingEnd="@dimen/_16pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@={chatVM.chatModel.etDescriptionValue}"
                android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_600&quot;}"
                android:textColorHint="@color/{&quot;name&quot;:&quot;blue_gray_600&quot;}"
                android:textFontWeight="400"
                android:textSize="@dimen/_14pxh"
                android:textStyle="normal" tools:ignore="Autofill"/>
            <LinearLayout android:id="@+id/linearSeventySix"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearCheckmark"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCheckmark"
                        android:layout_gravity="top"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_checkmark_green_a400"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_checkmark_green_a400"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtText1"
                        android:layout_gravity="bottom"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{chatVM.chatModel.txtText1}"
                        style="@style/txtRalewaymedium10" tools:text="@string/lbl_10_45"/>
                </LinearLayout>
                <EditText
                    android:background="@drawable/rectangle_bg_name_blue_gray_800_01_radius_tr_16_bl_16_br_16"
                    android:fontFamily="@font/raleway"
                    android:hint="@string/msg_tempor_incididunt2"
                    android:id="@+id/etDescription1"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_14pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_14pxv"
                    android:singleLine="true"
                    android:text="@={chatVM.chatModel.etDescription1Value}"
                    android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                    android:textColorHint="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
            </LinearLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtText2"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{chatVM.chatModel.txtText2}"
                style="@style/txtRalewaymedium10" tools:text="@string/lbl_10_46"/>
            <FrameLayout android:id="@+id/frameSpacer"
                android:layout_height="wrap_content" android:layout_width="match_parent"/>
            <EditText
                android:background="@drawable/rectangle_bg_name_white_a700_radius_25"
                android:drawablePadding="@dimen/_10pxh"
                android:drawableStart="@drawable/img_camera"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:hint="@string/lbl_say_something"
                android:id="@+id/etFormChat" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@={chatVM.chatModel.etFormChatValue}"
                android:textColor="@color/{&quot;name&quot;:&quot;onPrimaryContainer&quot;}"
                android:textColorHint="@color/{&quot;name&quot;:&quot;onPrimaryContainer&quot;}"
                android:textFontWeight="400"
                android:textSize="@dimen/_14pxh"
                android:textStyle="normal" tools:ignore="Autofill"/>
        </LinearLayout>
    </LinearLayout>
</layout>
