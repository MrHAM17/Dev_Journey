<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="editSuccessVM" type="com.housitbuyrentsellproperty.app.modules.editsuccess.data.viewmodel.EditSuccessVM"/>
    </data>
    <LinearLayout android:id="@+id/linearEditSuccess"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:id="@+id/linearModal"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingHorizontal="@dimen/_24pxh"
            android:paddingVertical="@dimen/_24pxv" style="@style/groupStylenamewhite_4">
            <View
                android:background="@color/{&quot;name&quot;:&quot;blue_gray_600&quot;}"
                android:id="@+id/lineSlide"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_3pxv" android:layout_width="@dimen/_60pxh"/>
            <LinearLayout android:id="@+id/linearItemAlertPrimary"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingHorizontal="@dimen/_16pxh"
                android:paddingVertical="@dimen/_16pxv" style="@style/groupStylenameprimarycornerRadius_1">
                <FrameLayout android:id="@+id/frameThirtyFive"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_110pxv"
                    android:layout_width="@dimen/_110pxh"
                    android:paddingHorizontal="@dimen/_20pxh"
                    android:paddingVertical="@dimen/_20pxv" style="@style/groupStylecornerRadius_3">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_name_primary_radius_35"
                        android:id="@+id/btnCheckmark"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_70pxh"
                        android:layout_width="@dimen/_70pxh"
                        android:paddingHorizontal="@dimen/_19pxh"
                        android:paddingVertical="@dimen/_19pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_checkmark_white_a700_70x70" tools:ignore="ContentDescription"/>
                </FrameLayout>
            </LinearLayout>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtTitle"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="@{editSuccessVM.editSuccessModel.txtTitle}"
                style="@style/txtRalewaysemibold24" tools:text="@string/msg_your_listing_just"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{editSuccessVM.editSuccessModel.txtDescription}"
                style="@style/txtRalewayregular12" tools:text="@string/msg_lorem_ipsum_dolor2"/>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_primary_radius_35"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:gravity="center" android:id="@+id/btnClose"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_70pxv"
                android:layout_width="@dimen/_327pxh"
                android:paddingBottom="@dimen/_25pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_25pxv"
                android:singleLine="true"
                android:text="@string/lbl_close"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_16pxh"
                android:textStyle="bold" tools:text="@string/lbl_close"/>
        </LinearLayout>
    </LinearLayout>
</layout>
