<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="userEmptyVM" type="com.housitbuyrentsellproperty.app.modules.userempty.data.viewmodel.UserEmptyVM"/>
    </data>
    <LinearLayout android:id="@+id/linearSkip"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingBottom="@dimen/_21pxv"
        android:paddingTop="@dimen/_21pxv" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageButton
                android:background="@drawable/rectangle_bg_name_gray_100_radius_bl_25_br_25"
                android:id="@+id/btnArrowLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrow_left" tools:ignore="ContentDescription"/>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_gray_100_radius_19"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:gravity="center" android:id="@+id/btnSkip"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_skip"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_800_01&quot;}"
                android:textFontWeight="400"
                android:textSize="@dimen/_14pxh"
                android:textStyle="normal" tools:text="@string/lbl_skip"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearUserEmpty"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_11pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_11pxv">
            <TextView android:gravity="start" android:id="@+id/txtTitle"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="@{userEmptyVM.userEmptyModel.txtTitle}"
                style="@style/txtRalewaysemibold24" tools:text="@string/msg_fill_your_information"/>
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{userEmptyVM.userEmptyModel.txtDescription}"
                style="@style/txtRalewayregular14" tools:text="@string/msg_you_can_edit_this"/>
            <LinearLayout android:id="@+id/linearComponentFiftyOne"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStylenamegray_7">
                <ImageView android:id="@+id/imageLock"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_42pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_lock_onprimarycontainer"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_lock_onprimarycontainer"/>
                <ImageButton
                    android:background="@drawable/rectangle_bg_name_blue_gray_800_01_radius_15"
                    android:id="@+id/btnEdit"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_width="@dimen/_30pxh"
                    android:paddingHorizontal="@dimen/_9pxh"
                    android:paddingVertical="@dimen/_9pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_edit" tools:ignore="ContentDescription"/>
            </LinearLayout>
            <EditText
                android:background="@drawable/rectangle_bg_name_gray_100_radius_12"
                android:drawableEnd="@drawable/drawable_img_lock_blue_gray_800_01"
                android:drawablePadding="@dimen/_30pxh"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:hint="@string/msg_jonathan_anderson"
                android:id="@+id/etLock" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_26pxv"
                android:paddingEnd="@dimen/_16pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_26pxv"
                android:singleLine="true"
                android:text="@={userEmptyVM.userEmptyModel.etLockValue}"
                android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_800_01&quot;}"
                android:textColorHint="@color/{&quot;name&quot;:&quot;blue_gray_800_01&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_name_gray_100_radius_12"
                android:drawablePadding="@dimen/_10pxh"
                android:drawableStart="@drawable/img_settings_blue_gray_800_01"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:hint="@string/lbl_mobile_number"
                android:id="@+id/etMobileNumber"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_26pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_26pxv"
                android:singleLine="true"
                android:text="@={userEmptyVM.userEmptyModel.etMobileNumberValue}"
                android:textColor="@color/{&quot;name&quot;:&quot;onPrimaryContainer&quot;}"
                android:textColorHint="@color/{&quot;name&quot;:&quot;onPrimaryContainer&quot;}"
                android:textFontWeight="400"
                android:textSize="@dimen/_14pxh"
                android:textStyle="normal" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_name_blue_gray_600_radius_12"
                android:drawableEnd="@drawable/img_arrowdown_blue_gray_50"
                android:drawablePadding="@dimen/_30pxh"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:hint="@string/lbl_user_email_com"
                android:id="@+id/etEmail" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_26pxv"
                android:paddingEnd="@dimen/_16pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_26pxv"
                android:singleLine="true"
                android:text="@={userEmptyVM.userEmptyModel.etEmailValue}"
                android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_50&quot;}"
                android:textColorHint="@color/{&quot;name&quot;:&quot;blue_gray_50&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:ignore="Autofill"/>
            <ImageView android:id="@+id/imageProgressBarGradient"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxh"
                android:layout_width="@dimen/_100pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_progress_bar_gradient"
                tools:ignore="ContentDescription" tools:src="@drawable/img_progress_bar_gradient"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:background="@drawable/rectangle_bg_name_primary_radius_35"
            android:ellipsize="end" android:fontFamily="@font/raleway"
            android:gravity="center" android:id="@+id/btnFinish"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_70pxv"
            android:layout_width="@dimen/_327pxh"
            android:paddingBottom="@dimen/_25pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_25pxv" android:singleLine="true"
            android:text="@string/lbl_finish"
            android:textAllCaps="false"
            android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
            android:textFontWeight="600"
            android:textSize="@dimen/_16pxh" android:textStyle="bold" tools:text="@string/lbl_finish"/>
    </LinearLayout>
</layout>
