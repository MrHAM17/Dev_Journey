<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="viewallreviewsRowModel" type="com.housitbuyrentsellproperty.app.modules.propertydetails.data.model.ViewallreviewsRowModel"/>
    </data>
    <LinearLayout android:gravity="center_vertical"
        android:id="@+id/linearCardReviewTotal"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:paddingHorizontal="@dimen/_16pxh"
        android:paddingVertical="@dimen/_16pxv" style="@style/groupStylecornerRadius_7">
        <ImageButton
            android:background="@drawable/rectangle_bg_name_red_a200_radius_17"
            android:id="@+id/btnClose"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_53pxh"
            android:layout_width="@dimen/_53pxh"
            android:paddingHorizontal="@dimen/_12pxh"
            android:paddingVertical="@dimen/_12pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_close" tools:ignore="ContentDescription"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearLayout2"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearLayout3"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <androidx.appcompat.widget.AppCompatRatingBar
                    android:id="@+id/ratingBarRatingBar"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:numStars="5" android:rating="0" style="@style/Widget.AppCompat.RatingBar.Small"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtText1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{viewallreviewsRowModel.txtText1}"
                    style="@style/txtRalewaybold18" tools:text="@string/lbl_4_9"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearLayout"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFrom"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{viewallreviewsRowModel.txtFrom}"
                    style="@style/txtRalewayregular9" tools:text="@string/lbl_from"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtText2"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{viewallreviewsRowModel.txtText2}"
                    style="@style/txtMontserratregular9" tools:text="@string/lbl_112"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:id="@+id/txtReviewers"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{viewallreviewsRowModel.txtReviewers}"
                    style="@style/txtRalewayregular9" tools:text="@string/lbl_reviewers"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameSpacer"
            android:layout_height="wrap_content" android:layout_width="wrap_content"/>
        <FrameLayout android:id="@+id/frameWidget"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_30pxv" android:layout_width="@dimen/_70pxh">
            <ImageView android:id="@+id/imageImage2"
                android:layout_gravity="start"
                android:layout_height="@dimen/_30pxh"
                android:layout_width="@dimen/_30pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_play"
                tools:ignore="ContentDescription" tools:src="@drawable/img_play"/>
            <ImageView android:id="@+id/imageImage3"
                android:layout_gravity="center"
                android:layout_height="@dimen/_30pxh"
                android:layout_width="@dimen/_30pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_play"
                tools:ignore="ContentDescription" tools:src="@drawable/img_play"/>
            <ImageView android:id="@+id/imageImage"
                android:layout_gravity="end"
                android:layout_height="@dimen/_30pxh"
                android:layout_width="@dimen/_30pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_play"
                tools:ignore="ContentDescription" tools:src="@drawable/img_play"/>
        </FrameLayout>
    </LinearLayout>
</layout>
