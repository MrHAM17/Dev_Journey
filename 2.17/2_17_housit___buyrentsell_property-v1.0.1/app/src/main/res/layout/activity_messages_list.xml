                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtVelmaCole"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
