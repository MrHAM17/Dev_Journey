                    android:text="@{messagesListVM.messagesListModel.txtQuisnostrudexercitationullamco}"
                    style="@style/txtRalewayregular12" tools:text="@string/msg_quis_nostrud_exercitation2"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDuration"
                android:layout_gravity="top"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{messagesListVM.messagesListModel.txtDuration}"
                style="@style/txtRalewaymedium8_3" tools:text="@string/lbl_1_day_ago"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearCardMessage4"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:paddingHorizontal="@dimen/_10pxh"
