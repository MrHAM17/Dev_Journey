                android:background="@drawable/rectangle_bg_name_white_a700_radius_tl_16_bl_16_br_16"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:gravity="center" android:id="@+id/btnListing"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_32pxv"
                android:layout_width="@dimen/_152pxh"
                android:paddingBottom="@dimen/_7pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_7pxv"
