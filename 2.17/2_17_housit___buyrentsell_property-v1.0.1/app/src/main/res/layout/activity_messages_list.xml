                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{messagesListVM.messagesListModel.txtDuration1}"
                    style="@style/txtRalewaymedium8_3" tools:text="@string/lbl_2_days_ago"/>
