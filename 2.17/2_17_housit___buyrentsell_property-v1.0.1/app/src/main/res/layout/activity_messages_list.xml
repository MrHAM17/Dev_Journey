                tools:ignore="ContentDescription" tools:src="@drawable/img_shape_26"/>
            <FrameLayout android:id="@+id/frameVelmaCole"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_40pxv" android:layout_width="@dimen/_247pxh">
