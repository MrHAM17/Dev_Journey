<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="topAgentsProfileDetailTabContainerVM" type="com.housitbuyrentsellproperty.app.modules.topagentsprofiledetailtabcontainer.data.viewmodel.TopAgentsProfileDetailTabContainerVM"/>
    </data>
    <LinearLayout android:id="@+id/linearOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageButton
                android:background="@drawable/rectangle_bg_name_gray_100_radius_bl_25_br_25"
                android:id="@+id/btnArrowLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrow_left" tools:ignore="ContentDescription"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPageTitle"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{topAgentsProfileDetailTabContainerVM.topAgentsProfileDetailTabContainerModel.txtPageTitle}"
                style="@style/txtRalewaybold14" tools:text="@string/lbl_profile"/>
            <ImageButton
                android:background="@drawable/rectangle_bg_name_gray_100_radius_bl_25_br_25"
                android:id="@+id/btnTwitter"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_twitter" tools:ignore="ContentDescription"/>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollView"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearTopAgentsProfile"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:id="@+id/framePhotosUser"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_101pxv" android:layout_width="@dimen/_100pxh">
                    <ImageView android:id="@+id/imageShape"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_100pxv"
                        android:layout_width="match_parent"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_shape_19}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_shape_19"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_orange_300_radius_8"
                        android:ellipsize="end"
                        android:fontFamily="@font/raleway"
                        android:gravity="center"
                        android:id="@+id/btnOne"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_width="@dimen/_22pxh"
                        android:paddingBottom="@dimen/_6pxv"
                        android:paddingEnd="@dimen/_8pxh"
                        android:paddingStart="@dimen/_8pxh"
                        android:paddingTop="@dimen/_6pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_1"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                        android:textFontWeight="600"
                        android:textSize="@dimen/_12pxh"
                        android:textStyle="bold" tools:text="@string/lbl_1"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAmanda"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{topAgentsProfileDetailTabContainerVM.topAgentsProfileDetailTabContainerModel.txtAmanda}"
                    style="@style/txtRalewaybold14" tools:text="@string/lbl_amanda"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEmail"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{topAgentsProfileDetailTabContainerVM.topAgentsProfileDetailTabContainerModel.txtEmail}"
                    style="@style/txtRalewaysemibold12_1" tools:text="@string/msg_amanda_email_com"/>
                <com.google.android.material.tabs.TabLayout
                    android:gravity="center_horizontal"
                    android:id="@+id/tabLayoutTabview"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:tabMode="fixed"/>
                <androidx.viewpager2.widget.ViewPager2
                    android:gravity="center_horizontal"
                    android:id="@+id/viewPagerTabBarView"
                    android:layout_height="match_parent"
                    android:layout_weight="1" android:layout_width="match_parent"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
