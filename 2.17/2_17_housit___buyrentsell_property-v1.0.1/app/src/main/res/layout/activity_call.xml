<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="callVM" type="com.housitbuyrentsellproperty.app.modules.call.data.viewmodel.CallVM"/>
    </data>
    <LinearLayout android:id="@+id/linearCall"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageButton
                android:background="@drawable/rectangle_bg_name_gray_100_radius_bl_25_br_25"
                android:id="@+id/btnArrowLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrow_left" tools:ignore="ContentDescription"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout
            android:id="@+id/linearOneThousandTwoHundredTwentyFive"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtMilano"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{callVM.callModel.txtMilano}"
                style="@style/txtRalewaybold24" tools:text="@string/lbl_milano"/>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_onprimarycontainer_radius_12"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:gravity="center"
                android:id="@+id/btnOneThousandTwoHundredTwentyFive"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_24pxv"
                android:layout_width="@dimen/_49pxh"
                android:paddingBottom="@dimen/_6pxv"
                android:paddingEnd="@dimen/_12pxh"
                android:paddingStart="@dimen/_12pxh"
                android:paddingTop="@dimen/_6pxv"
                android:singleLine="true"
                android:text="@string/lbl_12_25"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                android:textFontWeight="500"
                android:textSize="@dimen/_10pxh"
                android:textStyle="bold" tools:text="@string/lbl_12_25"/>
            <FrameLayout android:id="@+id/frameSpacer"
                android:layout_height="wrap_content" android:layout_width="match_parent"/>
            <LinearLayout android:id="@+id/linearPhotosUser"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingHorizontal="@dimen/_18pxh"
                android:paddingVertical="@dimen/_18pxv" style="@style/groupStyle{&quot;name&quot;:&quot;onPrimaryContainer&quot;}cornerRadius">
                <FrameLayout android:id="@+id/frameFour"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_156pxv"
                    android:layout_width="@dimen/_156pxh"
                    android:paddingHorizontal="@dimen/_13pxh"
                    android:paddingVertical="@dimen/_13pxv" style="@style/groupStylecornerRadius_6">
                    <ImageView android:id="@+id/imageShape"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_130pxh"
                        android:layout_width="@dimen/_130pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_shape_130x130}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_shape_130x130"/>
                </FrameLayout>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameSpacer1"
                android:layout_height="wrap_content" android:layout_width="match_parent"/>
            <LinearLayout android:id="@+id/linearCallOption"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_24pxv"
                android:paddingEnd="@dimen/_72pxh"
                android:paddingStart="@dimen/_72pxh"
                android:paddingTop="@dimen/_24pxv" style="@style/groupStylenamegray_6">
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearLayout"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_name_white_a700_radius_25"
                        android:id="@+id/btnSettings"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_50pxh"
                        android:layout_width="@dimen/_50pxh"
                        android:paddingHorizontal="@dimen/_15pxh"
                        android:paddingVertical="@dimen/_15pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_settings_blue_gray_800_01_50x50" tools:ignore="ContentDescription"/>
                    <ImageButton
                        android:background="@drawable/rectangle_bg_name_white_a700_radius_25"
                        android:id="@+id/btnMusic"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_50pxh"
                        android:layout_width="@dimen/_50pxh"
                        android:paddingHorizontal="@dimen/_15pxh"
                        android:paddingVertical="@dimen/_15pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_music" tools:ignore="ContentDescription"/>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:background="@drawable/rectangle_bg_name_red_a200_radius_35"
                    android:ellipsize="end"
                    android:fontFamily="@font/raleway"
                    android:gravity="center"
                    android:id="@+id/btnEndCall"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_70pxv"
                    android:layout_width="@dimen/_230pxh"
                    android:paddingBottom="@dimen/_25pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_25pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_end_call"
                    android:textAllCaps="false"
                    android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/lbl_end_call"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
