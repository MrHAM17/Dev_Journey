<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="datalistRowModel" type="com.housitbuyrentsellproperty.app.modules.realestateslistbycategory.data.model.DatalistRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearEstatesCardHorizontal"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="horizontal"
        android:paddingHorizontal="@dimen/_8pxh"
        android:paddingVertical="@dimen/_8pxv" style="@style/groupStyle{&quot;name&quot;:&quot;gray_100&quot;}cornerRadius">
        <FrameLayout android:id="@+id/frameShape"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_140pxv" android:layout_width="@dimen/_126pxh">
            <ImageView android:id="@+id/imageVilla1"
                android:layout_gravity="center"
                android:layout_height="@dimen/_140pxh"
                android:layout_width="@dimen/_126pxh"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_18pxh}"
                imageRes="@{@drawable/img_shape_140x126}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_shape_140x126"/>
            <LinearLayout android:id="@+id/linearFavorite"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <ImageButton
                    android:background="@drawable/rectangle_bg_name_red_a200_radius_12"
                    android:id="@+id/btnFavorite"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_width="@dimen/_25pxh"
                    android:paddingHorizontal="@dimen/_6pxh"
                    android:paddingVertical="@dimen/_6pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_favorite" tools:ignore="ContentDescription"/>
                <LinearLayout android:id="@+id/linearLayout3"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_4pxv"
                    android:paddingTop="@dimen/_4pxv" style="@style/groupStylenameblue_gray_700_1">
                    <ImageView android:id="@+id/imageVilla2"
                        android:layout_gravity="top"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_width="@dimen/_11pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_close"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtVilla"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{datalistRowModel.txtVilla}"
                        style="@style/txtRalewaymedium8_1" tools:text="@string/lbl_villa"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearTheLaurelsVilla"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:id="@+id/txtTheLaurelsVilla"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="@{datalistRowModel.txtTheLaurelsVilla}"
                style="@style/txtRalewaysemibold14_1" tools:text="@string/msg_the_laurels_villa"/>
            <LinearLayout android:id="@+id/linearLayout4"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageImage"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_9pxh"
                    android:layout_width="@dimen/_9pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_signal_orange_300"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_signal_orange_300"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtText"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{datalistRowModel.txtText}"
                    style="@style/txtRalewaybold8" tools:text="@string/lbl_4_92"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearLayout"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageBaliIndonesia"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_9pxh"
                    android:layout_width="@dimen/_9pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_linkedin"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_linkedin"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:id="@+id/txtBaliIndonesia"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{datalistRowModel.txtBaliIndonesia}"
                    style="@style/txtRalewaymedium8" tools:text="@string/lbl_bali_indonesia"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearLayout1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:id="@+id/txtPrice"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{datalistRowModel.txtPrice}"
                    style="@style/txtRalewaysemibold16" tools:text="@string/lbl_320"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMonth"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{datalistRowModel.txtMonth}"
                    style="@style/txtRalewaymedium8_2" tools:text="@string/lbl_night"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
