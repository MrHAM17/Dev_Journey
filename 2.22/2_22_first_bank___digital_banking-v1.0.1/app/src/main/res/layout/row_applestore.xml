<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="applestoreRowModel" type="com.firstbankdigitalbanking.app.modules.investments.data.model.ApplestoreRowModel"/>
    </data>
    <LinearLayout android:gravity="center_vertical"
        android:id="@+id/linearAppleStore1"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:paddingHorizontal="@dimen/_12pxh"
        android:paddingVertical="@dimen/_12pxv" style="@style/groupStylenameonPrimaryopacity1cornerRadius_1">
        <ImageView android:id="@+id/imageImage"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_45pxh"
            android:layout_width="@dimen/_45pxh"
            android:scaleType="fitXY" android:src="@drawable/img_close"
            tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearAppleStore2"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtAppleStore"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{applestoreRowModel.txtAppleStore}"
                style="@style/txtIntermedium14_2" tools:text="@string/lbl_apple_store"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtEcommerceMarketplace"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{applestoreRowModel.txtEcommerceMarketplace}"
                style="@style/txtInterregular12" tools:text="@string/msg_e_commerce_marketplace"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameSpacer"
            android:layout_height="wrap_content" android:layout_width="wrap_content"/>
        <TextView android:ellipsize="end" android:id="@+id/txtSixteen"
            android:layout_gravity="center_vertical"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{applestoreRowModel.txtSixteen}"
            style="@style/txtIntersemibold14_2" tools:text="@string/lbl_16"/>
    </LinearLayout>
</layout>
