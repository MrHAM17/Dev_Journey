<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/{&quot;name&quot;:&quot;gray_100&quot;}"/>
            <corners android:radius="@dimen/_22pxh"/>
        </shape>
    </item>
    <item android:state_selected="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/{&quot;name&quot;:&quot;onPrimaryContainer&quot;,&quot;opacity&quot;:1}"/>
            <corners android:radius="@dimen/_22pxh"/>
            <stroke
                android:color="@color/{&quot;name&quot;:&quot;gray_300&quot;}" android:width="@dimen/_1pxh"/>
        </shape>
    </item>
</selector>
