<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="frameRowModel" type="com.hiredjobsearch.app.modules.home.data.model.FrameRowModel"/>
    </data>
    <LinearLayout android:gravity="center_vertical"
        android:id="@+id/linearBag" android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="horizontal"
        android:paddingHorizontal="@dimen/_16pxh"
        android:paddingVertical="@dimen/_16pxv" style="@style/groupStyle{&quot;name&quot;:&quot;primary&quot;}cornerRadius">
        <ImageButton
            android:background="@drawable/rectangle_bg_name_onprimarycontainer_opacity_1_radius_8"
            android:id="@+id/btnBag"
            android:layout_height="@dimen/_48pxh"
            android:layout_width="@dimen/_48pxh"
            android:paddingHorizontal="@dimen/_8pxh"
            android:paddingVertical="@dimen/_8pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_bag" tools:ignore="ContentDescription"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearSeniorUIUXDesigner"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtSeniorUIUXDesigner"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{frameRowModel.txtSeniorUIUXDesigner}"
                style="@style/txtPlusJakartaSansbold14_1" tools:text="@string/msg_senior_ui_ux_designer"/>
            <TextView android:alpha="0.8" android:ellipsize="end"
                android:id="@+id/txtShopee"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{frameRowModel.txtShopee}"
                style="@style/txtPlusJakartaSanssemibold12" tools:text="@string/lbl_shopee"/>
            <TextView android:alpha="0.64" android:ellipsize="end"
                android:id="@+id/txtJakartaIndonesia"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{frameRowModel.txtJakartaIndonesia}"
                style="@style/txtPlusJakartaSansmedium12_1" tools:text="@string/msg_jakarta_indonesia"/>
            <TextView android:ellipsize="end" android:id="@+id/txtPrice"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{frameRowModel.txtPrice}"
                style="@style/txtPlusJakartaSansmedium12_2" tools:text="@string/msg_1100_12_000_month"/>
            <FrameLayout android:id="@+id/frameFulltime1"
                android:layout_height="wrap_content" android:layout_width="wrap_content">
                <FrameLayout
                    android:background="@drawable/rectangle_bg_name_indigo_900_radius_14"
                    android:id="@+id/frameFulltime2"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_12pxh"
                    android:paddingStart="@dimen/_12pxh" android:paddingTop="@dimen/_6pxv"/>
                <FrameLayout
                    android:background="@drawable/rectangle_bg_name_indigo_900_radius_14"
                    android:id="@+id/frameTwodaysago"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_12pxh"
                    android:paddingStart="@dimen/_12pxh" android:paddingTop="@dimen/_6pxv"/>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
