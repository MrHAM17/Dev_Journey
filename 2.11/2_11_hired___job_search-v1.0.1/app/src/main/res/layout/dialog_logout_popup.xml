<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="logoutPopupVM" type="com.hiredjobsearch.app.modules.logoutpopup.data.viewmodel.LogoutPopupVM"/>
    </data>
    <LinearLayout android:id="@+id/linearLogOut"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollView"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearPopUp"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:paddingHorizontal="@dimen/_32pxh"
                android:paddingVertical="@dimen/_32pxv" style="@style/groupStylenameonPrimaryContaineropacity1cornerRadius_8">
                <ImageView android:id="@+id/imageProfile"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_82pxh"
                    android:layout_width="@dimen/_82pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_profile_onprimary"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_profile_onprimary"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtConfirmation"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:singleLine="true"
                    android:text="@{logoutPopupVM.logoutPopupModel.txtConfirmation}"
                    style="@style/txtPlusJakartaSansbold18" tools:text="@string/lbl_are_you_sure"/>
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtTimeZone"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:text="@{logoutPopupVM.logoutPopupModel.txtTimeZone}"
                    style="@style/txtPlusJakartaSanssemibold14_4" tools:text="@string/msg_ullamcorper_imperdiet"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearButton"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_border_name_primary_radius_20"
                        android:ellipsize="end"
                        android:fontFamily="@font/plus_jakarta_sans"
                        android:gravity="center"
                        android:id="@+id/btnLogOut"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_46pxv"
                        android:layout_width="@dimen/_115pxh"
                        android:letterSpacing="0.0"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:paddingBottom="@dimen/_14pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_14pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_log_out2"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                        android:textFontWeight="600"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:text="@string/lbl_log_out2"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_primary_radius_20"
                        android:ellipsize="end"
                        android:fontFamily="@font/plus_jakarta_sans"
                        android:gravity="center"
                        android:id="@+id/btnCancel"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_46pxv"
                        android:layout_width="@dimen/_115pxh"
                        android:letterSpacing="0.0"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:paddingBottom="@dimen/_14pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_14pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_cancel"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;onPrimaryContainer&quot;,&quot;opacity&quot;:1}"
                        android:textFontWeight="600"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:text="@string/lbl_cancel"/>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
