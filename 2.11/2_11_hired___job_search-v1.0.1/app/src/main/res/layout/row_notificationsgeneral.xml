<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="notificationsgeneralRowModel" type="com.hiredjobsearch.app.modules.notificationsgeneral.data.model.NotificationsgeneralRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearBag1"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearBag2"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageButton
                android:background="@drawable/rectangle_bg_name_gray_100_radius_16"
                android:id="@+id/btnBag"
                android:layout_height="@dimen/_32pxh"
                android:layout_width="@dimen/_32pxh"
                android:paddingHorizontal="@dimen/_4pxh"
                android:paddingVertical="@dimen/_4pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_bag" tools:ignore="ContentDescription"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearJuniorUIDesigner"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtJuniorUIDesigner"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{notificationsgeneralRowModel.txtJuniorUIDesigner}"
                    style="@style/txtPlusJakartaSansbold14" tools:text="@string/msg_junior_ui_designer"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtShopeeSg"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{notificationsgeneralRowModel.txtShopeeSg}"
                    style="@style/txtPlusJakartaSansmedium12" tools:text="@string/lbl_shopee_sg"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameSpacer"
                android:layout_height="wrap_content" android:layout_width="wrap_content"/>
            <TextView android:ellipsize="end" android:id="@+id/txtTime"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{notificationsgeneralRowModel.txtTime}"
                style="@style/txtPlusJakartaSansmedium12_3" tools:text="@string/lbl_32_min_ago"/>
        </LinearLayout>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtLoremIpsumDolor"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:letterSpacing="0.0"
            android:lineSpacingExtra="@dimen/_8pxh"
            android:text="@{notificationsgeneralRowModel.txtLoremIpsumDolor}"
            style="@style/txtPlusJakartaSansmedium12_4" tools:text="@string/msg_lorem_ipsum_dolor7"/>
    </LinearLayout>
</layout>
