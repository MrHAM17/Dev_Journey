<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="chatVM" type="com.fashionistaecommerceapp.app.modules.chat.data.viewmodel.ChatVM"/>
    </data>
    <LinearLayout android:id="@+id/linearMessage"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageButton
                android:background="@drawable/rectangle_bg_name_gray_200_radius_20"
                android:id="@+id/btnArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrow_down" tools:ignore="ContentDescription"/>
            <LinearLayout android:id="@+id/linearGroup54"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCircleImage"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse_65}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_65"/>
                <LinearLayout android:id="@+id/linearGroup55"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtMahbuba"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{chatVM.chatModel.txtMahbuba}"
                        style="@style/txtPoppinsmedium16_1" tools:text="@string/lbl_mahbuba"/>
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtOnline"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{chatVM.chatModel.txtOnline}"
                        style="@style/txtPoppinsregular14_5" tools:text="@string/lbl_online"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearGroup56"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCall"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_call"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_call"/>
                <ImageView android:id="@+id/imageVideocam"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_videocam"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_videocam"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearChat"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_17pxv"
            android:paddingEnd="@dimen/_16pxh"
            android:paddingStart="@dimen/_16pxh" android:paddingTop="@dimen/_17pxv">
            <TextView android:ellipsize="end"
                android:id="@+id/txtYesterday"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{chatVM.chatModel.txtYesterday}"
                style="@style/txtPoppinsmedium10" tools:text="@string/lbl_yesterday"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearHiAnneIamFashionExpertHowmayIh"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCircleImage1"
                    android:layout_gravity="bottom"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse_65}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_65"/>
                <LinearLayout
                    android:id="@+id/linearHiAnneIamFashionExpertHowmayIh1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtHiAnneIamFashionExpertHowmayIh"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingEnd="@dimen/_16pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:text="@{chatVM.chatModel.txtHiAnneIamFashionExpertHowmayIh}"
                        style="@style/txtSolidRounded" tools:text="@string/msg_hi_anne_i_am_fashion"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime"
                        android:layout_gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{chatVM.chatModel.txtTime}"
                        style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_18_27"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearNinetySeven"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearTime"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_19pxv"
                    android:paddingEnd="@dimen/_32pxh"
                    android:paddingStart="@dimen/_32pxh"
                    android:paddingTop="@dimen/_19pxv" style="@style/groupStyle{&quot;name&quot;:&quot;gray_500&quot;}cornerRadius">
                    <ImageView android:id="@+id/imageImage"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_35pxh"
                        android:layout_width="@dimen/_181pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group_230"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_group_230"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime1"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{chatVM.chatModel.txtTime1}"
                        style="@style/txtPoppinsregular16_2" tools:text="@string/lbl_0_37"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageCircleImage2"
                    android:layout_gravity="bottom"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse_66}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_66"/>
            </LinearLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtTime2"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{chatVM.chatModel.txtTime2}"
                style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_18_27"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearHiAnneIamFashionExpertHowmayIh2"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCircleImage3"
                    android:layout_gravity="bottom"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse_65}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_65"/>
                <LinearLayout
                    android:id="@+id/linearHiAnneIamFashionExpertHowmayIh3"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtHiAnneIamFashionExpertHowmayIh1"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingEnd="@dimen/_16pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:text="@{chatVM.chatModel.txtHiAnneIamFashionExpertHowmayIh1}"
                        style="@style/txtSolidRounded" tools:text="@string/msg_hi_anne_i_am_fashion"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime3"
                        android:layout_gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{chatVM.chatModel.txtTime3}"
                        style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_18_27"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearNinetyTwo"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:gravity="center_vertical"
                    android:id="@+id/txtHidontworryIamhereLe"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_26pxh"
                    android:paddingStart="@dimen/_26pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:text="@{chatVM.chatModel.txtHidontworryIamhereLe}"
                    style="@style/txtSolidRounded_1" tools:text="@string/msg_hi_don_t_worry"/>
                <ImageView android:id="@+id/imageCircleImage4"
                    android:layout_gravity="bottom"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse_66}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_66"/>
            </LinearLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtTime4"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{chatVM.chatModel.txtTime4}"
                style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_18_27"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearHiAnneIamFashionExpertHowmayIh4"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCircleImage5"
                    android:layout_gravity="bottom"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse_65}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_65"/>
                <LinearLayout
                    android:id="@+id/linearHiAnneIamFashionExpertHowmayIh5"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtHiAnneIamFashionExpertHowmayIh2"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingEnd="@dimen/_16pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:text="@{chatVM.chatModel.txtHiAnneIamFashionExpertHowmayIh2}"
                        style="@style/txtSolidRounded" tools:text="@string/msg_hi_anne_i_am_fashion"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime5"
                        android:layout_gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{chatVM.chatModel.txtTime5}"
                        style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_18_27"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearMahbubaIsTyping"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCircleImage6"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse_65}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_65"/>
                <TextView android:ellipsize="end"
                    android:gravity="bottom"
                    android:id="@+id/txtMahbubaIsTyping"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{chatVM.chatModel.txtMahbubaIsTyping}"
                    style="@style/txtPoppinsregular14_2" tools:text="@string/msg_mahbuba_is_typing"/>
            </LinearLayout>
        </LinearLayout>
        <EditText
            android:background="@drawable/rectangle_bg_name_gray_200_radius_12"
            android:drawableEnd="@drawable/img_mic"
            android:drawablePadding="@dimen/_30pxh"
            android:ellipsize="end" android:fontFamily="@font/poppins"
            android:hint="@string/lbl_type_a_message"
            android:id="@+id/etMessage" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_15pxv"
            android:paddingEnd="@dimen/_27pxh"
            android:paddingStart="@dimen/_16pxh"
            android:paddingTop="@dimen/_15pxv" android:singleLine="true"
            android:text="@={chatVM.chatModel.etMessageValue}"
            android:textColor="@color/{&quot;name&quot;:&quot;gray_500&quot;}"
            android:textColorHint="@color/{&quot;name&quot;:&quot;gray_500&quot;}"
            android:textFontWeight="400"
            android:textSize="@dimen/_14pxh" android:textStyle="normal" tools:ignore="Autofill"/>
    </LinearLayout>
</layout>
