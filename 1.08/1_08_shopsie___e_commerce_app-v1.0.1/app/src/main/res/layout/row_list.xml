<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearWomenBlueSkinny"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="vertical">
        <ImageView android:id="@+id/imageImage"
            android:layout_height="@dimen/_236pxh"
            android:layout_width="@dimen/_171pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_8pxh}"
            imageRes="@{@drawable/img_image_3}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_image_3"/>
        <TextView android:id="@+id/txtWomenBlueSkinny"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:text="@string/msg_women_blue_skinny"
            style="@style/txtLatoregular12_3" tools:text="@string/msg_women_blue_skinny"/>
        <TextView android:ellipsize="end" android:id="@+id/txtPrice"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:singleLine="true" android:text="@string/lbl_13_99"
            style="@style/txtLatosemibold14" tools:text="@string/lbl_13_99"/>
    </LinearLayout>
</layout>
