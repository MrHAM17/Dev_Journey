<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearPassword"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;gray_100_01&quot;}">
        <LinearLayout android:id="@+id/linearPassword1"
            android:layout_height="match_parent"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearTwofactorAuthentication"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTwofactorAuthentication"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/msg_two_factor_authentication"
                    style="@style/txtIntermedium14_2" tools:text="@string/msg_two_factor_authentication"/>
                <LinearLayout android:id="@+id/linearEnableOrDisable"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">

                    <com.google.android.material.switchmaterial.SwitchMaterial
                        android:id="@+id/switchMaterialSwitch"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <TextView android:id="@+id/txtEnableOrDisable"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:text="@string/msg_enable_or_disable"
                        style="@style/txtInterregular12" tools:text="@string/msg_enable_or_disable"/>
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtChangePassword"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_change_password"
                style="@style/txtIntermedium14_2" tools:text="@string/lbl_change_password"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearInputField7"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCardType"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/msg_current_password"
                    style="@style/txtInterregular12" tools:text="@string/msg_current_password"/>
                <EditText
                    android:background="@drawable/rectangle_bg_name_onprimary_opacity_1_border_name_teal_50_radius_10"
                    android:ellipsize="end"
                    android:fontFamily="@font/inter"
                    android:hint="@string/lbl_charlene_123"
                    android:id="@+id/etPassword"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_15pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_900&quot;}"
                    android:textColorHint="@color/{&quot;name&quot;:&quot;blue_gray_900&quot;}"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearInputField8"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCardType1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_new_password"
                    style="@style/txtInterregular12" tools:text="@string/lbl_new_password"/>
                <EditText
                    android:background="@drawable/rectangle_bg_name_onprimary_opacity_1_border_name_teal_50_radius_10"
                    android:ellipsize="end"
                    android:fontFamily="@font/inter"
                    android:hint="@string/lbl_charlene_123"
                    android:id="@+id/etNewpassword"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_15pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_900&quot;}"
                    android:textColorHint="@color/{&quot;name&quot;:&quot;blue_gray_900&quot;}"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_12pxh"
                    android:textStyle="normal" tools:ignore="Autofill"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_primary_radius_20"
                android:ellipsize="end" android:fontFamily="@font/inter"
                android:gravity="center" android:id="@+id/btnSave"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_10pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_10pxv"
                android:singleLine="true"
                android:text="@string/lbl_save"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;onPrimary&quot;,&quot;opacity&quot;:1}"
                android:textFontWeight="500"
                android:textSize="@dimen/_16pxh"
                android:textStyle="bold" tools:text="@string/lbl_save"/>
        </LinearLayout>
    </LinearLayout>
</layout>
