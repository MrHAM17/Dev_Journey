<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearFollow"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;onErrorContainer&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_down"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTitleSection"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_jenny_wilson"
                style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_jenny_wilson"/>
            <LinearLayout android:id="@+id/linearGroup39"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageImage"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconly_curved_notification"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_curved_notification"/>
                <ImageButton android:id="@+id/btnSettings"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_settings_gray_900" tools:ignore="ContentDescription"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollView"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearProfile"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:id="@+id/imageCircleImage"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_120pxh"
                    android:layout_width="@dimen/_120pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse_120x120}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_120x120"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtJennyWilson"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_jenny_wilson2"
                    style="@style/txtUrbanistromanbold20" tools:text="@string/lbl_jenny_wilson2"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtActressSinger"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@string/msg_actress_singer"
                    style="@style/txtUrbanistromanmedium14_3" tools:text="@string/msg_actress_singer"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearFrameNineteen"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/linearAutoLayoutVertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:orientation="vertical"
                        android:paddingEnd="@dimen/_20pxh"
                        android:paddingStart="@dimen/_20pxh" style="@style/groupStylenameonErrorContainercornerRadius_6">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSixHundredSeventyNine"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@string/lbl_679"
                            style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_679"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPosts"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@string/lbl_posts"
                            style="@style/txtUrbanistromanmedium14_4" tools:text="@string/lbl_posts"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearAutoLayoutVertical1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:orientation="vertical"
                        android:paddingEnd="@dimen/_11pxh"
                        android:paddingStart="@dimen/_11pxh" style="@style/groupStylenameonErrorContainercornerRadius_6">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtDistance"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl_2_6m"
                            style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_2_6m"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtFollowers"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@string/lbl_followers"
                            style="@style/txtUrbanistromanmedium14_4" tools:text="@string/lbl_followers"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearAutoLayoutVertical2"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:orientation="vertical"
                        android:paddingEnd="@dimen/_11pxh"
                        android:paddingStart="@dimen/_11pxh" style="@style/groupStylenameonErrorContainercornerRadius_6">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSixHundredFortyEight"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@string/lbl_648"
                            style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_648"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtFollowing"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@string/lbl_following"
                            style="@style/txtUrbanistromanmedium14_4" tools:text="@string/lbl_following"/>
                    </LinearLayout>
                    <FrameLayout
                        android:id="@+id/frameAutoLayoutVertical"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_53pxv" android:layout_width="@dimen/_83pxh">
                        <LinearLayout android:id="@+id/linearTen"
                            android:layout_gravity="center"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:paddingBottom="@dimen/_1pxv"
                            android:paddingEnd="@dimen/_25pxh"
                            android:paddingStart="@dimen/_25pxh"
                            android:paddingTop="@dimen/_1pxv" style="@style/groupStylenameonErrorContainercornerRadius_6">
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtLikes"
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.01"
                                android:singleLine="true"
                                android:text="@string/lbl_likes"
                                style="@style/txtUrbanistromanmedium14_4" tools:text="@string/lbl_likes"/>
                        </LinearLayout>
                        <TextView android:ellipsize="end"
                            android:gravity="top|start"
                            android:id="@+id/txtDistance1"
                            android:layout_gravity="top|center"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl_27m"
                            style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_27m"/>
                    </FrameLayout>
                </LinearLayout>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearAutoLayoutHorizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_primary_radius_19"
                        android:drawablePadding="@dimen/_8pxh"
                        android:drawableStart="@drawable/img_search"
                        android:ellipsize="end"
                        android:fontFamily="@font/urbanist"
                        android:gravity="center"
                        android:id="@+id/btnFollow"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_38pxv"
                        android:layout_width="@dimen/_132pxh"
                        android:letterSpacing="0.01"
                        android:paddingBottom="@dimen/_9pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_9pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_follow"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;onErrorContainer&quot;}"
                        android:textFontWeight="600"
                        android:textSize="@dimen/_16pxh"
                        android:textStyle="bold" tools:text="@string/lbl_follow"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_border_name_primary_radius_19"
                        android:drawablePadding="@dimen/_8pxh"
                        android:drawableStart="@drawable/img_user_primary"
                        android:ellipsize="end"
                        android:fontFamily="@font/urbanist"
                        android:gravity="center"
                        android:id="@+id/btnMessage"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_38pxv"
                        android:layout_width="@dimen/_132pxh"
                        android:letterSpacing="0.01"
                        android:paddingBottom="@dimen/_9pxv"
                        android:paddingEnd="@dimen/_21pxh"
                        android:paddingStart="@dimen/_21pxh"
                        android:paddingTop="@dimen/_9pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_message"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                        android:textFontWeight="600"
                        android:textSize="@dimen/_16pxh"
                        android:textStyle="bold" tools:text="@string/lbl_message"/>
                    <ImageButton
                        android:background="@drawable/rectangle_bg_name_red_a200_14_radius_20"
                        android:id="@+id/btnCategoriesInstagram"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_40pxh"
                        android:layout_width="@dimen/_40pxh"
                        android:paddingHorizontal="@dimen/_8pxh"
                        android:paddingVertical="@dimen/_8pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_categories_instagram" tools:ignore="ContentDescription"/>
                    <ImageButton
                        android:background="@drawable/rectangle_bg_name_red_a200_14_radius_20"
                        android:id="@+id/btnFavorite"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_40pxh"
                        android:layout_width="@dimen/_40pxh"
                        android:paddingHorizontal="@dimen/_8pxh"
                        android:paddingVertical="@dimen/_8pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_favorite_primary" tools:ignore="ContentDescription"/>
                </LinearLayout>
                <com.google.android.material.tabs.TabLayout
                    android:gravity="start"
                    android:id="@+id/tabLayoutTabview"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:tabMode="fixed"/>
                <androidx.viewpager2.widget.ViewPager2
                    android:gravity="center_horizontal"
                    android:id="@+id/viewPagerTabBarView"
                    android:layout_height="match_parent"
                    android:layout_weight="1" android:layout_width="match_parent"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
