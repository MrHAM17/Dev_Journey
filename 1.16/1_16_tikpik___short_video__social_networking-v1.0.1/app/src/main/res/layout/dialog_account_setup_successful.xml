<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearAccountsetupsuccessful"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:id="@+id/linearThemeLightModal"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_32pxv"
            android:paddingEnd="@dimen/_38pxh"
            android:paddingStart="@dimen/_38pxh"
            android:paddingTop="@dimen/_32pxv" style="@style/groupStylenameonErrorContainercornerRadius_1">
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtModalTitle"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/msg_congratulations"
                style="@style/txtUrbanistromanbold24_2" tools:text="@string/msg_congratulations"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtLoremIpsumDolor"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01"
                android:text="@string/msg_your_account_is"
                style="@style/txtUrbanistregular16" tools:text="@string/msg_your_account_is"/>
            <ImageView android:id="@+id/imageVector"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_60pxh"
                android:layout_width="@dimen/_60pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_vector_60x60}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_60x60"/>
        </LinearLayout>
    </LinearLayout>
</layout>
