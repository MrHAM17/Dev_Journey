<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearMessage"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;onErrorContainer&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_down"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTitleSection"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_jenny_wilson3"
                style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_jenny_wilson3"/>
            <LinearLayout android:id="@+id/linearGroup44"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageRewind"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rewind_gray_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rewind_gray_900"/>
                <ImageView android:id="@+id/imageIconlyCurvedCall"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconly_curved_call"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_curved_call"/>
                <ImageButton android:id="@+id/btnSettings"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_settings_gray_900" tools:ignore="ContentDescription"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearSendMessage"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_28pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_28pxv">
            <ImageView android:id="@+id/imageCircleImage"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_120pxh"
                android:layout_width="@dimen/_120pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_ellipse_120x120}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_120x120"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtJennyWilson"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_jenny_wilson2"
                style="@style/txtUrbanistromanbold20" tools:text="@string/lbl_jenny_wilson2"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtActressSinger"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@string/msg_actress_singer"
                style="@style/txtUrbanistromanmedium14_3" tools:text="@string/msg_actress_singer"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearAutoLayoutHorizontal"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_20pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_11pxv" style="@style/groupStylecornerRadius_3">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLoremIpsumDolor"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@string/msg_hi_jenny_good_morning"
                    style="@style/txtUrbanistregular16_1" tools:text="@string/msg_hi_jenny_good_morning"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTime"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@string/lbl_10_01"
                    style="@style/txtUrbanistregular12" tools:text="@string/lbl_10_01"/>
            </LinearLayout>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearAutoLayoutHorizontal1"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:paddingEnd="@dimen/_20pxh"
                android:paddingStart="@dimen/_20pxh" style="@style/groupStylecornerRadius_3">
                <TextView android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:text="@string/msg_i_m_your_fan_i"
                    style="@style/txtUrbanistregular16_1" tools:text="@string/msg_i_m_your_fan_i"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTime1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@string/lbl_10_01"
                    style="@style/txtUrbanistregular12" tools:text="@string/lbl_10_01"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearAutoLayoutHorizontal2"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <EditText
                android:background="@drawable/rectangle_bg_name_onerrorcontainer_border_name_gray_400_radius_12"
                android:ellipsize="end"
                android:fontFamily="@font/urbanist"
                android:hint="@string/lbl_message2"
                android:id="@+id/etMessage" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_19pxv"
                android:paddingEnd="@dimen/_18pxh"
                android:paddingStart="@dimen/_18pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:textColor="@color/{&quot;name&quot;:&quot;gray_500&quot;}"
                android:textColorHint="@color/{&quot;name&quot;:&quot;gray_500&quot;}"
                android:textFontWeight="400"
                android:textSize="@dimen/_14pxh"
                android:textStyle="normal" tools:ignore="Autofill"/>
            <ImageButton
                android:background="@drawable/rectangle_gradient_s_name_deep_orange_a400_e_name_orange_600_radius_28"
                android:id="@+id/btnIconButton"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_56pxh"
                android:layout_width="@dimen/_56pxh"
                android:paddingHorizontal="@dimen/_16pxh"
                android:paddingVertical="@dimen/_16pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_auto_layout_horizontal_onerrorcontainer" tools:ignore="ContentDescription"/>
        </LinearLayout>
    </LinearLayout>
</layout>
