<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearSoundUsed"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;onErrorContainer&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_down"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
            <ImageView android:id="@+id/imageCategoriesShare"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_categories_share_gray_900"
                tools:ignore="ContentDescription" tools:src="@drawable/img_categories_share_gray_900"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearPlaySong"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_20pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_20pxv">
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameMask"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_140pxv" android:layout_width="@dimen/_140pxh">
                    <ImageView android:id="@+id/imageImage"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_140pxh"
                        android:layout_width="@dimen/_140pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_24pxh}"
                        imageRes="@{@drawable/img_image}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image"/>
                    <ImageView android:id="@+id/imageOverflowMenu"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_32pxh"
                        android:layout_width="@dimen/_32pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_overflow_menu}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_overflow_menu"/>
                </FrameLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearAutoLayoutVertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:id="@+id/txtBeautifulGirlBy"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:text="@string/msg_beautiful_girl_by"
                        style="@style/txtUrbanistromanbold24" tools:text="@string/msg_beautiful_girl_by"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDistance"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/lbl_28_7m_videos"
                        style="@style/txtUrbanistromanmedium14_2" tools:text="@string/lbl_28_7m_videos"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearAutoLayoutHorizontal1"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <androidx.appcompat.widget.AppCompatButton
                    android:background="@drawable/rectangle_border_name_primary_radius_8"
                    android:drawablePadding="@dimen/_4pxh"
                    android:drawableStart="@drawable/img_overflowmenu_primary"
                    android:ellipsize="end"
                    android:fontFamily="@font/urbanist"
                    android:gravity="center"
                    android:id="@+id/btnPlaySong"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_38pxv"
                    android:layout_width="@dimen/_186pxh"
                    android:letterSpacing="0.01"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_play_song"
                    android:textAllCaps="false"
                    android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="bold" tools:text="@string/lbl_play_song"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:background="@drawable/rectangle_border_name_primary_radius_8"
                    android:drawablePadding="@dimen/_6pxh"
                    android:drawableStart="@drawable/img_bookmark_primary"
                    android:ellipsize="end"
                    android:fontFamily="@font/urbanist"
                    android:gravity="center"
                    android:id="@+id/btnAddToFavorites"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_38pxv"
                    android:layout_width="@dimen/_186pxh"
                    android:letterSpacing="0.01"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_28pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@string/msg_add_to_favorites"
                    android:textAllCaps="false"
                    android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="bold" tools:text="@string/msg_add_to_favorites"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearTypeDefaultList"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout
                    android:id="@+id/linearAutoLayoutHorizontal2"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCircleImage"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_60pxh"
                        android:layout_width="@dimen/_60pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_ellipse_1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_1"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearAutoLayoutVertical1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtName"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl_sean_kingston"
                            style="@style/txtUrbanistromanbold18_1" tools:text="@string/lbl_sean_kingston"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtInformation"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@string/msg_professional_singer"
                            style="@style/txtUrbanistromanmedium14_2" tools:text="@string/msg_professional_singer"/>
                    </LinearLayout>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:background="@drawable/rectangle_bg_name_primary_radius_8"
                    android:ellipsize="end"
                    android:fontFamily="@font/urbanist"
                    android:gravity="center" android:id="@+id/btnFollow"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_32pxv"
                    android:layout_width="@dimen/_73pxh"
                    android:letterSpacing="0.01"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_follow"
                    android:textAllCaps="false"
                    android:textColor="@color/{&quot;name&quot;:&quot;onErrorContainer&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="bold" tools:text="@string/lbl_follow"/>
            </LinearLayout>
            <View
                android:background="@color/{&quot;name&quot;:&quot;gray_200&quot;}"
                android:id="@+id/lineLine"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_380pxh"/>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerSoundUsed"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
                app:spanCount="3" itemSpace="@{@dimen/_8pxh}"
                tools:itemCount="6" tools:listitem="@layout/row_soundused"/>
        </LinearLayout>
    </LinearLayout>
</layout>
