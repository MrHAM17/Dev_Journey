<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearForgotPassword"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;onErrorContainer&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_down"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTitleSection"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_forgot_password"
                style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_forgot_password"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearContinue"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_89pxv">
            <TextView android:gravity="start"
                android:id="@+id/txtSelectWhichContact"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01"
                android:text="@string/msg_select_which_contact"
                style="@style/txtUrbanistromanmedium18_1" tools:text="@string/msg_select_which_contact"/>
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:paddingHorizontal="@dimen/_21pxh"
                android:paddingVertical="@dimen/_21pxv" style="@style/groupStylenameonErrorContainercornerRadius_2">
                <FrameLayout android:id="@+id/frameAutoLayoutHorizontal"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_80pxv"
                    android:layout_width="@dimen/_80pxh"
                    android:paddingHorizontal="@dimen/_24pxh"
                    android:paddingVertical="@dimen/_24pxv" style="@style/groupStyle{&quot;name&quot;:&quot;red_A200_14&quot;}cornerRadius">
                    <ImageView android:id="@+id/imageIconlyBoldChat"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_32pxh"
                        android:layout_width="@dimen/_32pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_iconly_bold_chat"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_bold_chat"/>
                </FrameLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearViaSMS"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtViaSMS"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/lbl_via_sms"
                        style="@style/txtUrbanistromanmedium14_5" tools:text="@string/lbl_via_sms"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtOneHundredElevenThousandOneHun"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/lbl_1_111_99"
                        style="@style/txtUrbanistromanbold16" tools:text="@string/lbl_1_111_99"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal1"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:paddingHorizontal="@dimen/_23pxh"
                android:paddingVertical="@dimen/_23pxv" style="@style/groupStylenameonErrorContainercornerRadius_3">
                <FrameLayout
                    android:id="@+id/frameAutoLayoutHorizontal1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_80pxv"
                    android:layout_width="@dimen/_80pxh"
                    android:paddingHorizontal="@dimen/_24pxh"
                    android:paddingVertical="@dimen/_24pxv" style="@style/groupStyle{&quot;name&quot;:&quot;red_A200_14&quot;}cornerRadius">
                    <ImageView android:id="@+id/imageIconlyBoldMessage"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_32pxh"
                        android:layout_width="@dimen/_32pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_iconly_bold_message"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_bold_message"/>
                </FrameLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearViaEmail"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtViaEmail"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/lbl_via_email"
                        style="@style/txtUrbanistromanmedium14_5" tools:text="@string/lbl_via_email"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAbcGGmailCom"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/msg_abc_g_gmail_com"
                        style="@style/txtUrbanistromanbold16" tools:text="@string/msg_abc_g_gmail_com"/>
                </LinearLayout>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_primary_radius_16"
                android:ellipsize="end"
                android:fontFamily="@font/urbanist"
                android:gravity="center" android:id="@+id/btnContinue"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_58pxv"
                android:layout_width="@dimen/_380pxh"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_19pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:text="@string/lbl_continue"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;onErrorContainer&quot;}"
                android:textFontWeight="700"
                android:textSize="@dimen/_16pxh"
                android:textStyle="bold" tools:text="@string/lbl_continue"/>
        </LinearLayout>
    </LinearLayout>
</layout>
