<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearGoLive"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylenameonErrorContainer_9">
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameAddSound"
            android:layout_height="@dimen/_850pxv" android:layout_width="match_parent">
            <View
                android:background="@drawable/rectangle_gradient_s_name_primarycontainer_e_name_primarycontainer_opacity_0_6"
                android:id="@+id/viewView"
                android:layout_gravity="bottom|center"
                android:layout_height="@dimen/_200pxv" android:layout_width="@dimen/_428pxh"/>
            <LinearLayout android:id="@+id/linearAutoLayoutVertical"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:gravity="center_horizontal"
                    android:id="@+id/toolbarAppBar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <ImageView android:id="@+id/imageCategoriesXClose"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_categories_x_close_onerrorcontainer"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_categories_x_close_onerrorcontainer"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_gray_800_99_radius_16"
                        android:drawablePadding="@dimen/_8pxh"
                        android:drawableStart="@drawable/img_categories_music"
                        android:ellipsize="end"
                        android:fontFamily="@font/urbanist"
                        android:gravity="center"
                        android:id="@+id/btnAddSound"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:paddingStart="@dimen/_16pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_add_sound"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;onErrorContainer&quot;}"
                        android:textFontWeight="600"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:text="@string/lbl_add_sound"/>
                    <ImageView
                        android:id="@+id/imageIconlyCurvedSetting"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_iconly_curved_setting"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_curved_setting"/>
                </androidx.appcompat.widget.Toolbar>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutVertical1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_26pxh" android:paddingStart="@dimen/_26pxh">
                    <ImageView android:id="@+id/imageCategoriesRepost"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_categories_repost"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_categories_repost"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFlip"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/lbl_flip"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_flip"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutVertical2"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_26pxh" android:paddingStart="@dimen/_26pxh">
                    <ImageView android:id="@+id/imageCategoriesFilter"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_categories_filter"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_categories_filter"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFilter"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/lbl_filter"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_filter"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutVertical3"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_24pxh" android:paddingStart="@dimen/_24pxh">
                    <ImageView android:id="@+id/imageUser"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user_onerrorcontainer_36x36"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user_onerrorcontainer_36x36"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEffects"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/lbl_effects"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_effects"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutVertical4"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_26pxh" android:paddingStart="@dimen/_26pxh">
                    <ImageView android:id="@+id/imageCategoriesTimer"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_categories_timer"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_categories_timer"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTimer"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/lbl_timer"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_timer"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutVertical5"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_24pxh" android:paddingStart="@dimen/_24pxh">
                    <ImageView android:id="@+id/imageUser1"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtComm"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/lbl_comm"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_comm"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearAutoLayoutVertical6"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_26pxh" android:paddingStart="@dimen/_26pxh">
                    <ImageView android:id="@+id/imageCategoriesShare"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_categories_share"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_categories_share"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtShare"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/lbl_share"
                        style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_share"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameSpacer"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:background="@drawable/rectangle_bg_name_primary_radius_16"
                    android:ellipsize="end"
                    android:fontFamily="@font/urbanist"
                    android:gravity="center"
                    android:id="@+id/btnGoLIVENow"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:paddingBottom="@dimen/_19pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_19pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_go_live_now"
                    android:textAllCaps="false"
                    android:textColor="@color/{&quot;name&quot;:&quot;onErrorContainer&quot;}"
                    android:textFontWeight="700"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/lbl_go_live_now"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
