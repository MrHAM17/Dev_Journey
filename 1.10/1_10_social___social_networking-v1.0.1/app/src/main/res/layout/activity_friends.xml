<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearFollow"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;primary&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowBack"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_back_deep_purple_a200"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_back_deep_purple_a200"/>
            <ImageView android:id="@+id/imagePersonAddAltOne"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_person_add_alt_1"
                tools:ignore="ContentDescription" tools:src="@drawable/img_person_add_alt_1"/>
        </androidx.appcompat.widget.Toolbar>
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollView"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearFriends"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFriends"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_friends"
                    style="@style/txtInterbold32" tools:text="@string/lbl_friends"/>
                <androidx.appcompat.widget.SearchView
                    android:background="@drawable/rectangle_bg_name_gray_50_radius_20"
                    android:gravity="center_horizontal"
                    android:id="@+id/searchViewSearch"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:theme="@style/searchViewsearch"
                    app:iconifiedByDefault="false"
                    app:queryBackground="@android:color/transparent"
                    app:queryHint="@string/lbl_search" app:searchIcon="@drawable/img_rewind_blue_gray_400"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearSocials"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingEnd="@dimen/_16pxh" android:paddingStart="@dimen/_16pxh">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtConnectToYour"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@string/msg_connect_to_your"
                        style="@style/txtInterbold20" tools:text="@string/msg_connect_to_your"/>
                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/recyclerEightyThree"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:nestedScrollingEnabled="false"
                        android:orientation="horizontal"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        itemSpace="@{@dimen/_30pxh}" tools:itemCount="4" tools:listitem="@layout/row_eightythree"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearFollow1"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtRecommeded"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_recommeded"
                        style="@style/txtInterbold20" tools:text="@string/lbl_recommeded"/>
                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/recyclerFollow"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:nestedScrollingEnabled="false"
                        android:orientation="vertical"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        itemSpace="@{@dimen/_16pxh}" tools:itemCount="6" tools:listitem="@layout/row_follow"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearProfileItem"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCircleImage"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_50pxh"
                        android:layout_width="@dimen/_50pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_ellipse_11}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_11"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRosalia"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtRosalia"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@string/lbl_rosalia"
                            style="@style/txtIntersemibold20_1" tools:text="@string/lbl_rosalia"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtYourEFriendsOn"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/msg_your_e_friends_on"
                            style="@style/txtInterregular14_5" tools:text="@string/msg_your_e_friends_on"/>
                    </LinearLayout>
                    <FrameLayout android:id="@+id/frameSpacer"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_border_name_deep_purple_a200_radius_16"
                        android:ellipsize="end"
                        android:fontFamily="@font/inter"
                        android:gravity="center"
                        android:id="@+id/btnFollow"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_33pxv"
                        android:layout_width="@dimen/_76pxh"
                        android:paddingBottom="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_16pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_8pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_follow"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;deep_purple_A200&quot;}"
                        android:textFontWeight="600"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:text="@string/lbl_follow"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>
</layout>
