<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="center_vertical"
        android:id="@+id/linearSongCard"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <LinearLayout android:id="@+id/linearSongTitle"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageImage1"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_80pxh"
                android:layout_width="@dimen/_80pxh"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_20pxh}"
                imageRes="@{@drawable/img_image_61}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_image_61"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearAutoLayoutVertical"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSongTitle"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/msg_starboy_speed_up"
                    style="@style/txtUrbanistromanbold18" tools:text="@string/msg_starboy_speed_up"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDetails"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@string/lbl_just_lowkey"
                    style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_just_lowkey"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameSpacer"
            android:layout_height="wrap_content" android:layout_width="wrap_content"/>
        <ImageButton android:id="@+id/btnOverflowMenu"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_32pxh"
            android:layout_width="@dimen/_32pxh"
            android:paddingHorizontal="@dimen/_2pxh"
            android:paddingVertical="@dimen/_2pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_overflow_menu_onprimarycontainer" tools:ignore="ContentDescription"/>
        <ImageView android:id="@+id/imageImage2"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_20pxh"
            android:layout_width="@dimen/_20pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_categories_more"
            tools:ignore="ContentDescription" tools:src="@drawable/img_categories_more"/>
    </LinearLayout>
</layout>
