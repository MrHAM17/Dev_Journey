<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearAutoLayoutVertical"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingHorizontal="@dimen/_24pxh"
        android:paddingVertical="@dimen/_24pxv" style="@style/groupStylecornerRadius_8">
        <ImageView android:id="@+id/imageImage3"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_44pxh"
            android:layout_width="@dimen/_50pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_user_white_a700"
            tools:ignore="ContentDescription" tools:src="@drawable/img_user_white_a700"/>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearAutoLayoutHorizontal3"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:gravity="center_vertical"
                android:id="@+id/txtPrice"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_9_99"
                style="@style/txtUrbanistromanbold32_2" tools:text="@string/lbl_9_99"/>
            <TextView android:ellipsize="end" android:id="@+id/txtMonth"
                android:layout_gravity="center_vertical"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@string/lbl_month"
                style="@style/txtUrbanistromanmedium18_4" tools:text="@string/lbl_month"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearAutoLayoutHorizontal4"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageImage4"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_32pxh"
                android:layout_width="@dimen/_32pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_categories_check"
                tools:ignore="ContentDescription" tools:src="@drawable/img_categories_check"/>
            <TextView android:ellipsize="end" android:gravity="bottom"
                android:id="@+id/txtListeningWithBetter"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@string/msg_listening_with_better"
                style="@style/txtUrbanistromanmedium16_2" tools:text="@string/msg_listening_with_better"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearAutoLayoutHorizontal"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageImage"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_32pxh"
                android:layout_width="@dimen/_32pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_categories_check"
                tools:ignore="ContentDescription" tools:src="@drawable/img_categories_check"/>
            <TextView android:ellipsize="end" android:gravity="bottom"
                android:id="@+id/txtListeningWithout"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@string/msg_listening_without"
                style="@style/txtUrbanistromanmedium16_2" tools:text="@string/msg_listening_without"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearAutoLayoutHorizontal1"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageImage1"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_32pxh"
                android:layout_width="@dimen/_32pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_categories_check"
                tools:ignore="ContentDescription" tools:src="@drawable/img_categories_check"/>
            <TextView android:ellipsize="end" android:gravity="bottom"
                android:id="@+id/txtShufflePlayDownload"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@string/msg_shuffle_play_download"
                style="@style/txtUrbanistromanmedium16_2" tools:text="@string/msg_shuffle_play_download"/>
        </LinearLayout>
    </LinearLayout>
</layout>
