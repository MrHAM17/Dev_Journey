<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearName"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_down"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFillYourProfile"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_add_new_card"
                style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_add_new_card"/>
            <ImageView android:id="@+id/imageIconlyLightScan"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_iconly_light_scan"
                tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_light_scan"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearAddNewCard"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_31pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_31pxv">
            <LinearLayout android:id="@+id/linearFrame"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingHorizontal="@dimen/_30pxh"
                android:paddingVertical="@dimen/_30pxv" style="@style/groupStylecornerRadius_10">
                <LinearLayout android:id="@+id/linearMocard"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMocard"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/lbl_mocard"
                        style="@style/txtUrbanistromanbold16_3" tools:text="@string/lbl_mocard"/>
                    <ImageView android:id="@+id/imageSettings"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_width="@dimen/_60pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_settings_white_a700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_settings_white_a700"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:gravity="start" android:id="@+id/txtText"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true" android:text="@string/msg"
                    style="@style/txtUrbanistromanbold48_1" tools:text="@string/msg"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearCardHolderName"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearAutoLayoutVertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtCardHolderName"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@string/msg_card_holder_name"
                            style="@style/txtUrbanistromanmedium10" tools:text="@string/msg_card_holder_name"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtText1"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@string/lbl2"
                            style="@style/txtUrbanistsemibold14_3" tools:text="@string/lbl2"/>
                    </LinearLayout>
                    <FrameLayout android:id="@+id/frameSpacer"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearAutoLayoutVertical1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtExpiryDate"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@string/lbl_expiry_date"
                            style="@style/txtUrbanistromanmedium10" tools:text="@string/lbl_expiry_date"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtText2"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@string/lbl3"
                            style="@style/txtUrbanistsemibold14_3" tools:text="@string/lbl3"/>
                    </LinearLayout>
                    <FrameLayout android:id="@+id/frameSpacer1"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <ImageView android:id="@+id/imageImage"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_58pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_group"/>
                </LinearLayout>
            </LinearLayout>
            <View
                android:background="@color/{&quot;name&quot;:&quot;blue_gray_100&quot;}"
                android:id="@+id/lineLine"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_380pxh"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCardName"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@string/lbl_card_name"
                style="@style/txtUrbanistromanbold18" tools:text="@string/lbl_card_name"/>
            <EditText
                android:background="@drawable/rectangle_bg_name_gray_100_radius_16"
                android:ellipsize="end"
                android:fontFamily="@font/urbanist"
                android:hint="@string/lbl_andrew_ainsley"
                android:id="@+id/etName" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_19pxv"
                android:paddingEnd="@dimen/_20pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:textColor="@color/{&quot;name&quot;:&quot;gray_900_01&quot;}"
                android:textColorHint="@color/{&quot;name&quot;:&quot;gray_900_01&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCardNumber"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@string/lbl_card_number"
                style="@style/txtUrbanistromanbold18" tools:text="@string/lbl_card_number"/>
            <EditText
                android:background="@drawable/rectangle_bg_name_gray_100_radius_16"
                android:ellipsize="end"
                android:fontFamily="@font/urbanist"
                android:hint="@string/msg_2672_4738_7837_7285"
                android:id="@+id/etCardNumber" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_19pxv"
                android:paddingEnd="@dimen/_20pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:textColor="@color/{&quot;name&quot;:&quot;gray_900_01&quot;}"
                android:textColorHint="@color/{&quot;name&quot;:&quot;gray_900_01&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:ignore="Autofill"/>
            <LinearLayout android:id="@+id/linearExpiryDate"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtExpiryDate1"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@string/lbl_expiry_date2"
                    style="@style/txtUrbanistromanbold18" tools:text="@string/lbl_expiry_date2"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCVV"
                    android:layout_gravity="top"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@string/lbl_cvv"
                    style="@style/txtUrbanistromanbold18" tools:text="@string/lbl_cvv"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearCvv"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="spaceAround"
                    android:id="@+id/linearInputFields"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_18pxv"
                    android:paddingTop="@dimen/_18pxv" style="@style/groupStyle{&quot;name&quot;:&quot;gray_100&quot;}cornerRadius">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDate"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/lbl_10_07_2026"
                        style="@style/txtUrbanistsemibold14" tools:text="@string/lbl_10_07_2026"/>
                    <ImageView android:id="@+id/imageIcons"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_icons_primary"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_icons_primary"/>
                </LinearLayout>
                <EditText
                    android:background="@drawable/rectangle_bg_name_gray_100_radius_16"
                    android:ellipsize="end"
                    android:fontFamily="@font/urbanist"
                    android:hint="@string/lbl_699"
                    android:id="@+id/etCvv" android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:paddingBottom="@dimen/_19pxv"
                    android:paddingEnd="@dimen/_20pxh"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_19pxv"
                    android:singleLine="true"
                    android:textColor="@color/{&quot;name&quot;:&quot;gray_900_01&quot;}"
                    android:textColorHint="@color/{&quot;name&quot;:&quot;gray_900_01&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_14pxh"
                    android:textStyle="bold" tools:ignore="Autofill"/>
            </LinearLayout>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:background="@drawable/rectangle_bg_name_primary_radius_29"
            android:ellipsize="end" android:fontFamily="@font/urbanist"
            android:gravity="center" android:id="@+id/btnAdd"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_58pxv"
            android:layout_width="@dimen/_380pxh"
            android:letterSpacing="0.01"
            android:paddingBottom="@dimen/_19pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_19pxv" android:singleLine="true"
            android:text="@string/lbl_add" android:textAllCaps="false"
            android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
            android:textFontWeight="700"
            android:textSize="@dimen/_16pxh" android:textStyle="bold" tools:text="@string/lbl_add"/>
    </LinearLayout>
</layout>
