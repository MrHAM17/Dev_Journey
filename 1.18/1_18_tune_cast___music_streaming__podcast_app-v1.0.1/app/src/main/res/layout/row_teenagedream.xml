<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="center_vertical"
        android:id="@+id/linearTeenageDream"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageTeenageDream1"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_80pxh"
            android:layout_width="@dimen/_80pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_20pxh}"
            imageRes="@{@drawable/img_image_53}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_image_53"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearAutoLayoutVertical"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtSongTitle"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_teenage_dream"
                style="@style/txtUrbanistromanbold18" tools:text="@string/lbl_teenage_dream"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDetails"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@string/lbl_katy_perry"
                style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_katy_perry"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameSpacer"
            android:layout_height="wrap_content" android:layout_width="wrap_content"/>
        <ImageButton android:id="@+id/btnOverflowMenu"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_32pxh"
            android:layout_width="@dimen/_32pxh"
            android:paddingHorizontal="@dimen/_2pxh"
            android:paddingVertical="@dimen/_2pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_overflow_menu_onprimarycontainer" tools:ignore="ContentDescription"/>
        <ImageView android:id="@+id/imageTeenageDream2"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_13pxh"
            android:layout_width="@dimen/_3pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_vector_gray_900_01"
            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_900_01"/>
    </LinearLayout>
</layout>
