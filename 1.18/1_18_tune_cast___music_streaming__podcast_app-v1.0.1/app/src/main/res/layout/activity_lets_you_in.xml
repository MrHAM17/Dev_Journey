<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearLetsYouIn"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageImage"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_iconly_light_outline_arrow"
                tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_light_outline_arrow"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearContinueWithFacebook"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_47pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_47pxv">
            <ImageView android:id="@+id/imageImage1"
                android:layout_gravity="end"
                android:layout_height="@dimen/_200pxh"
                android:layout_width="@dimen/_236pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_group_318"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group_318"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtLetSYouIn"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_let_s_you_in"
                style="@style/txtUrbanistromanbold48" tools:text="@string/lbl_let_s_you_in"/>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_white_a700_border_name_blue_gray_100_radius_16"
                android:drawablePadding="@dimen/_12pxh"
                android:ellipsize="end"
                android:fontFamily="@font/urbanist"
                android:gravity="center"
                android:id="@+id/btnContinueWithFacebook"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_60pxv"
                android:layout_width="@dimen/_380pxh"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_20pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_20pxv"
                android:singleLine="true"
                android:text="@string/msg_continue_with_facebook"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;gray_900_01&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_16pxh"
                android:textStyle="bold" tools:text="@string/msg_continue_with_facebook"/>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_white_a700_border_name_blue_gray_100_radius_16"
                android:drawablePadding="@dimen/_12pxh"
                android:drawableStart="@drawable/img_google"
                android:ellipsize="end"
                android:fontFamily="@font/urbanist"
                android:gravity="center"
                android:id="@+id/btnContinueWithGoogle"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_60pxv"
                android:layout_width="@dimen/_380pxh"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_20pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_20pxv"
                android:singleLine="true"
                android:text="@string/msg_continue_with_google"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;gray_900_01&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_16pxh"
                android:textStyle="bold" tools:text="@string/msg_continue_with_google"/>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_white_a700_border_name_blue_gray_100_radius_16"
                android:drawablePadding="@dimen/_12pxh"
                android:drawableStart="@drawable/img_icons"
                android:ellipsize="end"
                android:fontFamily="@font/urbanist"
                android:gravity="center"
                android:id="@+id/btnContinueWithApple"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_60pxv"
                android:layout_width="@dimen/_380pxh"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_20pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_20pxv"
                android:singleLine="true"
                android:text="@string/msg_continue_with_apple"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;gray_900_01&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_16pxh"
                android:textStyle="bold" tools:text="@string/msg_continue_with_apple"/>
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <View
                    android:background="@color/{&quot;name&quot;:&quot;blue_gray_100&quot;}"
                    android:id="@+id/lineLine"
                    android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_155pxh"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOr"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@string/lbl_or"
                    style="@style/txtUrbanistsemibold18_2" tools:text="@string/lbl_or"/>
                <View
                    android:background="@color/{&quot;name&quot;:&quot;blue_gray_100&quot;}"
                    android:id="@+id/lineLine1"
                    android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_155pxh"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_primary_radius_29"
                android:ellipsize="end"
                android:fontFamily="@font/urbanist"
                android:gravity="center"
                android:id="@+id/btnSignInWithPassword"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_58pxv"
                android:layout_width="@dimen/_380pxh"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_19pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:text="@string/msg_sign_in_with_password"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                android:textFontWeight="700"
                android:textSize="@dimen/_16pxh"
                android:textStyle="bold" tools:text="@string/msg_sign_in_with_password"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearThree"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end" android:gravity="top"
                    android:id="@+id/txtConfirmation"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@string/msg_don_t_have_an_account"
                    style="@style/txtUrbanistregular14" tools:text="@string/msg_don_t_have_an_account"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSignUp"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@string/lbl_sign_up"
                    style="@style/txtUrbanistsemibold14_1" tools:text="@string/lbl_sign_up"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
