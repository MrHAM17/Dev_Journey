<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearPlay"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_down"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
            <ImageButton android:id="@+id/btnSettings"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_settings_gray_900_01" tools:ignore="ContentDescription"/>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollView"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearPodcastEpisode"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/linearAutoLayoutHorizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageImage"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_100pxh"
                        android:layout_width="@dimen/_100pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_20pxh}"
                        imageRes="@{@drawable/img_image_54}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image_54"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearAutoLayoutVertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAuthorName"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/msg_the_jordan_harbinger2"
                            style="@style/txtUrbanistromanbold18" tools:text="@string/msg_the_jordan_harbinger2"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAuthorName1"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@string/msg_jordan_harbinger"
                            style="@style/txtUrbanistromanmedium12" tools:text="@string/msg_jordan_harbinger"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearAutoLayoutHorizontal1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDuration"
                        android:layout_gravity="bottom"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/lbl_1_day_ago"
                        style="@style/txtUrbanistromanmedium14_1" tools:text="@string/lbl_1_day_ago"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtText"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/lbl"
                        style="@style/txtUrbanistromanmedium14_1" tools:text="@string/lbl"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime"
                        android:layout_gravity="top"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/lbl_48_26_mins"
                        style="@style/txtUrbanistromanmedium14_1" tools:text="@string/lbl_48_26_mins"/>
                </LinearLayout>
                <TextView android:id="@+id/txtPodcastTitleHere"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:text="@string/msg_691_shaquille_o_neal"
                    style="@style/txtUrbanistromanbold24" tools:text="@string/msg_691_shaquille_o_neal"/>
                <LinearLayout
                    android:id="@+id/linearAutoLayoutHorizontal2"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearPlay1"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <androidx.appcompat.widget.AppCompatButton
                            android:background="@drawable/rectangle_bg_name_primary_radius_21"
                            android:drawablePadding="@dimen/_8pxh"
                            android:drawableStart="@drawable/img_categoriesplay_componentadditional_icons"
                            android:ellipsize="end"
                            android:fontFamily="@font/urbanist"
                            android:gravity="center"
                            android:id="@+id/btnPlay"
                            android:layout_height="@dimen/_42pxv"
                            android:layout_width="@dimen/_107pxh"
                            android:letterSpacing="0.01"
                            android:paddingBottom="@dimen/_11pxv"
                            android:paddingEnd="@dimen/_24pxh"
                            android:paddingStart="@dimen/_24pxh"
                            android:paddingTop="@dimen/_11pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_play"
                            android:textAllCaps="false"
                            android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                            android:textFontWeight="600"
                            android:textSize="@dimen/_16pxh"
                            android:textStyle="bold" tools:text="@string/lbl_play"/>
                        <ImageView android:id="@+id/imageFavorite"
                            android:layout_height="@dimen/_24pxh"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_favorite"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_favorite"/>
                        <FrameLayout android:id="@+id/frameSpacer"
                            android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                        <ImageView
                            android:id="@+id/imageCategoriesPlaylist"
                            android:layout_height="@dimen/_24pxh"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_categories_playlist"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_categories_playlist"/>
                        <FrameLayout android:id="@+id/frameSpacer1"
                            android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                        <ImageView android:id="@+id/imageLightBulb"
                            android:layout_height="@dimen/_24pxh"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_light_bulb"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_light_bulb"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageCategoriesShare"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_categories_share"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_categories_share"/>
                    <ImageView android:id="@+id/imageCategoriesMore"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_categories_more"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_categories_more"/>
                </LinearLayout>
                <TextView android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:text="@string/msg_shaquille_o_neal"
                    style="@style/txtUrbanistromanmedium16_1" tools:text="@string/msg_shaquille_o_neal"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtWhatWeDiscuss"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@string/msg_what_we_discuss"
                    style="@style/txtUrbanistromanbold16_2" tools:text="@string/msg_what_we_discuss"/>
                <LinearLayout android:id="@+id/linearView"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <View
                        android:background="@drawable/rectangle_bg_name_primary_radius_2"
                        android:id="@+id/viewView"
                        android:layout_height="@dimen/_4pxh" android:layout_width="@dimen/_4pxh"/>
                    <TextView android:id="@+id/txtTherealworldexperien"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:text="@string/msg_the_real_world_experience"
                        style="@style/txtUrbanistromanmedium16_1" tools:text="@string/msg_the_real_world_experience"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearView1"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <View
                        android:background="@drawable/rectangle_bg_name_primary_radius_2"
                        android:id="@+id/viewView1"
                        android:layout_height="@dimen/_4pxh" android:layout_width="@dimen/_4pxh"/>
                    <TextView android:id="@+id/txtDescription1"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:text="@string/msg_how_shaq_assembled"
                        style="@style/txtUrbanistromanmedium16_1" tools:text="@string/msg_how_shaq_assembled"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearView2"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <View
                        android:background="@drawable/rectangle_bg_name_primary_radius_2"
                        android:id="@+id/viewView2"
                        android:layout_height="@dimen/_4pxh" android:layout_width="@dimen/_4pxh"/>
                    <TextView android:id="@+id/txtWhyShaqDoesnT"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:text="@string/msg_why_shaq_doesn_t"
                        style="@style/txtUrbanistromanmedium16_1" tools:text="@string/msg_why_shaq_doesn_t"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearView3"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <View
                        android:background="@drawable/rectangle_bg_name_primary_radius_2"
                        android:id="@+id/viewView3"
                        android:layout_height="@dimen/_4pxh" android:layout_width="@dimen/_4pxh"/>
                    <TextView android:id="@+id/txtHowShaqManages"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:text="@string/msg_how_shaq_manages"
                        style="@style/txtUrbanistromanmedium16_1" tools:text="@string/msg_how_shaq_manages"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearView4"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <View
                        android:background="@drawable/rectangle_bg_name_primary_radius_2"
                        android:id="@+id/viewView4"
                        android:layout_gravity="bottom"
                        android:layout_height="@dimen/_4pxh" android:layout_width="@dimen/_4pxh"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_vertical"
                        android:id="@+id/txtAndMuchMoreView"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/msg_and_much_more_view"
                        style="@style/txtUrbanistromanmedium16_1" tools:text="@string/msg_and_much_more_view"/>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
