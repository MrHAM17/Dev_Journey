<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearFilter"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;gray_50&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStyle{&quot;name&quot;:&quot;gray_50&quot;}">
            <ImageButton
                android:background="@drawable/rectangle_bg_name_white_a700_border_name_gray_300_radius_20"
                android:id="@+id/btnArrowLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrow_left_primarycontainer" tools:ignore="ContentDescription"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNotification"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@string/lbl_recently_viewed2"
                style="@style/txtManropeextrabold18" tools:text="@string/lbl_recently_viewed2"/>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollView"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearRecentlyViews"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearFilterSort"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_border_name_gray_300_radius_5"
                        android:drawableEnd="@drawable/img_settings_24x24"
                        android:drawablePadding="@dimen/_10pxh"
                        android:ellipsize="end"
                        android:fontFamily="@font/manrope"
                        android:gravity="center"
                        android:id="@+id/btnFilter"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_40pxv"
                        android:layout_width="@dimen/_92pxh"
                        android:paddingBottom="@dimen/_10pxv"
                        android:paddingEnd="@dimen/_12pxh"
                        android:paddingStart="@dimen/_12pxh"
                        android:paddingTop="@dimen/_10pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_filter"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;primaryContainer&quot;}"
                        android:textFontWeight="600"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:text="@string/lbl_filter"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_border_name_gray_300_radius_5"
                        android:drawableEnd="@drawable/img_frame_primarycontainer"
                        android:drawablePadding="@dimen/_10pxh"
                        android:ellipsize="end"
                        android:fontFamily="@font/manrope"
                        android:gravity="center"
                        android:id="@+id/btnSort"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_40pxv"
                        android:layout_width="@dimen/_87pxh"
                        android:paddingBottom="@dimen/_10pxv"
                        android:paddingEnd="@dimen/_12pxh"
                        android:paddingStart="@dimen/_12pxh"
                        android:paddingTop="@dimen/_10pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_sort"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;primaryContainer&quot;}"
                        android:textFontWeight="600"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:text="@string/lbl_sort"/>
                    <FrameLayout android:id="@+id/frameSpacer"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHomesCounter"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_8_homes"
                        style="@style/txtManropesemibold14" tools:text="@string/lbl_8_homes"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameImg"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_180pxv" android:layout_width="@dimen/_327pxh">
                    <ImageView android:id="@+id/imageImg"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_180pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_img}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_img"/>
                    <ImageButton
                        android:background="@drawable/rectangle_bg_name_gray_50_border_name_gray_300_radius_18"
                        android:id="@+id/btnFavorite"
                        android:layout_gravity="top|end"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:paddingHorizontal="@dimen/_8pxh"
                        android:paddingVertical="@dimen/_8pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_favorite" tools:ignore="ContentDescription"/>
                </FrameLayout>
                <LinearLayout android:id="@+id/linearGoldenMeadows"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearTitleAndLocation"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtGoldenMeadows"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@string/lbl_golden_meadows"
                            style="@style/txtManropeextrabold18" tools:text="@string/lbl_golden_meadows"/>
                        <LinearLayout android:id="@+id/linearLocation"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageIconMapPin"
                                android:layout_gravity="top"
                                android:layout_height="@dimen/_14pxh"
                                android:layout_width="@dimen/_14pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_icon_map_pin"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_icon_map_pin"/>
                            <TextView android:ellipsize="end"
                                android:gravity="center_vertical"
                                android:id="@+id/txtStCelinaDelaware"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.02"
                                android:singleLine="true"
                                android:text="@string/msg_st_celina_delaware2"
                                style="@style/txtManrope12" tools:text="@string/msg_st_celina_delaware2"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearRateAndDuration"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:gravity="end"
                            android:id="@+id/txtPrice"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.02"
                            android:singleLine="true"
                            android:text="@string/lbl_price"
                            style="@style/txtManrope12" tools:text="@string/lbl_price"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtPrice1"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@string/lbl_500"
                            style="@style/txtManropeextrabold18_1" tools:text="@string/lbl_500"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearTwo"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <FrameLayout android:id="@+id/frameImg1"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_180pxv" android:layout_width="@dimen/_327pxh">
                        <ImageView android:id="@+id/imageImg1"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_180pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_10pxh}"
                            imageRes="@{@drawable/img_img_180x327}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_img_180x327"/>
                        <ImageButton
                            android:background="@drawable/rectangle_bg_name_gray_50_border_name_gray_300_radius_18"
                            android:id="@+id/btnFavorite1"
                            android:layout_gravity="top|end"
                            android:layout_height="@dimen/_36pxh"
                            android:layout_width="@dimen/_36pxh"
                            android:paddingHorizontal="@dimen/_8pxh"
                            android:paddingVertical="@dimen/_8pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_favorite" tools:ignore="ContentDescription"/>
                    </FrameLayout>
                    <LinearLayout android:id="@+id/linearGoldenMeadows1"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearTitleAndLocation1"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtGoldenMeadows1"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.01"
                                android:singleLine="true"
                                android:text="@string/lbl_golden_meadows"
                                style="@style/txtManropeextrabold18" tools:text="@string/lbl_golden_meadows"/>
                            <LinearLayout
                                android:id="@+id/linearLocation1"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content" android:orientation="horizontal">
                                <ImageView
                                    android:id="@+id/imageIconMapPin1"
                                    android:layout_gravity="top"
                                    android:layout_height="@dimen/_14pxh"
                                    android:layout_width="@dimen/_14pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_icon_map_pin"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_icon_map_pin"/>
                                <TextView android:ellipsize="end"
                                    android:gravity="center_vertical"
                                    android:id="@+id/txtStCelinaDelaware1"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:letterSpacing="0.02"
                                    android:singleLine="true"
                                    android:text="@string/msg_st_celina_delaware2"
                                    style="@style/txtManrope12" tools:text="@string/msg_st_celina_delaware2"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearRateAndDuration1"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:gravity="end"
                                android:id="@+id/txtPrice2"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.02"
                                android:singleLine="true"
                                android:text="@string/lbl_price"
                                style="@style/txtManrope12" tools:text="@string/lbl_price"/>
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtPrice3"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.01"
                                android:singleLine="true"
                                android:text="@string/lbl_500"
                                style="@style/txtManropeextrabold18_1" tools:text="@string/lbl_500"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearThree"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <FrameLayout android:id="@+id/frameImg2"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_180pxv" android:layout_width="@dimen/_327pxh">
                        <ImageView android:id="@+id/imageImg2"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_180pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_10pxh}"
                            imageRes="@{@drawable/img_img_7}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_img_7"/>
                        <ImageButton
                            android:background="@drawable/rectangle_bg_name_gray_50_border_name_gray_300_radius_18"
                            android:id="@+id/btnFavorite2"
                            android:layout_gravity="top|end"
                            android:layout_height="@dimen/_36pxh"
                            android:layout_width="@dimen/_36pxh"
                            android:paddingHorizontal="@dimen/_8pxh"
                            android:paddingVertical="@dimen/_8pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_favorite" tools:ignore="ContentDescription"/>
                    </FrameLayout>
                    <LinearLayout android:id="@+id/linearGoldenMeadows2"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearTitleAndLocation2"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtGoldenMeadows2"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.01"
                                android:singleLine="true"
                                android:text="@string/lbl_golden_meadows"
                                style="@style/txtManropeextrabold18" tools:text="@string/lbl_golden_meadows"/>
                            <LinearLayout
                                android:id="@+id/linearLocation2"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content" android:orientation="horizontal">
                                <ImageView
                                    android:id="@+id/imageIconMapPin2"
                                    android:layout_gravity="top"
                                    android:layout_height="@dimen/_14pxh"
                                    android:layout_width="@dimen/_14pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_icon_map_pin"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_icon_map_pin"/>
                                <TextView android:ellipsize="end"
                                    android:gravity="center_vertical"
                                    android:id="@+id/txtStCelinaDelaware2"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:letterSpacing="0.02"
                                    android:singleLine="true"
                                    android:text="@string/msg_st_celina_delaware2"
                                    style="@style/txtManrope12" tools:text="@string/msg_st_celina_delaware2"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearRateAndDuration2"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:gravity="end"
                                android:id="@+id/txtPrice4"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.02"
                                android:singleLine="true"
                                android:text="@string/lbl_price"
                                style="@style/txtManrope12" tools:text="@string/lbl_price"/>
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtPrice5"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.01"
                                android:singleLine="true"
                                android:text="@string/lbl_500"
                                style="@style/txtManropeextrabold18_1" tools:text="@string/lbl_500"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
