<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearProperty"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}cornerRadius">
        <ImageView android:id="@+id/imagePrimaryApartment"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_109pxh"
            android:layout_width="@dimen/_102pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_10pxh}"
            imageRes="@{@drawable/img_image}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_image"/>
        <LinearLayout android:id="@+id/linearDetails1"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtPrimaryApartment"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@string/msg_primary_apartment"
                style="@style/txtManropebold16" tools:text="@string/msg_primary_apartment"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearDetails2"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageImage1"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_icon_blue_gray_500"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_icon_blue_gray_500"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtThree"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_3"
                    style="@style/txtManropemedium14" tools:text="@string/lbl_3"/>
                <ImageView android:id="@+id/imageImage2"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_icon_blue_gray_500_16x16"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_icon_blue_gray_500_16x16"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTwo"
                    android:layout_gravity="top"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_2"
                    style="@style/txtManropemedium14" tools:text="@string/lbl_2"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearFt"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageFt"
                        android:layout_gravity="top"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_icon_16x16"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_icon_16x16"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_vertical"
                        android:id="@+id/txtFtCounter"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@string/lbl_1_880_ft"
                        style="@style/txtManropeextrabold14" tools:text="@string/lbl_1_880_ft"/>
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtPrice"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@string/lbl_1_600_1_800"
                style="@style/txtManropeextrabold16" tools:text="@string/lbl_1_600_1_800"/>
        </LinearLayout>
    </LinearLayout>
</layout>
