<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="doctorDetailVM" type="com.helthiotelehealth.app.modules.doctordetail.data.viewmodel.DoctorDetailVM"/>
    </data>
    <LinearLayout android:id="@+id/linearBookApointment"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageIconChevronLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_icon_chevron_left"
                tools:ignore="ContentDescription" tools:src="@drawable/img_icon_chevron_left"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDetails"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{doctorDetailVM.doctorDetailModel.txtDetails}"
                style="@style/txtRalewaysemibold18_1" tools:text="@string/lbl_details"/>
            <ImageView android:id="@+id/imageIconChevronLeft1"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_icon_chevron_left_onprimary_24x24"
                tools:ignore="ContentDescription" tools:src="@drawable/img_icon_chevron_left_onprimary_24x24"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearDoctorDetail"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_31pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_31pxv">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearDoctorInformation"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearDrMarcusHorizon"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageImage"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_111pxh"
                        android:layout_width="@dimen/_111pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_8pxh}"
                        imageRes="@{@drawable/img_rectangle_959}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_959"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearDrMarcusHorizon1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDrMarcusHorizon"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{doctorDetailVM.doctorDetailModel.txtDrMarcusHorizon}"
                            style="@style/txtRalewaysemibold18_1" tools:text="@string/msg_dr_marcus_horizon"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtChardiologist"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{doctorDetailVM.doctorDetailModel.txtChardiologist}"
                            style="@style/txtRalewaymedium12" tools:text="@string/lbl_chardiologist"/>
                        <LinearLayout android:id="@+id/linearSignal"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageSignal"
                                android:layout_gravity="top"
                                android:layout_height="@dimen/_16pxh"
                                android:layout_width="@dimen/_16pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_signal"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_signal"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtFortySeven"
                                android:layout_gravity="bottom"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{doctorDetailVM.doctorDetailModel.txtFortySeven}"
                                style="@style/txtRalewayromanmedium12" tools:text="@string/lbl_4_7"/>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearFifteen"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageLinkedin"
                                android:layout_gravity="top"
                                android:layout_height="@dimen/_13pxh"
                                android:layout_width="@dimen/_13pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_linkedin"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_linkedin"/>
                            <TextView android:ellipsize="end"
                                android:gravity="center_vertical"
                                android:id="@+id/txtDistance"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{doctorDetailVM.doctorDetailModel.txtDistance}"
                                style="@style/txtRalewaymedium12" tools:text="@string/lbl_800m_away"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAbout"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{doctorDetailVM.doctorDetailModel.txtAbout}"
                    style="@style/txtRalewaysemibold16_1" tools:text="@string/lbl_about"/>
                <FrameLayout android:id="@+id/frameLoremIpsumDolor"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearDateTime"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">

                <com.prolificinteractive.materialcalendarview.MaterialCalendarView
                    android:gravity="center_horizontal"
                    android:id="@+id/calendarCalendar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:mcv_selectionMode="single"/>
                <View
                    android:background="@color/{&quot;name&quot;:&quot;gray_300&quot;}"
                    android:id="@+id/lineLine"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_327pxh"/>
                <FrameLayout android:id="@+id/frameAM"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content" android:layout_width="wrap_content">
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_white_a700_border_name_gray_300_radius_15"
                        android:id="@+id/frameAM1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_11pxv"
                        android:paddingEnd="@dimen/_25pxh"
                        android:paddingStart="@dimen/_25pxh" android:paddingTop="@dimen/_11pxv"/>
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_white_a700_border_name_gray_300_radius_15"
                        android:id="@+id/frameAM2"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_11pxv"
                        android:paddingEnd="@dimen/_25pxh"
                        android:paddingStart="@dimen/_25pxh" android:paddingTop="@dimen/_11pxv"/>
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_white_a700_border_name_gray_300_radius_15"
                        android:id="@+id/frameAM3"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_11pxv"
                        android:paddingEnd="@dimen/_27pxh"
                        android:paddingStart="@dimen/_27pxh" android:paddingTop="@dimen/_11pxv"/>
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_white_a700_border_name_gray_300_radius_15"
                        android:id="@+id/framePM"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_11pxv"
                        android:paddingEnd="@dimen/_26pxh"
                        android:paddingStart="@dimen/_26pxh" android:paddingTop="@dimen/_11pxv"/>
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_primary_radius_15"
                        android:id="@+id/framePM1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_11pxv"
                        android:paddingEnd="@dimen/_25pxh"
                        android:paddingStart="@dimen/_25pxh" android:paddingTop="@dimen/_11pxv"/>
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_white_a700_border_name_gray_300_radius_15"
                        android:id="@+id/framePM2"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_11pxv"
                        android:paddingEnd="@dimen/_25pxh"
                        android:paddingStart="@dimen/_25pxh" android:paddingTop="@dimen/_11pxv"/>
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_white_a700_border_name_gray_300_radius_15"
                        android:id="@+id/framePM3"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_11pxv"
                        android:paddingEnd="@dimen/_25pxh"
                        android:paddingStart="@dimen/_25pxh" android:paddingTop="@dimen/_11pxv"/>
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_white_a700_border_name_gray_300_radius_15"
                        android:id="@+id/framePM4"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_11pxv"
                        android:paddingEnd="@dimen/_25pxh"
                        android:paddingStart="@dimen/_25pxh" android:paddingTop="@dimen/_11pxv"/>
                    <FrameLayout
                        android:background="@drawable/rectangle_bg_name_white_a700_border_name_gray_300_radius_15"
                        android:id="@+id/framePM5"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_11pxv"
                        android:paddingEnd="@dimen/_25pxh"
                        android:paddingStart="@dimen/_25pxh" android:paddingTop="@dimen/_11pxv"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearSixtyEight"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageButton
                android:background="@drawable/rectangle_bg_name_onerrorcontainer_radius_8"
                android:id="@+id/btnSettings"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_50pxh"
                android:layout_width="@dimen/_50pxh"
                android:paddingHorizontal="@dimen/_13pxh"
                android:paddingVertical="@dimen/_13pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_settings" tools:ignore="ContentDescription"/>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_primary_radius_tl_8_bl_8_br_8"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:gravity="center"
                android:id="@+id/btnBookApointment"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_50pxv"
                android:layout_width="@dimen/_261pxh"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_book_apointment"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:text="@string/lbl_book_apointment"/>
        </LinearLayout>
    </LinearLayout>
</layout>
