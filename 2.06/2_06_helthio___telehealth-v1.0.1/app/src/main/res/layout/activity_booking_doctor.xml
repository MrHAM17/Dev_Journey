<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="bookingDoctorVM" type="com.helthiotelehealth.app.modules.bookingdoctor.data.viewmodel.BookingDoctorVM"/>
    </data>
    <LinearLayout android:id="@+id/linearCheckout"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageIconChevronLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_icon_chevron_left"
                tools:ignore="ContentDescription" tools:src="@drawable/img_icon_chevron_left"/>
            <TextView android:ellipsize="end" android:id="@+id/txtLogin"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{bookingDoctorVM.bookingDoctorModel.txtLogin}"
                style="@style/txtRalewaysemibold18_1" tools:text="@string/lbl_appointment"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearBookingDoctor"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_28pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_28pxv">
            <LinearLayout android:id="@+id/linearDoctorInformation"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageImage"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_111pxh"
                    android:layout_width="@dimen/_111pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_8pxh}"
                    imageRes="@{@drawable/img_rectangle_959}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_959"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearDrMarcusHorizon"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDrMarcusHorizon"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{bookingDoctorVM.bookingDoctorModel.txtDrMarcusHorizon}"
                        style="@style/txtRalewaysemibold18_1" tools:text="@string/msg_dr_marcus_horizon"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtChardiologist"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{bookingDoctorVM.bookingDoctorModel.txtChardiologist}"
                        style="@style/txtRalewaymedium12" tools:text="@string/lbl_chardiologist"/>
                    <LinearLayout android:id="@+id/linearSignal"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageSignal"
                            android:layout_gravity="top"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_signal"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_signal"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFortySeven"
                            android:layout_gravity="bottom"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{bookingDoctorVM.bookingDoctorModel.txtFortySeven}"
                            style="@style/txtRalewayromanmedium12" tools:text="@string/lbl_4_7"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearFifteen"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageLinkedin"
                            android:layout_gravity="top"
                            android:layout_height="@dimen/_13pxh"
                            android:layout_width="@dimen/_13pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_linkedin"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_linkedin"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_vertical"
                            android:id="@+id/txtDistance"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{bookingDoctorVM.bookingDoctorModel.txtDistance}"
                            style="@style/txtRalewaymedium12" tools:text="@string/lbl_800m_away"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearDate"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearNinetyTwo"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDate"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{bookingDoctorVM.bookingDoctorModel.txtDate}"
                        style="@style/txtRalewaysemibold16_1" tools:text="@string/lbl_date"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtChange"
                        android:layout_gravity="bottom"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{bookingDoctorVM.bookingDoctorModel.txtChange}"
                        style="@style/txtRalewaymedium14_1" tools:text="@string/lbl_change"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearFifty"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_name_onerrorcontainer_radius_18"
                        android:id="@+id/btnIconButton"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_width="@dimen/_36pxh"
                        android:paddingHorizontal="@dimen/_8pxh"
                        android:paddingVertical="@dimen/_8pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_group_48" tools:ignore="ContentDescription"/>
                    <TextView android:ellipsize="end"
                        android:gravity="bottom"
                        android:id="@+id/txtPrice"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{bookingDoctorVM.bookingDoctorModel.txtPrice}"
                        style="@style/txtRalewaysemibold14_2" tools:text="@string/msg_wednesday_jun_23"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearReason"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearReason1"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:id="@+id/linearEightyFive"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtReason"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{bookingDoctorVM.bookingDoctorModel.txtReason}"
                            style="@style/txtRalewaysemibold16_1" tools:text="@string/lbl_reason"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtChange1"
                            android:layout_gravity="bottom"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{bookingDoctorVM.bookingDoctorModel.txtChange1}"
                            style="@style/txtRalewaymedium14_1" tools:text="@string/lbl_change"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearFiftyThree"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageButton
                            android:background="@drawable/rectangle_bg_name_onerrorcontainer_radius_18"
                            android:id="@+id/btnUser"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_36pxh"
                            android:layout_width="@dimen/_36pxh"
                            android:paddingHorizontal="@dimen/_8pxh"
                            android:paddingVertical="@dimen/_8pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_user_primary" tools:ignore="ContentDescription"/>
                        <TextView android:ellipsize="end"
                            android:gravity="bottom"
                            android:id="@+id/txtChestPain"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{bookingDoctorVM.bookingDoctorModel.txtChestPain}"
                            style="@style/txtRalewaysemibold14_3" tools:text="@string/lbl_chest_pain"/>
                    </LinearLayout>
                </LinearLayout>
                <View
                    android:background="@color/{&quot;name&quot;:&quot;gray_300&quot;}"
                    android:id="@+id/lineLine"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_327pxh"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearPaymentDetail"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPaymentDetail"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtPaymentDetail}"
                    style="@style/txtRalewaysemibold16_1" tools:text="@string/lbl_payment_detail"/>
                <LinearLayout android:id="@+id/linearConsultation"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtConsultation"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{bookingDoctorVM.bookingDoctorModel.txtConsultation}"
                        style="@style/txtRalewayregular14_3" tools:text="@string/lbl_consultation"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPrice1"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{bookingDoctorVM.bookingDoctorModel.txtPrice1}"
                        style="@style/txtRalewayregular14" tools:text="@string/lbl_60_00"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearAdminFee"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAdminFee"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{bookingDoctorVM.bookingDoctorModel.txtAdminFee}"
                        style="@style/txtRalewayregular14_3" tools:text="@string/lbl_admin_fee"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPrice2"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{bookingDoctorVM.bookingDoctorModel.txtPrice2}"
                        style="@style/txtRalewayregular14" tools:text="@string/lbl_01_00"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearAdditonal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAditionalDiscount"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{bookingDoctorVM.bookingDoctorModel.txtAditionalDiscount}"
                        style="@style/txtRalewayregular14_3" tools:text="@string/msg_aditional_discount"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtText"
                        android:layout_gravity="top"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{bookingDoctorVM.bookingDoctorModel.txtText}"
                        style="@style/txtRalewayromanregular14" tools:text="@string/lbl"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearTotal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTotal"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{bookingDoctorVM.bookingDoctorModel.txtTotal}"
                        style="@style/txtRalewaysemibold14_3" tools:text="@string/lbl_total"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPrice3"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{bookingDoctorVM.bookingDoctorModel.txtPrice3}"
                        style="@style/txtRalewaysemibold14_1" tools:text="@string/lbl_61_00"/>
                </LinearLayout>
            </LinearLayout>
            <View
                android:background="@color/{&quot;name&quot;:&quot;gray_300&quot;}"
                android:id="@+id/lineLine1"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_319pxh"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearPaymentMethod"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPaymentMethod"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtPaymentMethod}"
                    style="@style/txtRalewaysemibold16_1" tools:text="@string/lbl_payment_method"/>
                <LinearLayout android:id="@+id/linearCard"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_15pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_13pxv" style="@style/groupStylenamewhite_4">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtVISA"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{bookingDoctorVM.bookingDoctorModel.txtVISA}"
                        style="@style/txtInterblackitalic16" tools:text="@string/lbl_visa"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtChange2"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{bookingDoctorVM.bookingDoctorModel.txtChange2}"
                        style="@style/txtRalewaymedium12_1" tools:text="@string/lbl_change"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearNinetyEight"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearTotal1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTotal1"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtTotal1}"
                    style="@style/txtRalewaymedium14" tools:text="@string/lbl_total"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPrice4"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtPrice4}"
                    style="@style/txtRalewaysemibold18_1" tools:text="@string/lbl_61_002"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_primary_radius_tl_8_bl_8_br_8"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:gravity="center" android:id="@+id/btnCheckout"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_50pxv"
                android:layout_width="@dimen/_192pxh"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_checkout"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:text="@string/lbl_checkout"/>
        </LinearLayout>
    </LinearLayout>
</layout>
