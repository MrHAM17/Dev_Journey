<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="chatWithDoctorVM" type="com.helthiotelehealth.app.modules.chatwithdoctor.data.viewmodel.ChatWithDoctorVM"/>
    </data>
    <LinearLayout android:id="@+id/linearMessage"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageIconChevronLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_icon_chevron_left"
                tools:ignore="ContentDescription" tools:src="@drawable/img_icon_chevron_left"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDrMarcusHorizon"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{chatWithDoctorVM.chatWithDoctorModel.txtDrMarcusHorizon}"
                style="@style/txtRalewaysemibold16_1" tools:text="@string/msg_dr_marcus_horizon"/>
            <LinearLayout android:id="@+id/linearGroup24"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageUVideo"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_u_video"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_u_video"/>
                <ImageView android:id="@+id/imageUPhoneAlt"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_u_phone_alt"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_u_phone_alt"/>
                <ImageView android:id="@+id/imageImage"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_width="@dimen/_4pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_icon_chevron_left_onprimary_24x24"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_icon_chevron_left_onprimary_24x24"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearChatWithDoctor"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_42pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_42pxv">
            <LinearLayout android:id="@+id/linearConsultationStart"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_14pxv"
                android:paddingEnd="@dimen/_39pxh"
                android:paddingStart="@dimen/_39pxh"
                android:paddingTop="@dimen/_14pxv" style="@style/groupStylenamewhite_4">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtConsultionStart"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{chatWithDoctorVM.chatWithDoctorModel.txtConsultionStart}"
                    style="@style/txtRalewaysemibold16_4" tools:text="@string/msg_consultion_start"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtYouCanConsult"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{chatWithDoctorVM.chatWithDoctorModel.txtYouCanConsult}"
                    style="@style/txtRalewaymedium12" tools:text="@string/msg_you_can_consult"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearEightyEight"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearProfile"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageClose"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_40pxh"
                        android:layout_width="@dimen/_40pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_close_40x40"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_close_40x40"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearFiftyFour"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDrMarcusHorizon1"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{chatWithDoctorVM.chatWithDoctorModel.txtDrMarcusHorizon1}"
                            style="@style/txtRalewaysemibold14_3" tools:text="@string/msg_dr_marcus_horizon"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTime"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{chatWithDoctorVM.chatWithDoctorModel.txtTime}"
                            style="@style/txtRalewaymedium10" tools:text="@string/lbl_10_min_ago"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearChatTile"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_15pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_7pxv" style="@style/groupStyle{&quot;name&quot;:&quot;gray_300&quot;}cornerRadius">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtHelloHowCanI"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{chatWithDoctorVM.chatWithDoctorModel.txtHelloHowCanI}"
                        style="@style/txtRalewayregular14_4" tools:text="@string/msg_hello_how_can_i"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearChatTile1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:paddingHorizontal="@dimen/_6pxh"
                    android:paddingVertical="@dimen/_6pxv" style="@style/groupStyle{&quot;name&quot;:&quot;primary&quot;}cornerRadius">
                    <TextView android:gravity="center_vertical"
                        android:id="@+id/txtDuration"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:text="@{chatWithDoctorVM.chatWithDoctorModel.txtDuration}"
                        style="@style/txtRalewayregular14_5" tools:text="@string/msg_i_have_suffering"/>
                    <ImageView android:id="@+id/imageBasicCheckAllBig"
                        android:layout_gravity="bottom"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_basic_check_all_big"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_basic_check_all_big"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearProfile1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageClose1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_close_40x40"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_close_40x40"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearSixtyThree"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDrMarcusHorizon2"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{chatWithDoctorVM.chatWithDoctorModel.txtDrMarcusHorizon2}"
                        style="@style/txtRalewayromansemibold14" tools:text="@string/msg_dr_marcus_horizon"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{chatWithDoctorVM.chatWithDoctorModel.txtTime1}"
                        style="@style/txtRalewaymedium10" tools:text="@string/lbl_5_min_ago"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearChatTile2"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_5pxv"
                android:paddingEnd="@dimen/_13pxh"
                android:paddingStart="@dimen/_13pxh"
                android:paddingTop="@dimen/_5pxv" style="@style/groupStyle{&quot;name&quot;:&quot;gray_300&quot;}cornerRadius">
                <TextView android:id="@+id/txtOkDoYouHave"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:text="@{chatWithDoctorVM.chatWithDoctorModel.txtOkDoYouHave}"
                    style="@style/txtRalewayregular14_4" tools:text="@string/msg_ok_do_you_have"/>
            </LinearLayout>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearChatTile3"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/_6pxv"
                android:paddingEnd="@dimen/_5pxh"
                android:paddingStart="@dimen/_5pxh"
                android:paddingTop="@dimen/_6pxv" style="@style/groupStyle{&quot;name&quot;:&quot;primary&quot;}cornerRadius">
                <TextView android:id="@+id/txtIDonTHaveAny"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:text="@{chatWithDoctorVM.chatWithDoctorModel.txtIDonTHaveAny}"
                    style="@style/txtRalewayromanregular14_1" tools:text="@string/msg_i_don_t_have_any"/>
                <ImageView android:id="@+id/imageBasicCheckAllBig1"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_basic_check_all_big"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_basic_check_all_big"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearSeventyTwo"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearClose"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageClose2"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_40pxh"
                        android:layout_width="@dimen/_40pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_close_40x40"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_close_40x40"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearDrMarcusHorizon"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDrMarcusHorizon3"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{chatWithDoctorVM.chatWithDoctorModel.txtDrMarcusHorizon3}"
                            style="@style/txtRalewayromansemibold14" tools:text="@string/msg_dr_marcus_horizon"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtOnline"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{chatWithDoctorVM.chatWithDoctorModel.txtOnline}"
                            style="@style/txtRalewaymedium10" tools:text="@string/lbl_online"/>
                    </LinearLayout>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameTyping"
                    android:layout_height="@dimen/_22pxv"
                    android:layout_width="@dimen/_58pxh"
                    android:paddingBottom="@dimen/_8pxv"
                    android:paddingEnd="@dimen/_13pxh"
                    android:paddingStart="@dimen/_13pxh"
                    android:paddingTop="@dimen/_8pxv" style="@style/groupStylenamegray_2">
                    <ImageView android:id="@+id/imageImage1"
                        android:layout_gravity="bottom|center"
                        android:layout_height="@dimen/_5pxh"
                        android:layout_width="@dimen/_32pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group_141"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_group_141"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearSeventySix"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <EditText
                android:background="@drawable/rectangle_bg_name_white_a700_border_name_gray_300_radius_8"
                android:drawableEnd="@drawable/img_attach"
                android:drawablePadding="@dimen/_30pxh"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:hint="@string/msg_type_message"
                android:id="@+id/etMessage" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_17pxh"
                android:paddingStart="@dimen/_19pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@={chatWithDoctorVM.chatWithDoctorModel.etMessageValue}"
                android:textColor="@color/{&quot;name&quot;:&quot;gray_500&quot;}"
                android:textColorHint="@color/{&quot;name&quot;:&quot;gray_500&quot;}"
                android:textFontWeight="400"
                android:textSize="@dimen/_14pxh"
                android:textStyle="normal" tools:ignore="Autofill"/>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_primary_radius_tl_8_bl_8_br_8"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:gravity="center" android:id="@+id/btnSend"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_50pxv"
                android:layout_width="@dimen/_111pxh"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_send"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:text="@string/lbl_send"/>
        </LinearLayout>
    </LinearLayout>
</layout>
