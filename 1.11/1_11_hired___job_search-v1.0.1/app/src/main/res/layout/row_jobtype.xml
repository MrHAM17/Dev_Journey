<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearWork"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="vertical"
        android:paddingBottom="@dimen/_20pxv"
        android:paddingEnd="@dimen/_17pxh"
        android:paddingStart="@dimen/_17pxh"
        android:paddingTop="@dimen/_20pxv" style="@style/groupStylenameonPrimaryContaineropacity1cornerRadius_4">
        <ImageButton
            android:background="@drawable/rectangle_bg_name_primary_radius_32"
            android:id="@+id/btnWork"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_64pxh"
            android:layout_width="@dimen/_64pxh"
            android:paddingHorizontal="@dimen/_16pxh"
            android:paddingVertical="@dimen/_16pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_work" tools:ignore="ContentDescription"/>
        <TextView android:ellipsize="end" android:id="@+id/txtFindAJob"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:letterSpacing="0.0"
            android:lineSpacingExtra="@dimen/_5pxh"
            android:singleLine="true"
            android:text="@string/lbl_find_a_job"
            style="@style/txtPlusJakartaSanssemibold16" tools:text="@string/lbl_find_a_job"/>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtItSEasyToFind"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:letterSpacing="0.0"
            android:lineSpacingExtra="@dimen/_8pxh"
            android:text="@string/msg_it_s_easy_to_find"
            style="@style/txtPlusJakartaSansmedium12_3" tools:text="@string/msg_it_s_easy_to_find"/>
    </LinearLayout>
</layout>
