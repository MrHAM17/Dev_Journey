                            android:layout_height="@dimen/_24pxh"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_thumbs_up_primary"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_thumbs_up_primary"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtExperience"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:letterSpacing="0.0"
                            android:lineSpacingExtra="@dimen/_5pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_experience"
                            style="@style/txtPlusJakartaSanssemibold16" tools:text="@string/lbl_experience"/>
                        <FrameLayout android:id="@+id/frameSpacer1"
                            android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                        <ImageView android:id="@+id/imageArrowRight1"
                            android:layout_height="@dimen/_24pxh"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrow_right_primary"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right_primary"/>
                    </LinearLayout>
                    <View
