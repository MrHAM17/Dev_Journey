                        android:layout_gravity="bottom"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.0"
                        android:lineSpacingExtra="@dimen/_5pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_help_feedback"
                        style="@style/txtPlusJakartaSanssemibold16" tools:text="@string/lbl_help_feedback"/>
                    <FrameLayout android:id="@+id/frameSpacer6"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <ImageView android:id="@+id/imageArrowRight6"
                        android:layout_gravity="top"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_right_primary"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right_primary"/>
                </LinearLayout>
                <View
                    android:background="@color/{&quot;name&quot;:&quot;gray_300&quot;}"
                    android:id="@+id/lineLine6"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_291pxh"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearAboutUs"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageVideoCamera"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_video_camera"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_video_camera"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAboutUs"
                        android:layout_gravity="bottom"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.0"
                        android:lineSpacingExtra="@dimen/_5pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_about_us"
