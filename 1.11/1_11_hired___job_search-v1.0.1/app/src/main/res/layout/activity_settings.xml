                            android:scaleType="fitXY"
                            android:src="@drawable/img_shield_done"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_shield_done"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSecurity"
                            android:layout_gravity="bottom"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:letterSpacing="0.0"
                            android:lineSpacingExtra="@dimen/_5pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_security"
                            style="@style/txtPlusJakartaSanssemibold16" tools:text="@string/lbl_security"/>
