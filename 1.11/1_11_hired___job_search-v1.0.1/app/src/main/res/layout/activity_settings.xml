                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_about"
                    style="@style/txtPlusJakartaSanssemibold12_2" tools:text="@string/lbl_about"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearLegalAndPolicies"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageShield"
                        android:layout_gravity="top"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_shield"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_shield"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLegalAndPolicies"
                        android:layout_gravity="bottom"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.0"
                        android:lineSpacingExtra="@dimen/_5pxh"
                        android:singleLine="true"
                        android:text="@string/msg_legal_and_policies"
                        style="@style/txtPlusJakartaSanssemibold16" tools:text="@string/msg_legal_and_policies"/>
                    <FrameLayout android:id="@+id/frameSpacer5"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <ImageView android:id="@+id/imageArrowRight5"
                        android:layout_gravity="top"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_right_primary"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right_primary"/>
                </LinearLayout>
                <View
                    android:background="@color/{&quot;name&quot;:&quot;gray_300&quot;}"
                    android:id="@+id/lineLine5"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_291pxh"/>
