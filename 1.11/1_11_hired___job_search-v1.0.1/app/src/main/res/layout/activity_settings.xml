                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_64pxv" android:layout_width="@dimen/_64pxh">
                        <FrameLayout
                            android:background="@drawable/rectangle_border_name_onprimarycontainer"
                            android:id="@+id/frameCircleProgressBar"
                            android:layout_gravity="center"
                            android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                        <TextView android:ellipsize="end"
                            android:gravity="top|start"
                            android:id="@+id/txtSixtyFive"
                            android:layout_gravity="center"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.0"
                            android:lineSpacingExtra="@dimen/_5pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_65"
                            style="@style/txtPlusJakartaSansbold16_1" tools:text="@string/lbl_65"/>
                    </FrameLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearTitleDesc"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAplicationName"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.0"
                            android:lineSpacingExtra="@dimen/_5pxh"
                            android:singleLine="true"
                            android:text="@string/msg_profile_completeness"
                            style="@style/txtPlusJakartaSansbold16_1" tools:text="@string/msg_profile_completeness"/>
                        <TextView android:alpha="0.8"
                            android:id="@+id/txtQualityProfiles"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.0"
                            android:lineSpacingExtra="@dimen/_8pxh"
                            android:text="@string/msg_quality_profiles"
                            style="@style/txtPlusJakartaSansmedium12_1" tools:text="@string/msg_quality_profiles"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearAccount"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAccount"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.0"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_account"
                        style="@style/txtPlusJakartaSanssemibold12_2" tools:text="@string/lbl_account"/>
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearAccount1"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imagePerson"
                            android:layout_height="@dimen/_24pxh"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_person"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_person"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPersonalInfo"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:letterSpacing="0.0"
                            android:lineSpacingExtra="@dimen/_5pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_personal_info"
                            style="@style/txtPlusJakartaSanssemibold16" tools:text="@string/lbl_personal_info"/>
                        <FrameLayout android:id="@+id/frameSpacer"
