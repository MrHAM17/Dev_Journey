<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearThirtyEight"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;gray_300_01&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_down"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
            <LinearLayout android:id="@+id/linearGroup10"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageSearch"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_search"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                <ImageView android:id="@+id/imageImage"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group_86"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group_86"/>
                <ImageView android:id="@+id/imageImage1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group_84"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group_84"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollView"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearShop"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtShop"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_shop"
                    style="@style/txtPlayfairDisplayitalicregular40" tools:text="@string/lbl_shop"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtHollywoodHairstyles"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/msg_hollywood_hairstyles"
                    style="@style/txtPlayfairDisplayplayfairdisplaymedium14" tools:text="@string/msg_hollywood_hairstyles"/>
                <LinearLayout android:id="@+id/linearSeventeen"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_border_name_black_900_radius_18"
                        android:drawablePadding="@dimen/_10pxh"
                        android:drawableStart="@drawable/img_share"
                        android:ellipsize="end"
                        android:fontFamily="@font/playfair_display"
                        android:gravity="center"
                        android:id="@+id/btnSort"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_58pxv"
                        android:layout_width="@dimen/_169pxh"
                        android:paddingBottom="@dimen/_14pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_14pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_sort"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;black_900_01&quot;}"
                        android:textFontWeight="500"
                        android:textSize="@dimen/_22pxh"
                        android:textStyle="bold" tools:text="@string/lbl_sort"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:background="@drawable/rectangle_bg_name_black_900_radius_19"
                        android:drawablePadding="@dimen/_10pxh"
                        android:drawableStart="@drawable/img_group_24"
                        android:ellipsize="end"
                        android:fontFamily="@font/playfair_display"
                        android:gravity="center"
                        android:id="@+id/btnFilter"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_58pxv"
                        android:layout_width="@dimen/_177pxh"
                        android:paddingBottom="@dimen/_14pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_14pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_filter"
                        android:textAllCaps="false"
                        android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                        android:textFontWeight="500"
                        android:textSize="@dimen/_22pxh"
                        android:textStyle="bold" tools:text="@string/lbl_filter"/>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerShop"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:nestedScrollingEnabled="false"
                    app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
                    app:spanCount="2" itemSpace="@{@dimen/_16pxh}"
                    tools:itemCount="4" tools:listitem="@layout/row_shop"/>
                <LinearLayout android:id="@+id/linearThirtySeven"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearTwentyNine"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSurgeShort"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl_surge_short"
                            style="@style/txtPlayfairDisplayplayfairdisplaymedium16_1" tools:text="@string/lbl_surge_short"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPrice"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl_68_usd"
                            style="@style/txtPlayfairDisplayplayfairdisplaymedium16" tools:text="@string/lbl_68_usd"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearTwentyNine1"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSurgeShort1"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/msg_sweat_jogger_french"
                            style="@style/txtPlayfairDisplayplayfairdisplaymedium16_1" tools:text="@string/msg_sweat_jogger_french"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPrice1"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@string/lbl_68_usd"
                            style="@style/txtPlayfairDisplayplayfairdisplaymedium16" tools:text="@string/lbl_68_usd"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>
</layout>
