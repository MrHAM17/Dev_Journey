<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearHome"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;gray_300_01&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <TextView android:ellipsize="end"
                android:id="@+id/txtStreetStyle"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_street_style"
                style="@style/txtPlayfairDisplayplayfairdisplaybold24" tools:text="@string/lbl_street_style"/>
            <LinearLayout android:id="@+id/linearGroup1"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageSearch"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_search"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                <ImageView android:id="@+id/imageImage"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group_86"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group_86"/>
                <ImageView android:id="@+id/imageImage1"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group_84"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group_84"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearFiftyFive"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_49pxv"
            android:paddingEnd="@dimen/_40pxh"
            android:paddingStart="@dimen/_40pxh" android:paddingTop="@dimen/_49pxv">
            <LinearLayout android:id="@+id/linearNine"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_38pxv"
                android:paddingEnd="@dimen/_31pxh"
                android:paddingStart="@dimen/_31pxh"
                android:paddingTop="@dimen/_38pxv" style="@style/groupStylecornerRadius">
                <View
                    android:background="@color/{&quot;name&quot;:&quot;black_900&quot;}"
                    android:id="@+id/lineLineCopy"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_150pxh"/>
                <TextView android:gravity="start"
                    android:id="@+id/txtBIGSEASONALSALE"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_2pxh"
                    android:text="@string/msg_big_seasonal_sale"
                    style="@style/txtPlayfairDisplayitalicregular50" tools:text="@string/msg_big_seasonal_sale"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSAVEUPTO"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_save_up_to"
                    style="@style/txtPlayfairDisplayplayfairdisplaybold16" tools:text="@string/lbl_save_up_to"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSAVEUPTO1"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_75"
                    style="@style/txtPlayfairDisplayplayfairdisplaybold36" tools:text="@string/lbl_75"/>
                <View
                    android:background="@color/{&quot;name&quot;:&quot;black_900&quot;}"
                    android:id="@+id/lineLineFive"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_150pxh"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtBestSellers"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_bestsellers"
                style="@style/txtPlayfairDisplayitalicregular40" tools:text="@string/lbl_bestsellers"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtFuelYourWorkout"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="@string/msg_fuel_your_workout"
                style="@style/txtPlayfairDisplayplayfairdisplaybold14" tools:text="@string/msg_fuel_your_workout"/>
        </LinearLayout>
    </LinearLayout>
</layout>
