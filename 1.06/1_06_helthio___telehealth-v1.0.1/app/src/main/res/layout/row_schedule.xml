<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearCancel1"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingBottom="@dimen/_15pxv"
        android:paddingEnd="@dimen/_14pxh"
        android:paddingStart="@dimen/_14pxh"
        android:paddingTop="@dimen/_15pxv" style="@style/groupStylenamewhite_8">
        <LinearLayout android:id="@+id/linearDrMarcusHorizon1"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearDrMarcusHorizon2"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDrMarcusHorizon"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/msg_dr_marcus_horizon"
                    style="@style/txtRalewaysemibold18_1" tools:text="@string/msg_dr_marcus_horizon"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtChardiologist"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_chardiologist"
                    style="@style/txtRalewaymedium12_2" tools:text="@string/lbl_chardiologist"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageDrMarcusHorizon"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_46pxh"
                android:layout_width="@dimen/_46pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_close_40x40"
                tools:ignore="ContentDescription" tools:src="@drawable/img_close_40x40"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearDate1"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:id="@+id/linearDate2"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageImage"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_width="@dimen/_15pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconly_light_calendar"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_light_calendar"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:id="@+id/txtDate"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/lbl_26_06_2022"
                    style="@style/txtRalewaymedium12_3" tools:text="@string/lbl_26_06_2022"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearAM"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageAM"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_width="@dimen/_15pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_clock"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_clock"/>
                <TextView android:ellipsize="end"
                    android:gravity="bottom" android:id="@+id/txtTime"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/lbl_10_30_am"
                    style="@style/txtRalewaymedium12_3" tools:text="@string/lbl_10_30_am"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearView"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <View
                    android:background="@drawable/rectangle_bg_name_green_600_radius_3"
                    android:id="@+id/viewView"
                    android:layout_gravity="bottom"
                    android:layout_height="@dimen/_6pxh" android:layout_width="@dimen/_6pxh"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:id="@+id/txtConfirmed"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/lbl_confirmed"
                    style="@style/txtRalewaymedium12_3" tools:text="@string/lbl_confirmed"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearCancel2"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_gray_100_radius_8"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:gravity="center" android:id="@+id/btnCancel"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_46pxv"
                android:layout_width="@dimen/_145pxh"
                android:paddingBottom="@dimen/_14pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@string/lbl_cancel"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_700&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:text="@string/lbl_cancel"/>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_name_blue_50_radius_8"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:gravity="center" android:id="@+id/btnReschedule"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_46pxv"
                android:layout_width="@dimen/_145pxh"
                android:paddingBottom="@dimen/_14pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@string/lbl_reschedule"
                android:textAllCaps="false"
                android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:text="@string/lbl_reschedule"/>
        </LinearLayout>
    </LinearLayout>
</layout>
