<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="start" android:id="@+id/linearDoctor"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingBottom="@dimen/_8pxv"
        android:paddingEnd="@dimen/_6pxh"
        android:paddingStart="@dimen/_6pxh"
        android:paddingTop="@dimen/_8pxv" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}cornerRadius">
        <FrameLayout android:id="@+id/frameFortySeven"
            android:layout_gravity="end"
            android:layout_height="@dimen/_82pxv" android:layout_width="@dimen/_105pxh">
            <ImageView android:id="@+id/imageCircleImage"
                android:layout_gravity="bottom|start"
                android:layout_height="@dimen/_71pxh"
                android:layout_width="@dimen/_71pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_ellipse_88}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_88"/>
            <LinearLayout android:id="@+id/linearFortySeven"
                android:layout_gravity="top|end"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageImage"
                    android:layout_gravity="top"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_signal"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_signal"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFortySeven"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_4_7"
                    style="@style/txtRalewaysemibold12" tools:text="@string/lbl_4_7"/>
            </LinearLayout>
        </FrameLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtDrMarcusHorizon"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@string/msg_dr_marcus_horizon"
            style="@style/txtRalewaysemibold16_2" tools:text="@string/msg_dr_marcus_horizon"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtChardiologist"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@string/lbl_chardiologist"
            style="@style/txtRalewaymedium14" tools:text="@string/lbl_chardiologist"/>
        <LinearLayout android:id="@+id/linearFifteen"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageMaway"
                android:layout_gravity="top"
                android:layout_height="@dimen/_16pxh"
                android:layout_width="@dimen/_16pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_linkedin"
                tools:ignore="ContentDescription" tools:src="@drawable/img_linkedin"/>
            <TextView android:ellipsize="end" android:gravity="bottom"
                android:id="@+id/txtDistance"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_800m_away"
                style="@style/txtRalewaymedium14" tools:text="@string/lbl_800m_away"/>
        </LinearLayout>
    </LinearLayout>
</layout>
