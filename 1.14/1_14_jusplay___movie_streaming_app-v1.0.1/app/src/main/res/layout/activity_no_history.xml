<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearNoHistory"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;black_900&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageButton
                android:background="@drawable/rectangle_bg_name_gray_900_radius_17"
                android:id="@+id/btnArrowLeft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrow_left" tools:ignore="ContentDescription"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtComedy"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_history"
                style="@style/txtPoppinsmedium18" tools:text="@string/lbl_history"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearNoHistory1"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" android:paddingTop="@dimen/_180pxv">
            <ImageView android:id="@+id/imageImage"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_184pxh"
                android:layout_width="@dimen/_184pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_group_427318934}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group_427318934"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNoHistory"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_no_history"
                style="@style/txtPoppinsmedium18" tools:text="@string/lbl_no_history"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtThereAreCurrently"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/msg_there_are_currently"
                style="@style/txtPoppinsregular12_1" tools:text="@string/msg_there_are_currently"/>
        </LinearLayout>
    </LinearLayout>
</layout>
