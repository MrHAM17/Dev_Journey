<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="chartsVM" type="com.tunecastmusicstreamingpodcastapp.app.modules.charts.data.viewmodel.ChartsVM"/>
    </data>
    <LinearLayout android:id="@+id/linearArrowDown"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_down"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFillYourProfile"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{chartsVM.chartsModel.txtFillYourProfile}"
                style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_charts"/>
            <ImageView android:id="@+id/imageRewind"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_rewind"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rewind"/>
        </androidx.appcompat.widget.Toolbar>
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameWeeklyAlbumCharts"
            android:layout_height="@dimen/_907pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_27pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_27pxv">
            <TextView android:ellipsize="end"
                android:gravity="top|start"
                android:id="@+id/txtWeeklyAlbumCharts1"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{chartsVM.chartsModel.txtWeeklyAlbumCharts1}"
                style="@style/txtUrbanistromanbold24" tools:text="@string/msg_weekly_album_charts"/>
            <LinearLayout android:id="@+id/linearWeeklyAlbumCharts"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtWeeklyAlbumCharts1"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{chartsVM.chartsModel.txtWeeklyAlbumCharts1}"
                    style="@style/txtUrbanistromanbold24" tools:text="@string/msg_weekly_album_charts"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerTOPALBUMSGLOBAL"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_12pxh}" tools:itemCount="2" tools:listitem="@layout/row_topalbumsglobal"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtWeeklySongCharts"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{chartsVM.chartsModel.txtWeeklySongCharts}"
                    style="@style/txtUrbanistromanbold24" tools:text="@string/msg_weekly_song_charts"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerTOPALBUMSGLOBAL1"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_12pxh}" tools:itemCount="2" tools:listitem="@layout/row_topalbumsglobal1"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
