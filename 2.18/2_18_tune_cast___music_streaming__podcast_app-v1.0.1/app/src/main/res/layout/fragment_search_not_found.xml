<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="searchNotFoundVM" type="com.tunecastmusicstreamingpodcastapp.app.modules.searchnotfound.data.viewmodel.SearchNotFoundVM"/>
    </data>
    <LinearLayout android:id="@+id/linearNotFound"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <LinearLayout android:id="@+id/linearNotFound1"
            android:layout_height="match_parent"
            android:layout_width="match_parent" android:orientation="vertical">
            <ImageView android:id="@+id/imageImage"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_250pxh"
                android:layout_width="@dimen/_339pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_group_324"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group_324"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNotFound"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{searchNotFoundVM.searchNotFoundModel.txtNotFound}"
                style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_not_found"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtSorryTheKeyword"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01"
                android:text="@{searchNotFoundVM.searchNotFoundModel.txtSorryTheKeyword}"
                style="@style/txtUrbanistregular18" tools:text="@string/msg_sorry_the_keyword"/>
        </LinearLayout>
    </LinearLayout>
</layout>
