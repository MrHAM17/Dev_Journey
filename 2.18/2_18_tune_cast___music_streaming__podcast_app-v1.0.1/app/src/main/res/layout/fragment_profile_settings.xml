<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileSettingsVM" type="com.tunecastmusicstreamingpodcastapp.app.modules.profilesettings.data.viewmodel.ProfileSettingsVM"/>
    </data>
    <LinearLayout android:id="@+id/linearGetPremium"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageVector"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtMyLibrary"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{profileSettingsVM.profileSettingsModel.txtMyLibrary}"
                style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_profile"/>
            <ImageButton android:id="@+id/btnSettings"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitCenter"
                android:src="@drawable/img_settings_gray_900_01" tools:ignore="ContentDescription"/>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollView"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearProfileSettings"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearEdit"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <FrameLayout android:id="@+id/frameTypeEditAvatar"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_80pxv" android:layout_width="@dimen/_80pxh">
                        <ImageView android:id="@+id/imageCircleImage"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_80pxh"
                            android:layout_width="@dimen/_80pxh"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_ellipse_80x80}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_80x80"/>
                        <ImageView android:id="@+id/imageEdit"
                            android:layout_gravity="bottom|end"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_edit"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_edit"/>
                    </FrameLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearAutoLayoutVertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAndrewAinsley"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{profileSettingsVM.profileSettingsModel.txtAndrewAinsley}"
                            style="@style/txtUrbanistromanbold20" tools:text="@string/lbl_andrew_ainsley"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtEmail"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{profileSettingsVM.profileSettingsModel.txtEmail}"
                            style="@style/txtUrbanistsemibold14" tools:text="@string/lbl_user_domain_com"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearGetPremium1"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:paddingEnd="@dimen/_23pxh"
                    android:paddingStart="@dimen/_23pxh" style="@style/groupStyle{&quot;name&quot;:&quot;onPrimaryContainer&quot;}cornerRadius">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearGetPremium2"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtEnjoyAllBenefits"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{profileSettingsVM.profileSettingsModel.txtEnjoyAllBenefits}"
                            style="@style/txtUrbanistromanbold24_1" tools:text="@string/msg_enjoy_all_benefits"/>
                        <TextView android:id="@+id/txtEnjoyListening"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:text="@{profileSettingsVM.profileSettingsModel.txtEnjoyListening}"
                            style="@style/txtUrbanistregular12" tools:text="@string/msg_enjoy_listening"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:background="@drawable/rectangle_bg_name_white_a700_radius_16"
                            android:ellipsize="end"
                            android:fontFamily="@font/urbanist"
                            android:gravity="center"
                            android:id="@+id/btnGetPremium"
                            android:layout_height="@dimen/_32pxv"
                            android:layout_width="@dimen/_116pxh"
                            android:letterSpacing="0.01"
                            android:paddingBottom="@dimen/_7pxv"
                            android:paddingEnd="@dimen/_16pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_7pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_get_premium"
                            android:textAllCaps="false"
                            android:textColor="@color/{&quot;name&quot;:&quot;onPrimaryContainer&quot;}"
                            android:textFontWeight="600"
                            android:textSize="@dimen/_14pxh"
                            android:textStyle="bold" tools:text="@string/lbl_get_premium"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageImage"
                        android:layout_gravity="bottom"
                        android:layout_height="@dimen/_169pxh"
                        android:layout_width="@dimen/_115pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_music_fococlipping_standard}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_music_fococlipping_standard"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearAutoLayoutHorizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageLock"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_lock"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtProfile"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{profileSettingsVM.profileSettingsModel.txtProfile}"
                        style="@style/txtUrbanistsemibold18" tools:text="@string/lbl_profile"/>
                    <FrameLayout android:id="@+id/frameSpacer"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <ImageView android:id="@+id/imageArrowRight"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_right"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearAutoLayoutHorizontal1"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearNotification"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageImage1"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_width="@dimen/_28pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icons_gray_900_01"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_icons_gray_900_01"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_vertical"
                            android:id="@+id/txtNotification"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{profileSettingsVM.profileSettingsModel.txtNotification}"
                            style="@style/txtUrbanistsemibold18" tools:text="@string/lbl_notification"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowRight1"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_right"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearAutoLayoutHorizontal2"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearMenu"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageMenu"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_width="@dimen/_28pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_menu"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_menu"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_vertical"
                            android:id="@+id/txtAudioVideo"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{profileSettingsVM.profileSettingsModel.txtAudioVideo}"
                            style="@style/txtUrbanistsemibold18" tools:text="@string/lbl_audio_video"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowRight2"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_right"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearAutoLayoutHorizontal3"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/linearIconlyCurvedPlay"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/imageIconlyCurvedPlay"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_width="@dimen/_28pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_iconly_curved_play"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_curved_play"/>
                        <TextView android:ellipsize="end"
                            android:gravity="bottom"
                            android:id="@+id/txtPlayback"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{profileSettingsVM.profileSettingsModel.txtPlayback}"
                            style="@style/txtUrbanistsemibold18" tools:text="@string/lbl_playback"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowRight3"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_right"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearAutoLayoutHorizontal4"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearIconlyCurvedTick"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/imageIconlyCurvedTick"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_width="@dimen/_28pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_iconly_curved_tick"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_curved_tick"/>
                        <TextView android:ellipsize="end"
                            android:gravity="bottom"
                            android:id="@+id/txtDataSaverStorage"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{profileSettingsVM.profileSettingsModel.txtDataSaverStorage}"
                            style="@style/txtUrbanistsemibold18" tools:text="@string/msg_data_saver_storage"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowRight4"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_right"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearAutoLayoutHorizontal5"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageIconlyCurvedShield"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_iconly_curved_shield"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_curved_shield"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSecurity"
                        android:layout_gravity="bottom"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{profileSettingsVM.profileSettingsModel.txtSecurity}"
                        style="@style/txtUrbanistsemibold18" tools:text="@string/lbl_security"/>
                    <FrameLayout android:id="@+id/frameSpacer1"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <ImageView android:id="@+id/imageArrowRight5"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_right"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearAutoLayoutHorizontal6"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageIconlyCurvedMore"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_iconly_curved_more"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_curved_more"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLanguage"
                        android:layout_gravity="bottom"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{profileSettingsVM.profileSettingsModel.txtLanguage}"
                        style="@style/txtUrbanistsemibold18" tools:text="@string/lbl_language"/>
                    <FrameLayout android:id="@+id/frameSpacer2"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEnglishUS"
                        android:layout_gravity="bottom"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{profileSettingsVM.profileSettingsModel.txtEnglishUS}"
                        style="@style/txtUrbanistsemibold18" tools:text="@string/lbl_english_us"/>
                    <ImageView android:id="@+id/imageArrowRight6"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_right"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearAutoLayoutHorizontal7"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearEye"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageEye"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_width="@dimen/_28pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_eye_gray_900_01"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_eye_gray_900_01"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_vertical"
                            android:id="@+id/txtDarkMode"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{profileSettingsVM.profileSettingsModel.txtDarkMode}"
                            style="@style/txtUrbanistsemibold18" tools:text="@string/lbl_dark_mode"/>
                    </LinearLayout>

                    <com.google.android.material.switchmaterial.SwitchMaterial
                        android:id="@+id/switchMaterialStateDisabled"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearAutoLayoutHorizontal8"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageUser"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user_red_a200"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user_red_a200"/>
                    <TextView android:ellipsize="end"
                        android:gravity="bottom"
                        android:id="@+id/txtLogout"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{profileSettingsVM.profileSettingsModel.txtLogout}"
                        style="@style/txtUrbanistsemibold18_1" tools:text="@string/lbl_logout"/>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
