<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="newPlaylistVM" type="com.tunecastmusicstreamingpodcastapp.app.modules.newplaylist.data.viewmodel.NewPlaylistVM"/>
    </data>
    <LinearLayout android:id="@+id/linearNewPlaylist"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:id="@+id/linearAutoLayoutVertical"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_7pxv"
            android:paddingEnd="@dimen/_23pxh"
            android:paddingStart="@dimen/_23pxh"
            android:paddingTop="@dimen/_7pxv" style="@style/groupStylenamewhite_5">
            <ImageView android:id="@+id/imageImage"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_3pxh"
                android:layout_width="@dimen/_38pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_frame"
                tools:ignore="ContentDescription" tools:src="@drawable/img_frame"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNewPlaylist"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{newPlaylistVM.newPlaylistModel.txtNewPlaylist}"
                style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_new_playlist"/>
            <View
                android:background="@color/{&quot;name&quot;:&quot;blue_gray_100&quot;}"
                android:id="@+id/lineLine"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_380pxh"/>
            <EditText
                android:background="@drawable/rectangle_bg_name_gray_50_radius_16"
                android:ellipsize="end"
                android:fontFamily="@font/urbanist"
                android:hint="@string/msg_most_popular_songs"
                android:id="@+id/etLabel" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_19pxv"
                android:paddingEnd="@dimen/_20pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:text="@={newPlaylistVM.newPlaylistModel.etLabelValue}"
                android:textColor="@color/{&quot;name&quot;:&quot;gray_900_01&quot;}"
                android:textColorHint="@color/{&quot;name&quot;:&quot;gray_900_01&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_name_gray_50_radius_16"
                android:ellipsize="end"
                android:fontFamily="@font/urbanist"
                android:hint="@string/msg_most_popular_songs2"
                android:id="@+id/etDate" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.01"
                android:paddingBottom="@dimen/_19pxv"
                android:paddingEnd="@dimen/_20pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:text="@={newPlaylistVM.newPlaylistModel.etDateValue}"
                android:textColor="@color/{&quot;name&quot;:&quot;gray_900_01&quot;}"
                android:textColorHint="@color/{&quot;name&quot;:&quot;gray_900_01&quot;}"
                android:textFontWeight="600"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:ignore="Autofill"/>
            <Spinner
                android:background="@drawable/layer_list_bg_name_gray_50_radius_17"
                android:gravity="center_horizontal"
                android:id="@+id/spinnerEye"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
            <View
                android:background="@color/{&quot;name&quot;:&quot;blue_gray_100&quot;}"
                android:id="@+id/lineLine1"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_380pxh"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearAutoLayoutHorizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <androidx.appcompat.widget.AppCompatButton
                    android:background="@drawable/rectangle_border_name_primary_radius_29"
                    android:ellipsize="end"
                    android:fontFamily="@font/urbanist"
                    android:gravity="center" android:id="@+id/btnCancel"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_58pxv"
                    android:layout_width="@dimen/_184pxh"
                    android:letterSpacing="0.01"
                    android:paddingBottom="@dimen/_18pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_18pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_cancel"
                    android:textAllCaps="false"
                    android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                    android:textFontWeight="700"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/lbl_cancel"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:background="@drawable/rectangle_bg_name_primary_radius_29"
                    android:ellipsize="end"
                    android:fontFamily="@font/urbanist"
                    android:gravity="center" android:id="@+id/btnCreate"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_58pxv"
                    android:layout_width="@dimen/_184pxh"
                    android:letterSpacing="0.01"
                    android:paddingBottom="@dimen/_18pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_18pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_create"
                    android:textAllCaps="false"
                    android:textColor="@color/{&quot;name&quot;:&quot;white_A700&quot;}"
                    android:textFontWeight="700"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/lbl_create"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
