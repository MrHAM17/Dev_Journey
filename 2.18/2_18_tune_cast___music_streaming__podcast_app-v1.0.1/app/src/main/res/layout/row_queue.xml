<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="queueRowModel" type="com.tunecastmusicstreamingpodcastapp.app.modules.queue.data.model.QueueRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearPodcast"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageImage2"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_116pxh"
            android:layout_width="@dimen/_116pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_20pxh}"
            imageRes="@{@drawable/img_image_80x80}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_image_80x80"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearAutoLayoutVertical"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:id="@+id/txtPodcastTitle"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="@{queueRowModel.txtPodcastTitle}"
                style="@style/txtUrbanistromanbold18" tools:text="@string/msg_837_tristan_harris"/>
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAuthor"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{queueRowModel.txtAuthor}"
                    style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_apple_talk"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtText"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{queueRowModel.txtText}"
                    style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTime"
                    android:layout_gravity="top"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{queueRowModel.txtTime}"
                    style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_48_26_mins"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal2"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageImage3"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_favorite"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_favorite"/>
                <ImageView android:id="@+id/imageImage"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_categories_playlist"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_categories_playlist"/>
                <ImageView android:id="@+id/imageAppleTalk1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconly_light_outline_arrow_gray_900_01"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconly_light_outline_arrow_gray_900_01"/>
                <ImageView android:id="@+id/imageAppleTalk2"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_categories_more"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_categories_more"/>
                <FrameLayout android:id="@+id/frameSpacer"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <ImageButton android:id="@+id/btnIconlyBoldPlay"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_32pxh"
                    android:layout_width="@dimen/_32pxh"
                    android:paddingHorizontal="@dimen/_2pxh"
                    android:paddingVertical="@dimen/_2pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_iconly_bold_play" tools:ignore="ContentDescription"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
