<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="notificationSettingsVM" type="com.tunecastmusicstreamingpodcastapp.app.modules.notificationsettings.data.viewmodel.NotificationSettingsVM"/>
    </data>
    <LinearLayout android:id="@+id/linearNotificationSettings"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowDown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow_down"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFillYourProfile"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{notificationSettingsVM.notificationSettingsModel.txtFillYourProfile}"
                style="@style/txtUrbanistromanbold24" tools:text="@string/msg_notification_settings"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearRecommendedMusic"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_29pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_29pxv">
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearAutoLayoutVertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtRecommendedMusic"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{notificationSettingsVM.notificationSettingsModel.txtRecommendedMusic}"
                        style="@style/txtUrbanistsemibold18" tools:text="@string/msg_recommended_music"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPushEmail"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{notificationSettingsVM.notificationSettingsModel.txtPushEmail}"
                        style="@style/txtUrbanistromanmedium14_2" tools:text="@string/lbl_push_email"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowRight"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal1"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearAutoLayoutVertical1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtNewMusic"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{notificationSettingsVM.notificationSettingsModel.txtNewMusic}"
                        style="@style/txtUrbanistsemibold18" tools:text="@string/lbl_new_music"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPush"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{notificationSettingsVM.notificationSettingsModel.txtPush}"
                        style="@style/txtUrbanistromanmedium14_2" tools:text="@string/lbl_push"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowRight1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal2"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearAutoLayoutVertical2"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPlaylistUpdates"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{notificationSettingsVM.notificationSettingsModel.txtPlaylistUpdates}"
                        style="@style/txtUrbanistsemibold18" tools:text="@string/msg_playlist_updates"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPushEmail1"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{notificationSettingsVM.notificationSettingsModel.txtPushEmail1}"
                        style="@style/txtUrbanistromanmedium14_2" tools:text="@string/lbl_push_email"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowRight2"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal3"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearAutoLayoutVertical3"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtConcertNotifications"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{notificationSettingsVM.notificationSettingsModel.txtConcertNotifications}"
                        style="@style/txtUrbanistsemibold18" tools:text="@string/msg_concert_notifications"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPushEmail2"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{notificationSettingsVM.notificationSettingsModel.txtPushEmail2}"
                        style="@style/txtUrbanistromanmedium14_2" tools:text="@string/lbl_push_email"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowRight3"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal4"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearAutoLayoutVertical4"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtArtistUpdates"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{notificationSettingsVM.notificationSettingsModel.txtArtistUpdates}"
                        style="@style/txtUrbanistsemibold18" tools:text="@string/lbl_artist_updates"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPushEmail3"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{notificationSettingsVM.notificationSettingsModel.txtPushEmail3}"
                        style="@style/txtUrbanistromanmedium14_2" tools:text="@string/lbl_push_email"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowRight4"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal5"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearAutoLayoutVertical5"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtProductNews"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{notificationSettingsVM.notificationSettingsModel.txtProductNews}"
                        style="@style/txtUrbanistsemibold18" tools:text="@string/lbl_product_news"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEmail"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{notificationSettingsVM.notificationSettingsModel.txtEmail}"
                        style="@style/txtUrbanistromanmedium14_2" tools:text="@string/lbl_email"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowRight5"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal6"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearAutoLayoutVertical6"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHearmeOffers"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{notificationSettingsVM.notificationSettingsModel.txtHearmeOffers}"
                        style="@style/txtUrbanistsemibold18" tools:text="@string/lbl_hearme_offers"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPushEmail4"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{notificationSettingsVM.notificationSettingsModel.txtPushEmail4}"
                        style="@style/txtUrbanistromanmedium14_2" tools:text="@string/lbl_push_email"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowRight6"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearAutoLayoutHorizontal7"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearAutoLayoutVertical7"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtNewEpisodes"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{notificationSettingsVM.notificationSettingsModel.txtNewEpisodes}"
                        style="@style/txtUrbanistsemibold18" tools:text="@string/lbl_new_episodes"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPushEmail5"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{notificationSettingsVM.notificationSettingsModel.txtPushEmail5}"
                        style="@style/txtUrbanistromanmedium14_2" tools:text="@string/lbl_push_email"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowRight7"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
