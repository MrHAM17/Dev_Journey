<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="orderslistRowModel" type="com.shopsieecommerceapp.app.modules.profilemyorders.data.model.OrderslistRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearOrdersItem"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageImage"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_110pxh"
            android:layout_width="@dimen/_109pxh"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_image_110x109}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_image_110x109"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearNineteen"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:ellipsize="end" android:id="@+id/txtPrice"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{orderslistRowModel.txtPrice}"
                style="@style/txtLatomedium18" tools:text="@string/lbl_12_50"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtShortsInYellow"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{orderslistRowModel.txtShortsInYellow}"
                style="@style/txtLatoregular14_1" tools:text="@string/msg_shorts_in_yellow"/>
            <LinearLayout android:id="@+id/linearFrame"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:gravity="bottom" android:id="@+id/txtQty"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{orderslistRowModel.txtQty}"
                    style="@style/txtLatoregular14" tools:text="@string/lbl_qty"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOne"
                    android:layout_gravity="top"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{orderslistRowModel.txtOne}"
                    style="@style/txtLatoregular14_1" tools:text="@string/lbl_1"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
