<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="transferrequestRowModel" type="com.safebankmobilebankingapp.app.modules.transferrequest.data.model.TransferrequestRowModel"/>
    </data>
    <LinearLayout android:gravity="center_vertical"
        android:id="@+id/linearZanderWiza"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageZanderWiza"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_50pxh"
            android:layout_width="@dimen/_50pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_4pxh}"
            imageRes="@{@drawable/img_rectangle_63}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_63"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearFiftyFive"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtZanderWiza"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{transferrequestRowModel.txtZanderWiza}"
                style="@style/txtPoppinsmedium14" tools:text="@string/lbl_zander_wiza"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtUIUXDesigner"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{transferrequestRowModel.txtUIUXDesigner}"
                style="@style/txtPoppinsmedium10" tools:text="@string/lbl_ui_ux_designer"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameSpacer"
            android:layout_height="wrap_content" android:layout_width="wrap_content"/>
        <ImageButton
            android:background="@drawable/rectangle_bg_name_indigo_50_radius_5"
            android:id="@+id/btnPlus"
            android:layout_gravity="center_vertical"
            android:layout_height="@dimen/_25pxh"
            android:layout_width="@dimen/_25pxh"
            android:paddingHorizontal="@dimen/_8pxh"
            android:paddingVertical="@dimen/_8pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_plus" tools:ignore="ContentDescription"/>
    </LinearLayout>
</layout>
